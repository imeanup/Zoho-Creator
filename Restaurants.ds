 			void ManageUser.addAdmin()
			{
				insert into Add_Staff
				[
					Email=zoho.adminuserid
					Job_Title="Manager"
					Hire_Date=zoho.currentdate
					Name.first_name=zoho.adminuser
					Added_User=zoho.loginuser
				]
			}
 			void CustomAction.changeAvailability(Add_Menu_Item itemDetails)
			{
				if(itemDetails.Status == "Available")
				{
					itemDetails.Status="Not Available";
				}
				else if(itemDetails.Status == "Not Available")
				{
					itemDetails.Status="Available";
				}
			}
 			void CustomAction.addStock(Add_Invoice invoiceDetails)
			{
				//Updating each inventory item stock level
				for each  eachRow in invoiceDetails.Items
				{
					inventoryItem = Add_Inventory_Item[ID == eachRow.Item];
					inventoryItem.Available_Stock=inventoryItem.Available_Stock + eachRow.Quantity;
				}
				invoiceDetails.Items_added_to_inventory=true;
			}
 			void CustomAction.itemServed(Order_Item orderItemDetails)
			{
				//Updating kitchen order ticket status and order status.
				orderItemDetails.KOT_Status="Item Served";
				pendingKOTCount = Order_Item[Order_Number == orderItemDetails.Order_Number && KOT_Status == "New" || KOT_Status == "Item Accepted" || KOT_Status == "Ready To Serve"].count();
				if(pendingKOTCount == 0)
				{
					orderDetails = New_Order[ID == orderItemDetails.Order_Number];
					orderDetails.Status="Items Served";
				}
			}
 			void CustomAction.moveToBilling(New_Order orderDetails)
			{
				orderDetails.Status="Bill Generated";
				//Updating the sum of net amount with tax.
				subtotal = Order_Item[Order_Number == orderDetails.ID && KOT_Status == "Item Served"].sum(Total);
				taxDetails = Tax_Setting[ID != 0];
				if(taxDetails.count() > 0)
				{
					serviceCharge = subtotal * taxDetails.Service_Charge / 100;
					serviceTax = subtotal * taxDetails.Service_Tax / 100;
					vat = subtotal * taxDetails.VAT_Value_Added_Tax / 100;
					taxAmount = serviceCharge + serviceTax + vat;
				}
				else
				{
					taxAmount = 0;
				}
				orderDetails.Sub_Total=subtotal;
				orderDetails.Taxes=taxAmount;
				orderDetails.Net_Total=subtotal + taxAmount;
			}
 			map PageData.mergeItemNames(int orderID)
			{
				itemNameMap = Map();
				//Here merging duplicate items in an order.
				for each  eachOrderItem in Order_Item[Order_Number == input.orderID && KOT_Status == "Item Served"]
				{
					itemName = eachOrderItem.Item_Name.toString();
					if(itemNameMap.containKey(itemName))
					{
						quantity = itemNameMap.get(itemName).toLong() + eachOrderItem.Quantity;
						itemNameMap.put(itemName,quantity);
					}
					else
					{
						itemNameMap.put(itemName,eachOrderItem.Quantity);
					}
				}
				return itemNameMap;
			}
 			void CustomAction.viewBill(int recID)
			{
				openUrl("#View:Bill?recID=" + recID,"new window");
			}
 			string PageData.addCommasInCurrency(float currencyValue)
			{
				if(input.currencyValue != null)
				{
					currencyValue = currencyValue.round(2);
					currencyStr = currencyValue.toString().replaceAll("(?<!\.\d)(?<=\d)(?=(?:\d\d\d)+\b)",",");
				}
				return currencyStr;
			}
 			void ManageUser.shareProfile(string staffEmail, string jobTitle)
			{
				//share the application with respective staff
				thisapp.permissions.assignUserInProfile(staffEmail,jobTitle);
				//Send notification mail with link to access the application
				sendmail
				[
					from :zoho.adminuserid
					to :staffEmail
					subject :"Invitation to access Restaurants"
					message :"Hi,<div><br></div><div>You have been successfully added as " + jobTitle + ".</div><div><br></div><div>Click&nbsp;<a href=\"" + "https://app.zohocreator.com" + zoho.appuri + "\"" + " target=\"_blank\">here</a>&nbsp;to access \"Restaurants\"</div><div><br></div><div>Regards,</div><div>" + zoho.adminuser + "</div>"
				]
			}
	}



