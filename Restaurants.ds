				record event = on add

	on success
	{
					actions 
					{
						custom deluge script
						(
											for each  eachRow in input.Items
										{
											//Update each inventory item's stock
											inventoryItem = Add_Inventory_Item[ID == eachRow.Item];
											inventoryItem.Available_Stock=inventoryItem.Available_Stock - eachRow.Used_Quantity;
											if(inventoryItem.Available_Stock <= inventoryItem.Reorder_Level)
											{
												//Send notification mail to admin regarding stock level
												sendmail
												[
													from :zoho.loginuserid
													to :zoho.adminuserid
													subject :"Stock Level Alert"
													message :("Hi,<div><br></div><div>The stock level for the item '" + inventoryItem.Item_Name + "'" + " have reached the reorder level.</div><div><br></div><div>Please replenish the item as soon as possible.</div><div><br></div><div><a href=\"" + "https://app.zohocreator.com" + zoho.appuri + "#Report:Inventory_Items?Item_Code=" + inventoryItem.Item_Code + "\"" + " target=\"_blank\">Click here</a> to view the more details.</div><div><br></div><div>Regards,</div><div>" + input.Submitted_By.Name) + "<br></div>"
												]
											}
										}
						)
					}
	}

			}
