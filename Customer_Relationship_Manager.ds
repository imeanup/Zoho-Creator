										insert into Create_Contact
										[
											Added_User=zoho.loginuser
											Contact_Owner=input.Owner_of_the_records
											Fax=rec.Fax
											Mailing_Address1=rec.Address1
											Mobile_Number=rec.Mobile_Number
											Title=rec.Title
											Account_Name=accID.ID
											Phone_Number=rec.Phone_Number
											Name1.prefix=rec.Name1.prefix
											Name1.last_name=rec.Name1.last_name
											Name1.first_name=rec.Name1.first_name
											Name1.suffix=rec.Name1.suffix
											Email=rec.Email
											Added_User1=Add_User[Email == zoho.loginuserid].ID
										]
										conID = Create_Contact[Added_User == zoho.loginuser && Account_Name == accID.ID] sort by Added_Time desc;
										if(input.Create_a_new_Opportunity_for_this_Account)
										{
											insert into Create_Opportunity
											[
												Added_User=zoho.loginuser
												Amount=input.Amount
												Opportunity_Name=input.Opportunity_Name
												Close_Date=input.Closing_Date
												Stage=input.Stage
												Primary_Campaign_Source=input.Campaign_Source
												Opportunity_Owner=input.Owner_of_the_records
												Contact_Name=conID.ID
												Account_Name=accID.ID
												Lead_Name=rec.ID
												Added_User1=Add_User[Email == zoho.loginuserid].ID
											]
										}
										oppID = Create_Opportunity[Added_User == zoho.loginuser && Account_Name == accID.ID && Lead_Name == rec.ID && Contact_Name == conID.ID] sort by Added_Time desc;
										for each  taskRec in Create_Task[Lead_Name == rec.ID]
										{
											taskRec.Account_Name=accID.ID;
											taskRec.Contact_Name=conID.ID;
											taskRec.Opportunity_Name=oppID.ID;
										}
										for each  EventRec in Create_Event[Lead_Name == rec.ID]
										{
											EventRec.Account_Name=accID.ID;
											EventRec.Contact_Name=conID.ID;
											EventRec.Opportunity_Name=oppID.ID;
										}
										for each  CallRec in Create_Call[Lead_Name == rec.ID]
										{
											CallRec.Contact_Name=conID.ID;
										}
										for each  notesRec in Create_Notes[Lead_Name == rec.ID]
										{
											notesRec.Account_Name=accID.ID;
											notesRec.Contact_Name=conID.ID;
											notesRec.Opportunity_Name=oppID.ID;
										}
										for each  campaignRec in Campaign_For_Selecting[Lead_Name == rec.ID]
										{
											campaignRec.Contact_Name=conID.ID;
										}
										accID.Contact_Name={conID.ID};
										rec.Lead_Status="Converted";
										openUrl("#Script:dialog.close","same window");
					)
					}
				}
			}
