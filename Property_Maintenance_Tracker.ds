		schedule
		{
			Reminder as "Reminder"
			{
				type =  schedule
				form = Set_Reminder
				start = Starting_Time
				end   = End_Date
				frequency = daily
				time zone = "Asia/Kolkata"
				on start
				{
					actions 
					{
					on load
					(
						if(Next_Reminder_On == zoho.currentdate)
										{
											mail_content = "Hi , <br> Its time to do ";
											if(input.Frequency != "Once")
											{
												mail_content = mail_content + input.Frequency + " ";
											}
											mail_content = mail_content + "maintenance for " + input.Choose_Service.Service_Name;
											sendmail
											[
												from :zoho.adminuserid
												to :zoho.adminuserid
												subject :"Maintenance Reminder"
												message :mail_content
											]
											if(input.Frequency != "Once")
											{
												if(input.End_Date > zoho.currentdate || input.End_Date == null)
												{
													date_value = {"Daily":input.Next_Reminder_On.addDay(1),"Weekly":input.Next_Reminder_On.addWeek(1),"Monthly":input.Next_Reminder_On.addMonth(1),"Yearly":input.Next_Reminder_On.addYear(1)};
													rec = Set_Reminder[ID == input.ID];
													rec.Next_Reminder_On=date_value.get(input.Frequency);
												}
											}
										}
					)
					}
				}
			}
		}
