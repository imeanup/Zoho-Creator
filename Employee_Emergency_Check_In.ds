	functions
	{
 			void Crisis.Changing_Status_To_Safe(string crisis_id)
			{
				//Updating employee status as safe
				user = Add_Employee[Email == zoho.loginuserid].ID;
				branch = Add_Employee[ID == user].Branch;
				record_count = Employee_Check_In_Form[Crisis_Name == crisis_id.toLong() && Branch == branch && Employee_Name == user].count();
				if(record_count == 0)
				{
					insert into Employee_Check_In_Form
					[
						Added_User=zoho.loginuser
						Date_field=zoho.currentdate
						Branch=branch
						Employee_Name=user
						Status="Safe"
						Crisis_Name=crisis_id.toLong()
					]
					thisapp.Crisis.Add_Checked_In_List(crisis_id.toLong(),user);
				}
			}
 			void Crisis.Add_Checked_In_List(int Crisis_Id, int Emp_Id)
			{
				crisis_rec = Raise_New_Crisis[ID == input.Crisis_Id];
				employee_list = crisis_rec.Checked_In_Employees;
				employee_list.add(Emp_Id);
				crisis_rec.Checked_In_Employees=employee_list;
				openUrl("#Report:My_Status","same window");
			}
 			void Crisis.Changing_Status_To_Away_From_Location(string crisis_id)
			{
				//Updating employee status as Not there
				user = Add_Employee[Email == zoho.loginuserid].ID;
				branch = Add_Employee[ID == user].Branch;
				record_count = Employee_Check_In_Form[Crisis_Name == crisis_id.toLong() && Branch == branch && Employee_Name == user].count();
				if(record_count == 0)
				{
					insert into Employee_Check_In_Form
					[
						Added_User=zoho.loginuser
						Date_field=zoho.currentdate
						Branch=branch
						Employee_Name=user
						Status="Away From Location"
						Crisis_Name=crisis_id.toLong()
					]
					thisapp.Crisis.Add_Checked_In_List(crisis_id.toLong(),user);
				}
			}
 			void Crisis.Changing_Crisis_Status_To_Closed(int crisis_id)
			{
				//Changing crisis status to closed
				this_record = Raise_New_Crisis[ID == input.crisis_id];
				this_record.Status="Closed";
				request_count = Employee_Check_In_Form[Crisis_Name == input.crisis_id && Request_Status == "Pending"].count(Crisis_Name);
				if(request_count != 0)
				{
					for each  rec in Employee_Check_In_Form[Crisis_Name == input.crisis_id && Request_Status == "Pending"]
					{
						rec.Request_Status="Closed";
					}
				}
			}
 			void Crisis.Comment_To_Request(int request_id)
			{
				//Opening response page
				// 	url = "https://app.zohocreator.com" + zoho.appuri + "#Form:Response_Form?Request_Id=" + recid;
				url = "#Form:Response_Form?Request_Id=" + request_id;
				openUrl(url,"popup window");
			}
 			void Notification.Send_Notification(string userEmail)
			{
				//Notification to users
				appname = zoho.appname;
				sendmail
				[
					from :zoho.adminuserid
					to :userEmail
					subject :"Invitation to access '" + zoho.appname + "' "
					message :"<div style='width:100%;display:inline-block;box-sizing:border-box;border:1px solid #ddd;'><div style='padding:2%;text-align:center;'>You've been invited to use an application built on Zoho Creator by \"" + zoho.adminuserid + "\".</div><div style='background: #efefef; padding: 20.0px;margin: 20.0px 0 10.0px;text-align:center;'><h2>" + appname.toUpperCase() + "</h2><div style='margin: 20.0px 0 10.0px;'><a style='padding:10px;background-color: rgb(62,149,205);text-decoration: none;color: rgb(255,255,255);font-size: 14.0px;' href='https://creator.zoho.com" + zoho.appuri + "' target='_blank'> Go to application </a></div></div></div>"
				]
			}
 			void Request.Changing_Request_Status_To_Closed(int request_id)
			{
				//Changing the request status to closed
				this_record = Employee_Check_In_Form[ID == input.request_id];
				this_record.Request_Status="Closed";
			}
 			void Crisis.View_Crisis_Details(int crisis_id)
			{
				//Opening crisis details page
				url = "#Page:Crisis_Details?Crisis_Id=" + crisis_id;
				openUrl(url,"popup window");
			}
 			void Crisis.Open_Check_In_Page(int Crisis_Id)
			{
				url = "#Page:Crisis_View_Page?Crisis_Id=" + Crisis_Id;
				openUrl(url,"same window");
			}
	}



