	functions
	{
 			string Page.currency(float num)
			{
				if(input.num != null)
				{
					num = num.round(2);
					myStr = num.toString().replaceAll("(?<!\.\d)(?<=\d)(?=(?:\d\d\d)+\b)",",");
				}
				return myStr;
			}
 			float Util.calcPercent(int recID)
			{
				getPrd = Add_Review[Product_Name == input.recID].ID.getAll();
				total = 0;
				counter = 0;
				avg = 0.0;
				for each  eachrec in getPrd
				{
					getreview = Add_Review[ID == eachrec];
					rating = getreview.Rating;
					total = rating.toLong() + total;
					counter = counter + 1;
				}
				if(counter != 0)
				{
					avg = (total / (counter * 5) * 100).round(1);
				}
				return avg;
			}
 			string Page.starRating(int Rating)
			{
				strlist = leftpad("",Rating).replaceAll(" ","1,").removeLastOccurence(",").toList();
				str = "";
				for each  loop in strlist
				{
					str = str + "<span style='color:#ecab00;font-size:18px;' class='fa fa-star'></span>";
				}
				return str;
			}
 			void appSharing.toTraders(string email)
			{
				thisapp.permissions.assignUserInProfile(email,"Trader");
			}
 			void appUnsharing.toTraders(string emailID)
			{
				thisapp.permissions.deleteuser(emailID);
			}
 			void customActions.removeFromWishlist(int wishlistRecID)
			{
				delete from Add_to_wishlist[ID == input.wishlistRecID];
				info "The Product is removed from your wishlist!";
			}
	}



