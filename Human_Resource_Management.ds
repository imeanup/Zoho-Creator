				time zone = "Asia/Calcutta"
				on start
				{
					actions 
					{
					on load
					(
						if(input.Candidate_Ref_No != null)
										{
											thisapp.permissions.deleteuser(input.Personal_Email);
										}
										if(input.Is_a_Reporting_Manager)
										{
											thisapp.permissions.assignUserInProfile(input.Official_Email,"Manager");
										}
										else
										{
											thisapp.permissions.assignUserInProfile(input.Official_Email,"Employee");
										}
					)
					}
				}
			}
