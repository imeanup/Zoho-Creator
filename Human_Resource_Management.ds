 			string TimeCalc.GetStringTime(int hrsVal, int minsVal)
			{
				strhrs = hrsVal.toString();
				strmins = minsVal.toString();
				if(input.hrsVal < 10)
				{
					strhrs = "0" + hrsVal;
				}
				if(input.minsVal < 10)
				{
					strmins = "0" + minsVal;
				}
				return strhrs + ":" + strmins;
			}
 			string TimeCalc.GetDifference(date startDate, date endDate)
			{
				millihrs = (endDate - startDate);
				if(millihrs <= 0)
				{
					timeVal = "00:00";
				}
				else
				{
					hrsVal = ((millihrs % 86400000) / 3600000).toLong();
					minsVal = ((millihrs % 86400000 % 3600000) / 60000).toLong();
					timeVal = thisapp.TimeCalc.GetStringTime(hrsVal,minsVal);
				}
				return timeVal;
			}
 			string TimeCalc.HoursCalculation(string startTime, string endTime, date dateVal)
			{
				startDate = thisapp.TimeCalc.AddHoursToDate(dateVal,startTime);
				endDate = thisapp.TimeCalc.AddHoursToDate(dateVal,endTime);
				timeVal = thisapp.TimeCalc.GetDifference(startDate,endDate);
				return timeVal;
			}
