 			void Schedule.UpdateAttendance(Add_Employee EmpRec)
			{
				DayMap = {"1":"Sunday","2":"Monday","3":"Tuesday","4":"Wednesday","5":"Thursday","6":"Friday","7":"Saturday"};
				if(Add_Weekdays[Select_Weekdays.contains(DayMap.get(zoho.currentdate.getDayOfWeek().toString()))].count() == 0)
				{
					insert into Attendance
					[
						Added_User=zoho.loginuser
						Date_field=zoho.currentdate
						Employee_ID=EmpRec.ID
						Status="Weekend"
					]
				}
				else if(Add_Holidays[From_field >= zoho.currentdate && To_field <= zoho.currentdate].count() != 0)
				{
					insert into Attendance
					[
						Added_User=zoho.loginuser
						Date_field=zoho.currentdate
						Employee_ID=EmpRec.ID
						Status="Holiday"
					]
				}
				else if(Add_Leave[From_field >= zoho.currentdate && To_field <= zoho.currentdate].count() != 0)
				{
					insert into Attendance
					[
						Added_User=zoho.loginuser
						Date_field=zoho.currentdate
						Employee_ID=EmpRec.ID
						Status="On Leave"
					]
				}
				else
				{
					insert into Attendance
					[
						Added_User=zoho.loginuser
						Date_field=zoho.currentdate
						Employee_ID=EmpRec.ID
					]
				}
				if(Add_Weekdays[Select_Weekdays.contains(DayMap.get(zoho.currentdate.subDay(1).getDayOfWeek().toString()))].count() != 0 && Add_Holidays[From_field >= zoho.currentdate.subDay(1) && To_field <= zoho.currentdate.subDay(1)].count() == 0)
				{
					if(Attendance[Employee_ID == EmpRec.ID && Date_field == zoho.currentdate.subDay(1)].Status == null)
					{
						AttendanceRec = Attendance[Employee_ID == EmpRec.ID && Date_field == zoho.currentdate.subDay(1)];
						AttendanceRec.Status="Absent";
					}
				}
			}
