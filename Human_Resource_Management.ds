 			string Time.GetDifference(date startDate, date endDate)
			{
				millihrs = (endDate - startDate);
				if(millihrs <= 0)
				{
					timeVal = "00:00";
				}
				else
				{
					hrsVal = ((millihrs % 86400000) / 3600000).toLong();
