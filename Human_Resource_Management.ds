														row.Wednesday=eachSameLog.Hours;
													}
													else if(dayVal == 5)
													{
														row.Thursday=eachSameLog.Hours;
													}
													else if(dayVal == 6)
													{
														row.Friday=eachSameLog.Hours;
													}
													else if(dayVal == 7)
													{
														row.Saturday=eachSameLog.Hours;
													}
												}
											}
										}
						)
					}
	}

