	functions
	{
 			void Send_Mail.Send_Notification(string userEmail)
			{
				appname = zoho.appname;
				sendmail
				[
					from :zoho.adminuserid
					to :userEmail
					subject :"Invitation to access '" + zoho.appname + "' "
					message :"<div style='width:100%;display:inline-block;box-sizing:border-box;border:1px solid #ddd;'><div style='padding:2%;text-align:center;'>You've been invited to use an application built on Zoho Creator by \"" + zoho.adminuserid + "\".</div><div style='background: #efefef; padding: 20.0px;margin: 20.0px 0 10.0px;text-align:center;'><h2>" + appname.toUpperCase() + "</h2><div style='margin: 20.0px 0 10.0px;'><a style='padding:10px;background-color: rgb(62,149,205);text-decoration: none;color: rgb(255,255,255);font-size: 14.0px;' href='https://creator.zoho.com" + zoho.appuri + "' target='_blank'> Go to application </a></div></div></div>"
				]
			}
 			void Vote.upvote(string record_id)
			{
				employee_Id = Add_Employee[Email_Id == zoho.loginuserid].ID;
				rec = Raise_an_Idea[ID == record_id.toLong()];
				if(rec.Up_voters_list.contains(employee_Id))
				{
					up_vote_list = rec.Up_voters_list;
					up_vote_list.removeElement(employee_Id);
					rec.Up_voters_list=up_vote_list;
				}
				else
				{
					if(rec.Down_voters_list.contains(employee_Id))
					{
						down_vote_list = rec.Down_voters_list;
						down_vote_list.removeElement(employee_Id);
						rec.Down_voters_list=down_vote_list;
					}
					up_vote_list = List();
					up_vote_list.addAll(rec.Up_voters_list);
					up_vote_list.add(employee_Id);
					rec.Up_voters_list=up_vote_list;
				}
				openUrl("#Script:page.refresh","same window");
			}
 			void Vote.downvote(string record_id)
			{
				employee_Id = Add_Employee[Email_Id == zoho.loginuserid].ID;
				rec = Raise_an_Idea[ID == record_id.toLong()];
				if(rec.Down_voters_list.contains(employee_Id))
				{
					down_vote_list = rec.Down_voters_list;
					down_vote_list.removeElement(employee_Id);
					rec.Down_voters_list=down_vote_list;
				}
				else
				{
					if(rec.Up_voters_list.contains(employee_Id))
					{
						up_vote_list = rec.Up_voters_list;
						up_vote_list.removeElement(employee_Id);
						rec.Up_voters_list=up_vote_list;
					}
					down_vote_list = List();
					down_vote_list.addAll(rec.Down_voters_list);
					down_vote_list.add(employee_Id);
					rec.Down_voters_list=down_vote_list;
				}
				openUrl("#Script:page.refresh","same window");
			}
 			void Vote.vote(int record_id)
			{
				user_count = Add_Employee[Email_Id == zoho.loginuserid].count();
				if(user_count == 1)
				{
					url = "#Page:Voting_Page?Idea=" + record_id;
					openUrl(url,"new window");
				}
			}
	}



