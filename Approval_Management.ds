			Run_Scripts_While_Adding_8 as "Run Scripts While Adding a Record"
			{
				type =  form
				form = Raise_an_Idea
				record event = on add

	on load
	{
					actions 
					{
						custom deluge script
						(
											//Adding admin as an employee
										admin_count = Add_Employee[Email_Id == zoho.adminuserid].count(ID);
										if(admin_count == 0)
										{
											insert into Add_Employee
											[
												Email_Id=zoho.adminuserid
												Name.first_name="Admin"
												Added_User=zoho.loginuser
												Give_Approval_Privileges=true
												Select_Privilege="2nd Approver (Manager)"
											]
										}
										//Listing out the managers and leads
										employee_details = Add_Employee[Email_Id == zoho.loginuserid];
										if(employee_details.ID != null)
										{
											input.Raised_By = employee_details.ID;
											managers = Add_Employee[Select_Privilege == "2nd Approver (Manager)" && Email_Id != zoho.adminuserid].Email_Id.getAll();
											leads = Add_Employee[Select_Privilege == "1st Approver (Lead)" && Email_Id != zoho.adminuserid].Email_Id.getAll();
											if(employee_details.Select_Privilege == null)
											{
												input.Select_Second_Approver:ui.add(managers);
												input.Select_First_Approver:ui.add(leads);
											}
											else if(employee_details.Select_Privilege == "1st Approver (Lead)")
											{
												input.Select_First_Approver:ui.add(managers);
											}
										}
										if(zoho.loginuserid == zoho.adminuserid)
										{
											company_detail_count = Add_Company_Detail[ID != 0].count(ID);
											if(company_detail_count == 0)
											{
												openUrl("#Form:Add_Company_Detail","popup window");
											}
											admin_count = Add_Employee[Email_Id == zoho.adminuserid].count(ID);
											if(admin_count == 0)
											{
												insert into Add_Employee
												[
													Email_Id=zoho.adminuserid
													Name.first_name="Admin"
													Added_User=zoho.loginuser
													Give_Approval_Privileges=true
													Select_Privilege="2nd Approver (Manager)"
												]
											}
										}
										if(zoho.loginuserid != zoho.adminuserid)
										{
											input.Admin = zoho.adminuser;
										}
						)
					}
	}

			}
