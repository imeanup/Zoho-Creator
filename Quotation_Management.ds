			Create_record_scripts20 as "Create record scripts"
			{
				type =  form
				form = Create_Quotation
				record event = on add

	on load
	{
					actions 
					{
						custom deluge script
						(
											//Opening Create Company form to add user's company details first and to add admin as user after installation
										if(Create_Company[ID != 0].count(ID) == 0)
										{
											if(Add_User[ID != 0].count() == 0)
											{
												insert into Add_User
												[
													Email=zoho.loginuserid
													Name1.first_name="Admin"
													Name1.last_name="User"
													Added_User=zoho.loginuser
												]
											}
											openUrl("#Form:Create_Company","same window");
										}
										// Default selecting quotation date and disabling the rate and unit in subform
										input.Company_Details = Create_Company[ID != 0].ID;
										hide Company_Details;
										input.Quote_Owner = Add_User[Email == zoho.loginuserid].ID;
										input.Quotation_Date = zoho.currentdate;
										disable Products_On_Quote.Rate;
										disable Products_On_Quote.Unit;
										disable Sub_Total;
										disable Grand_Total;
										disable Discounted_Amount;
										disable Tax_Amount;
										disable Products_On_Quote.Amount;
										//if quotation request is not null then selecting the client from the request
										quoteRequest = Request_Quotation[ID == input.Quotation_Request_Number];
										input.Client_Name = quoteRequest.Client_Name;
										//Disabling 'Send_quotation_in_mail' option when the Quotation status is new else enabling it
										if(input.Quote_Status == "New")
										{
											input.Send_quotation_in_mail = null;
											disable Send_quotation_in_mail;
											hide Send_quotation_in_mail;
										}
										else
										{
											enable Send_quotation_in_mail;
											show Send_quotation_in_mail;
										}
						)
					}
	}

			}
			Create_record_scripts21 as "Create record scripts"
			{
				type =  form
				form = Create_Quotation
				record event = on add

	on validate
	{
					actions 
					{
						custom deluge script
						(
											//if quotation is for quotation request then checking whether all the requested products/services are been added into the quote or not, if not added then alert and cancel submit
										if(input.Quotation_Request_Number != null)
										{
											quotationRequest = Request_Quotation[ID == input.Quotation_Request_Number];
											rowNumber = 0;
											for each  item in Products_On_Quote
											{
												rowNumber = rowNumber + 1;
											}
											requestRowNumber = 0;
											for each  lineItem in quotationRequest.Items_On_Quote
											{
												requestRowNumber = requestRowNumber + 1;
											}
											if(requestRowNumber != rowNumber)
											{
												alert "Please select all the requested product/services in the Products On Quote subform for the selected quotation request.!";
												cancel submit;
											}
										}
						)
					}
	}

			}
			Create_record_scripts22 as "Create record scripts"
			{
				type =  form
				form = Create_Quotation
				record event = on add

	on success
	{
					actions 
					{
						custom deluge script
						(
											//Send quotation in pdf format to client in email
										clientDetails = Add_Client[ID == input.Client_Name];
										if(input.Quote_Status == "Sent To Client" || input.Send_quotation_in_mail)
										{
											sendmail
											[
												from :zoho.adminuserid
												to :clientDetails.Email
												subject :"Quotation for " + clientDetails.Client_Name
												message :"Greetings " + clientDetails.Client_Name + ", <br><br>Thank you for your recent enquiry. Please find the enclosed quoitation for your consideration.<br><br>Should you wish to instruct me further with this quotation or if you have any questions please do not hesitate to contact us.<br><br>Yours faithfully,<br>" + input.Company_Details.Company_Name
												Attachments :template:Quote_View as PDF
											]
										}
										//if quotation is for quotation request then changing the status of quotation request to sent to client
										if(input.Quotation_Request_Number != null)
										{
											quotationRequest = Request_Quotation[ID == input.Quotation_Request_Number];
											quotationRequest.Status="Sent to client";
										}
										if(input.Quote_Status == "New" && input.Send_quotation_in_mail)
										{
											input.Quote_Status = "Sent To Client";
										}
						)
					}
	}

			}
