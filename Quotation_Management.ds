			Field_scripts7 as "Field scripts"
			{
				type =  form
				form = Create_Quotation
				record event = on add or edit

	on user input of Products_On_Quote.Product_Services_Name
	{
					actions 
					{
						custom deluge script
						(
											//Updating rate, sub_total and grand_total fields based on input of the product/service
										if(row.Product_Services_Name != null)
										{
											row.Rate=row.Product_Services_Name;
											ProductRec = Add_Product_Service[ID == row.Product_Services_Name];
											row.Unit=ProductRec.Unit;
											//Updating the sub total and grand total
											if(input.Sub_Total != 0 && input.Sub_Total != null && row.Quantity != null)
											{
												input.Sub_Total = input.Sub_Total - row.Amount;
												discountAmount = input.Discount / 100 * input.Sub_Total;
												taxAmount = input.Tax / 100 * input.Sub_Total;
												input.Grand_Total = input.Sub_Total - discountAmount + taxAmount;
												input.Discounted_Amount = discountAmount;
												input.Tax_Amount = taxAmount;
											}
											row.Quantity=0;
											row.Amount=0;
										}
										else
										{
											row.Rate=null;
											row.Unit=null;
											//Updating the sub total and grand total
											if(input.Sub_Total != 0 && input.Sub_Total != null && row.Quantity != null)
											{
												input.Sub_Total = input.Sub_Total - row.Amount;
												discountAmount = input.Discount / 100 * input.Sub_Total;
												taxAmount = input.Tax / 100 * input.Sub_Total;
												input.Grand_Total = input.Sub_Total - discountAmount + taxAmount;
												input.Discounted_Amount = discountAmount;
												input.Tax_Amount = taxAmount;
											}
											row.Quantity=null;
											row.Amount=null;
										}
						)
					}
	}

