			Field_scripts15 as "Field scripts"
			{
				type =  form
				form = Create_Quotation
				record event = on add or edit

	on add row of Products_On_Quote
	{
					actions 
					{
						custom deluge script
						(
											//if quotation request is for quotation request then fetching products/services from the request and adding it to the row
										if(input.Quotation_Request_Number != null)
										{
											clear row.Product_Services_Name;
											quotationRequest = Request_Quotation[ID == input.Quotation_Request_Number];
											rowNumber = 0;
											for each  item in Products_On_Quote
											{
												rowNumber = rowNumber + 1;
											}
											requestRowNumber = 0;
											for each  lineItem in quotationRequest.Items_On_Quote
											{
												requestRowNumber = requestRowNumber + 1;
												if(requestRowNumber == rowNumber)
												{
													row.Product_Services_Name=lineItem.Product_Service;
													row.Rate=row.Product_Services_Name;
													ProductRec = Add_Product_Service[ID == row.Product_Services_Name];
													row.Unit=ProductRec.Unit;
													row.Quantity=lineItem.Quantity;
													//Selected item amount calculation and total amount calculation
													row.Amount=row.Rate.Rate * row.Quantity;
													totalAmount = 0.0;
													for each  item in Products_On_Quote
													{
														totalAmount = totalAmount + item.Amount;
													}
													input.Sub_Total = totalAmount;
													discountAmount = input.Discount / 100 * input.Sub_Total;
													taxAmount = input.Tax / 100 * input.Sub_Total;
													input.Grand_Total = input.Sub_Total - discountAmount + taxAmount;
													input.Discounted_Amount = discountAmount;
													input.Tax_Amount = taxAmount;
												}
											}
										}
										else
										{
											//Removing already selected items in quote from the product/services name options
											products = Add_Product_Service[ID != 0].ID.getAll();
											for each  item in Products_On_Quote
											{
												products.removeelement(item.Product_Services_Name);
											}
											row.Product_Services_Name:ui.add(products);
										}
						)
					}
	}

			}
