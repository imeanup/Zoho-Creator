										if(detail_rec_count == 0)
										{
											openUrl("#Form:Add_Organization","popup window");
										}
						)
					}
	}

			}
			Run_Scripts_While_Adding_6 as "Run Scripts - While Adding a Record"
			{
				type =  form
				form = Add_Employee
				record event = on add

	on validate
	{
					actions 
					{
						custom deluge script
						(
											user_count = Add_Employee[Email_Id == input.Email_Id].count(ID);
										if(user_count == 1)
										{
											alert "This user account has been already added to employees";
											cancel submit;
										}
						)
					}
	}

			}
			Run_Scripts_While_Adding_7 as "Run Scripts - While Adding a Record"
			{
				type =  form
				form = Add_Employee
				record event = on add

	on success
	{
					actions 
					{
						custom deluge script
						(
											//Sharing the forms to the employees
										if(input.Email_Id != zoho.adminuserid)
										{
											if(Choose_Category == "Company Staff")
											{
												thisapp.permissions.assignUserInProfile(input.Email_Id,"Company Staff");
												thisapp.Notification.Send_Notification(input.Email_Id);
											}
											else if(Choose_Category == "Management Employee")
											{
												thisapp.permissions.assignUserInProfile(input.Email_Id,"Employee");
												thisapp.Notification.Send_Notification(input.Email_Id);
											}
										}
						)
					}
	}

			}
			Run_Scripts_While_Editing1 as "Run Scripts - While Editing a Record"
			{
				type =  form
				form = Add_Employee
				record event = on edit

	on validate
	{
					actions 
					{
						custom deluge script
						(
											//Removes the user permission from old mail id and gives it to new mail id
										if(Email_Id != zoho.adminuserid)
										{
											if(Choose_Category == "Company Staff")
											{
												if(old.Email_Id != input.Email_Id)
												{
													thisapp.permissions.deleteuser(old.Email_Id);
													thisapp.permissions.assignUserInProfile(input.Email_Id,"Company Staff");
													thisapp.Notification.Send_Notification(input.Email_Id);
												}
											}
											else if(Choose_Category == "Management Employee")
											{
												if(old.Email_Id != input.Email_Id)
												{
													thisapp.permissions.deleteuser(old.Email_Id);
													thisapp.permissions.assignUserInProfile(input.Email_Id,"Employee");
													thisapp.Notification.Send_Notification(input.Email_Id);
												}
											}
										}
						)
					}
	}

			}
			Run_Scripts_While_Deletin1 as "Run Scripts - While Deleting a Record"
			{
				type =  form
				form = Add_Employee
				record event = on delete

	on validate
	{
					actions 
					{
						custom deluge script
						(
											//Removes the user permisiion from this email id
										if(Email_Id != zoho.adminuserid)
										{
											Assigned_Incidents = Raise_Incident[Handled_By == input.ID && Status != "Closed"].count(ID);
											if(Assigned_Incidents != 0)
											{
												thisapp.permissions.deleteuser(input.Email_Id);
											}
											else
											{
												info "This employee has assigned incidents you can't delete him right now delete after the incidents were closed";
											}
										}
						)
					}
	}

			}
			Run_Scripts_While_Adding_8 as "Run Scripts - While Adding a Record"
			{
				type =  form
				form = Raise_Incident
				record event = on add

	on load
	{
