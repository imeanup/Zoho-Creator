	functions
	{
 			void Incident.Assigning_Employee(int incident_name)
			{
				//Redirects to the employee assigning form
				url = "#Form:Assign_Employee?Incident_Name=" + incident_name;
				openUrl(url,"popup window");
			}
 			void Notification.Send_Notification(string userEmail)
			{
				appname = zoho.appname;
				sendmail
				[
					from :zoho.adminuserid
					to :userEmail
					subject :"Invitation to access '" + zoho.appname + "' "
					message :"<div style='width:100%;display:inline-block;box-sizing:border-box;border:1px solid #ddd;'><div style='padding:2%;text-align:center;'>You've been invited to use an application built on Zoho Creator by \"" + zoho.adminuserid + "\".</div><div style='background: #efefef; padding: 20.0px;margin: 20.0px 0 10.0px;text-align:center;'><h2>" + appname.toUpperCase() + "</h2><div style='margin: 20.0px 0 10.0px;'><a style='padding:10px;background-color: rgb(62,149,205);text-decoration: none;color: rgb(255,255,255);font-size: 14.0px;' href='https://creator.zoho.com" + zoho.appuri + "' target='_blank'> Go to application </a></div></div></div>"
				]
			}
 			void Incident.Closing_Incident(int incident_id)
			{
				//Redirects to the reporting form
				employee_id = Raise_Incident[ID == input.incident_id].Handled_By;
				employee_name = Add_Employee[ID == input.employee_id].ID;
				url = "#Form:Incident_Reporting?Incident_Name=" + incident_id + "&Handled_By=" + employee_name;
				openUrl(url,"popup window");
			}
	}



	workflow
	{
		form
		{
			Run_Scripts_On_Click_of_P as "Run Scripts - On Click of 'Proceed' Button"
			{
				type =  form
				form = Assign_Employee
				button = Assign
				on click
				{
					actions 
					{
					on click
					(
						//Changing the status as employee assigned and sent him an email about the incident
										rec_id = Raise_Incident[ID == input.Incident_Name];
										rec_id.Handled_By=input.Select_Employee;
										rec_id.Status="Employee Assigned";
										employee_email_id = Add_Employee[ID == input.Select_Employee].Email_Id;
										employee_name = Add_Employee[ID == input.Select_Employee].Name;
										url = "https://app.zohocreator.com" + zoho.appuri + "#Report:My_Assigned_Incidents";
										sendmail
										[
											from :zoho.adminuserid
											to :employee_email_id
											subject :"New Incident"
											message :("Hi " + employee_name) + ",<br></br>A New Incident was assigned to you <br></br><a href=" + url + ">Click here to take action</a>"
										]
										openUrl("#Script:page.refresh","same window");
					)
					}
				}
			}
			Run_Scripts_On_Click_of_P1 as "Run Scripts - On Click of 'Proceed' Button"
			{
				type =  form
				form = Incident_Reporting
				button = Submit
				on click
				{
					actions 
					{
					on click
					(
						//Updates the notes to the related incident and changing the status as closed
										rec_id = Raise_Incident[ID == input.Incident_Name];
										rec_id.Investigation_Notes=input.Report;
										rec_id.Status="Closed";
										openUrl("#Script:page.refresh","same window");
					)
					}
				}
			}
			Run_Scripts_On_Always1 as "Run Scripts - On Always"
			{
				type =  form
				form = Raise_Incident
				record event = on add or edit

	field rules
	{
					actions 
					{
						hide Investigation_Notes,Handled_By,Status;
						disable Complaint_By;
					}
	}

			}
			Run_Scripts_On_Always2 as "Run Scripts - On Always"
			{
				type =  form
				form = Raise_Incident
				record event = on add or edit

	on load
	{
					actions 
					{
						custom deluge script
						(
											//Redirects to add organization details
										detail_rec_count = Add_Organization[ID != 0].count(ID);
										if(detail_rec_count == 0)
										{
											openUrl("#Form:Add_Organization","popup window");
										}
										//Adding admin as an user by default
										if(zoho.loginuserid == zoho.adminuserid)
										{
											admin_count = Add_Employee[Email_Id == zoho.adminuserid].count(ID);
											if(admin_count == 0)
											{
												insert into Add_Employee
												[
													Email_Id=zoho.adminuserid
													Choose_Category="Company Staff"
													Added_User=zoho.loginuser
													Name.first_name="Admin"
												]
											}
										}
										//Changing status
										input.Status = "Request Sent";
										input.Time_of_Event = zoho.currenttime;
										employee_count = Add_Employee[ID != 0].count(Name);
										if(employee_count == 1 && detail_rec_count == 1)
										{
											openUrl("#Form:Add_Employee","same window");
										}
						)
					}
	}

			}
			Run_Scripts_On_Always3 as "Run Scripts - On Always"
			{
				type =  form
				form = Raise_Incident
				record event = on add or edit

	on user input of Time_of_Event
	{
					actions 
					{
						custom deluge script
						(
											//Check for invalid time
										if(input.Time_of_Event > zoho.currenttime)
										{
											alert "Enter a valid time";
											input.Time_of_Event = null;
										}
						)
					}
	}

			}
			Run_Scripts_While_Adding_5 as "Run Scripts - While Adding a Record"
			{
				type =  form
				form = Add_Employee
				record event = on add

	on load
	{
					actions 
					{
						custom deluge script
						(
											//Redirects to add organization details
