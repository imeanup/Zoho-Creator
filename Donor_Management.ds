		functions
		{
			Activate_Campaign as "Activate Campaign"
			{
				type =  functions
				form = New_Campaign
				on start
				{
					actions 
					{
					on click
					(
						if(input.Donated_Amount >= input.Goal_Amount)
										{
											input.Campaign_Status = "Goal Reached";
										}
										else if(input.Deadline > zoho.currentdate)
										{
											input.Campaign_Status = "Expired";
										}
										else
										{
											input.Campaign_Status = "Active";
										}
					)
					}
				}
			}
			Share as "Share"
			{
				type =  functions
				form = New_Campaign
				on start
				{
					actions 
					{
					on click
					(
						openUrl("#Form:Share_Campaign?Campaign=" + input.ID,"popup window");
					)
					}
				}
			}
			Donate as "Donate"
			{
				type =  functions
				form = New_Campaign
				on start
				{
					actions 
					{
					on click
					(
						openUrl("#Form:Donation?Campaign_Number=" + input.ID,"Popup window");
					)
					}
				}
			}
			Deactivate_Campaign as "Deactivate Campaign"
			{
				type =  functions
				form = New_Campaign
				on start
				{
					actions 
					{
					on click
					(
						input.Campaign_Status = "Deactivated";
					)
					}
				}
			}
			Approve_Campaign as "Approve Campaign"
			{
				type =  functions
				form = New_Campaign
				on start
				{
					actions 
					{
					on click
					(
						input.Campaign_Status = "Active";
										thisapp.Volunteer.notifyNewCampaign(input.ID);
					)
					}
				}
			}
			Reject_Campaign as "Reject Campaign"
			{
				type =  functions
				form = New_Campaign
				on start
				{
					actions 
					{
					on click
					(
						input.Campaign_Status = "Rejected";
										sendmail
										[
											from :input.Organizer.Email
											to :zoho.adminuserid
											subject :"Campaign Request-Rejected"
											message :"Hi,<div><br></div><div>Sorry your request&nbsp;" + input.Request_Number + " has been rejected.</div><div><br></div><div>Thank you very much for your interest in our organization.</div><div><br></div><div>Thanks,</div><div>" + zoho.adminuser + "<br></div>"
										]
					)
					}
				}
			}
			Approve_Volunteer as "Approve Volunteer"
			{
				type =  functions
				form = Add_Contact
				on start
				{
					actions 
					{
					on click
					(
						input.Volunteer_Status = "Approved";
										input.Contact_Type = "Volunteer";
										thisapp.Volunteer.shareWithVolunteer(input.Email);
					)
					}
				}
			}
			Reject_Volunteer as "Reject Volunteer"
			{
				type =  functions
				form = Add_Contact
				on start
				{
