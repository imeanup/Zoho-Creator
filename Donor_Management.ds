	functions
	{
 			void Volunteer.notifyNewCampaign(int recID)
			{
				campaignDetails = New_Campaign[ID == input.recID];
				if(campaignDetails.Deadline == null)
				{
					deadlineDate = "NA";
				}
				else
				{
					deadlineDate = campaignDetails.Deadline.toString();
				}
				for each  eachContact in Add_Contact[Contact_Type == "Volunteer" && Volunteer_Status == "Approved"]
				{
					sendmail
					[
						from :zoho.adminuserid
						to :eachContact.Email
						subject :"[" + campaignDetails.Request_Number + "]New Campaign - " + campaignDetails.Title
						message :("<font face=\"Arial, Helvetica, sans-serif\">Hi,</font><div><font face=\"Arial, Helvetica, sans-serif\"><br></font></div><div><font face=\"Arial, Helvetica, sans-serif\">New fund request has been raised.</font></div><div><span style=\"font-family: Arial, Helvetica, sans-serif;\"><br></span></div><div><span style=\"font-family: Arial, Helvetica, sans-serif;\">Please find the below details to spread the world.</span></div><div><span style=\"font-family: Arial, Helvetica, sans-serif;\"><br></span></div><blockquote style=\"border: 1px solid rgb(204, 204, 204); padding: 7px; background-color: rgb(245, 245, 245);\"><div><b><font face=\"Arial, Helvetica, sans-serif\" size=\"2\">Title:" + campaignDetails.Title + "</font></b></div><div><b><font face=\"Arial, Helvetica, sans-serif\" size=\"2\">Category:" + campaignDetails.Category + "</font></b></div><div><b><font face=\"Arial, Helvetica, sans-serif\" size=\"2\">Description:" + campaignDetails.Description + "</font></b></div><div><b><font face=\"Arial, Helvetica, sans-serif\" size=\"2\">Goal Amount:" + campaignDetails.Goal_Amount + "</font></b></div><div><b><font face=\"Arial, Helvetica, sans-serif\" size=\"2\">Deadline:" + deadlineDate + "</font></b></div><div><b><font face=\"Arial, Helvetica, sans-serif\" size=\"2\">Beneficiary Name:" + campaignDetails.Name) + "</font></b></div></blockquote><div></div><div><font face=\"Arial, Helvetica, sans-serif\"><br></font></div><div><font face=\"Arial, Helvetica, sans-serif\"><a href=\"https://app.zohocreator.com" + zoho.appuri + "#Report:Active_Campaigns?Request_Number=" + campaignDetails.Request_Number + "\" title=\"Login to Donor Management\" target=\"_blank\">Click Here</a>&nbsp;to&nbsp;</font><span style=\"font-family: Arial, Helvetica, sans-serif;\">donate&nbsp;</span><font face=\"Arial, Helvetica, sans-serif\">or share the details.</font></div><div><font face=\"Arial, Helvetica, sans-serif\"><br></font></div><div><span style=\"font-family: Arial, Helvetica, sans-serif;\">Try to be a rainbow in someone's cloud.</span></div><div><font face=\"Arial, Helvetica, sans-serif\"><br></font></div><div><font face=\"Arial, Helvetica, sans-serif\">Regards,</font></div><div><font face=\"Arial, Helvetica, sans-serif\" size=\"1\">" + zoho.adminuser + "</font><br></div>"
					]
				}
			}
 			void Volunteer.shareWithVolunteer(string volunteerMail)
			{
				thisapp.permissions.assignUserInProfile(volunteerMail,"Volunteer");
				sendmail
				[
					from :zoho.adminuserid
					to :volunteerMail
					subject :"Volunteer Request-Accepted"
					message :"<div>Congratulations...</div><div><br></div><div>We are very happy to have you as volunteer in our organization.</div><div><br></div><div><a href=\"https://app.zohocreator.com" + zoho.appuri + "\" target=\"_blank\">Click Here</a> to access our application.&nbsp;</div><div></div><div><br></div><div><span style=\"color: rgb(51, 51, 51); font-family: &quot;Helvetica Neue&quot;, Helvetica, Arial, sans-serif; font-size: 14px; line-height: 20px; background-color: rgb(255, 255, 255);\">What you do today can improve all your tomorrows.</span></div><div><br></div><div>Thanks,</div><div>" + zoho.adminuser + "<br style=\"box-sizing: border-box; background-color: rgb(255, 255, 255);\"><br></div>"
				]
			}
 			void Volunteer.volunteerRequest(string volunteerEmail)
			{
				volunteersRec = Add_Contact[Email == input.volunteerEmail];
				//Notify the admin regards new volunteer request
				sendmail
				[
					from :volunteerEmail
					to :zoho.adminuserid
					subject :"New Volunteer Request"
					message :("<div>Hi,</div><div><br></div><div>I am interested to be a volunteer in your organization, Please accept my humble request.</div><div><br></div><div>Please find my details on below link.</div><div><br></div><a href=\"https://app.zohocreator.com" + zoho.appuri + "#Report:Pending_Volunteers?Personal_Email=" + volunteerEmail + "\" target=\"_blank\">Click here</a><div><br></div><div><div>Thanks & Regards,</div><div>" + volunteersRec.Name) + "<br></div><div><br></div></div>"
				]
				//Notify the volunteer regards registration pending status
				sendmail
				[
					from :zoho.adminuserid
					to :volunteerEmail
					subject :"Registration completed successfully"
					message :"Hi,<div><br></div><div>Thanks for your interest in extending hands to help others.</div><div><br></div><div>Your details has been received and it is in progress of verification.</div><div><br></div><div>We will send you the confirmation mail shortly.</div><div><br></div><div>Thanks,</div><div>" + zoho.adminuser + "&nbsp;</div>"
				]
			}
	}



	workflow
	{
		form
		{
			Run_Scripts_On_Click_of_S as "Run Scripts - On Click of 'Submit' Button"
			{
				type =  form
				form = Registration
				button = Submit
				on click
				{
					actions 
					{
					on click
					(
						//Updating the registration details in contacts
										if(zoho.adminuserid == zoho.loginuserid)
										{
											volunteerStatus = "Approved";
											referredBy = zoho.adminuserid;
											thisapp.Volunteer.shareWithVolunteer(input.Email);
										}
										else
										{
											volunteerStatus = "Pending";
											thisapp.Volunteer.volunteerRequest(input.Email);
											loginContactDetails = Add_Contact[Email == zoho.loginuserid];
											if(loginContactDetails.Contact_Type == "Volunteer")
											{
												referredBy = zoho.loginuserid;
											}
											else
											{
												referredBy = loginContactDetails.Referred_By;
											}
										}
										contactDetails = Add_Contact[Email == input.Email];
										if(contactDetails.count() != 0)
										{
											if(contactDetails.Contact_Type == "Administrator" || contactDetails.Volunteer_Status == "Approved")
											{
												alert "Sorry, The email address is already registered as " + contactDetails.Contact_Type + "!";
											}
											else if(contactDetails.Volunteer_Status == "Pending")
											{
												alert "Sorry, The email address is already registered and is been waiting for the approval!";
											}
											else if(contactDetails.Volunteer_Status == "Rejected" || contactDetails.Volunteer_Status == "Not a Volunteer" && input.Email != zoho.loginuserid)
											{
												//Validating the OTP here
												if(input.OTP != null)
												{
													otpDetails = One_Time_Password[Email == input.Email && OTP == input.OTP];
													if(otpDetails.count() == 0)
													{
														alert "Invalid OTP! Please try again.";
													}
													else
													{
														if(zoho.adminuserid == zoho.loginuserid)
														{
															contactDetails.Contact_Type="Volunteer";
														}
														contactDetails.Name=input.Name;
														contactDetails.Phone_Number=input.Phone_Number;
														contactDetails.Address=input.Address;
														contactDetails.Volunteer_Status=volunteerStatus;
													}
												}
												else
												{
													alert "OTP is required to proceed further, Please enter the OTP!";
												}
											}
										}
										else
										{
											//Validating the OTP here
											if(input.OTP != null)
											{
												otpDetails = One_Time_Password[Email == input.Email && OTP == input.OTP];
												if(otpDetails.count() == 0)
												{
													alert "Invalid OTP! Please try again.";
												}
												else
												{
													insert into Add_Contact
													[
														Contact_Type="Volunteer"
														Name=input.Name
														Phone_Number=input.Phone_Number
														Email=input.Email
														Address=input.Address
														Referred_By=referredBy
														Volunteer_Status=volunteerStatus
														Added_User=zoho.loginuser
													]
												}
											}
											else
											{
												alert "OTP is required to proceed further, Please enter the OTP!";
											}
										}
					)
					}
				}
			}
			Run_Scripts_On_Click_of_S1 as "Run Scripts - On Click of 'Submit' Button"
			{
				type =  form
				form = Add_Visitor
				button = Add
				on click
				{
					actions 
					{
					on click
					(
						//Updating the visitor details in contacts
										if(zoho.adminuserid == zoho.loginuserid)
										{
											referredBy = zoho.adminuserid;
										}
										else
										{
											loginContactDetails = Add_Contact[Email == zoho.loginuserid];
											if(loginContactDetails.Contact_Type == "Volunteer")
											{
												referredBy = zoho.loginuserid;
											}
											else
											{
												referredBy = loginContactDetails.Referred_By;
											}
										}
										contactDetails = Add_Contact[Email == input.Email];
										if(contactDetails.count() != 0)
										{
											input.Name = null;
											input.Phone_Number = null;
											if(contactDetails.Contact_Type == "Visitor")
											{
												accessibleForString = contactDetails.Accessible_For + "," + zoho.loginuserid;
												accessList = accessibleForString.toList().distinct();
												contactDetails.Accessible_For=accessList.toString();
											}
											else if(contactDetails.Contact_Type == "Volunteer" && contactDetails.Volunteer_Status == "Rejected")
											{
												contactDetails.Contact_Type="Visitor";
												contactDetails.Referred_By=referredBy;
												contactDetails.Volunteer_Status="Not a Volunteer";
												contactDetails.Accessible_For=zoho.loginuserid;
												thisapp.permissions.assignUserInProfile(input.Email,"Visitor");
											}
											else
											{
												alert "Sorry, The email address you have entered is already registered as " + contactDetails.Contact_Type;
											}
										}
										else
										{
											insert into Add_Contact
											[
												Added_User=zoho.loginuser
												Contact_Type="Visitor"
												Name=input.Name
												Email=input.Email
												Phone_Number=input.Phone_Number
												Referred_By=referredBy
												Volunteer_Status="Not a Volunteer"
												Accessible_For=zoho.loginuserid
											]
											thisapp.permissions.assignUserInProfile(input.Email,"Visitor");
										}
					)
					}
