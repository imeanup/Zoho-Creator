			Run_Scripts_On_Click_of_P as "Run Scripts - On Click of 'Proceed' Button"
			{
				type =  form
				form = OTP_Confirmation
				button = Proceed
				on click
				{
					actions 
					{
					on click
					(
						//Decrypting the email
										decryptedEmail = zoho.encryption.aesDecode(zoho.loginuserid,input.Encoded_Email);
										//Generating the One Time Password and update the same in database
										randomNumberFromTime = (zoho.currenttime.toLong() / (zoho.currenttime.getSeconds() * zoho.currenttime.getDayOfYear() * zoho.currenttime.getMinutes() + 1)).toLong();
										randomNumberFromUser = input.Encoded_Email.getAlphaNumeric().removeAllAlpha().toLong();
										oneTimePassword = (randomNumberFromTime + randomNumberFromUser) % 10000;
										otpDetails = One_Time_Password[Email == decryptedEmail];
										if(otpDetails.count() == 0)
										{
											insert into One_Time_Password
											[
												Added_User=zoho.loginuser
												Email=decryptedEmail
												OTP=oneTimePassword
											]
										}
										else
										{
											otpDetails.OTP=oneTimePassword;
										}
										//Sending One Time Password in mail
										sendmail
										[
											from :zoho.adminuserid
											to :decryptedEmail
											subject :"One Time Password"
											message :"Your verification code is&nbsp;" + oneTimePassword + "<div><br></div><div>Please use this code to complete your registration process.</div><div><br></div><div>Regards,</div><div>" + zoho.adminuser + "</div>"
										]
										openUrl("#Form:Registration?Email=" + decryptedEmail,"same window");
					)
					}
				}
			}
