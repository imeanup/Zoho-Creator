			Run_Scripts_While_Adding_21 as "Run Scripts - While Adding a Record"
			{
				type =  form
				form = Add_Visitor
				record event = on add

	on user input of Email
	{
					actions 
					{
						custom deluge script
						(
											//Displaying the necessary fields according to the user's input
										contactDetails = Add_Contact[Email == input.Email];
										if(contactDetails.count() != 0)
										{
											hide Name;
											hide Phone_Number;
											input.Name = null;
											input.Phone_Number = null;
											show ExistenceAlert;
											//Email existence check
											if(contactDetails.Contact_Type == "Visitor" || (contactDetails.Contact_Type == "Volunteer" && contactDetails.Volunteer_Status == "Rejected"))
											{
												input.ExistenceAlert = "<font color='#ff3333'>The email address you have entered is already registered as " + contactDetails.Contact_Type + "!</font><div><font color='#ff3333'><br></font><div><font color='#ff3333'>Do you wish to add this contact as your visitor?</font></div></div><div><br></div>";
											}
											else
											{
												input.ExistenceAlert = "<font color='#ff3333'>Sorry, The email address you have entered is already registered as " + contactDetails.Contact_Type + "!</font>";
											}
										}
										else
										{
											show Name;
											show Phone_Number;
											hide ExistenceAlert;
										}
						)
					}
	}

			}
