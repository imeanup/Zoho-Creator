	section Bugs
	{
		icon = "design-bug"
		form New_Bug
		{
			displayname = "Submit Bug"
			success message = "Data Added Successfully!"
			field alignment = left
			feature property = 32
			Section
			(
				type = section
	 			row = 1
	 			column = 0   
				width = medium
			)
			Bug_ID
			(
    			type = text
				displayname = "Bug - ID"
	 			row = 1
	 			column = 1   
				width = 200px
			)
			must have Bug_Title
			(
    			type = text
				displayname = "Bug Title"
	 			row = 1
	 			column = 1   
				width = 200px
			)
			must have Products
			(
				type = picklist
				displayname = "Milestone"
				values  = Add_Miletone.ID
    			displayformat = [Product_Name]
				allow new entries = true
				[
					displayname = "Add New Milestone"
				]
				searchable = true
				sortorder = ascending
	 			row = 1
	 			column = 1   
				width = 200px
			)
			must have Notes
			(
    			type = textarea
				displayname = "Description"
				height = 100px
	 			row = 1
	 			column = 1   
				width = 200px
			)
			Steps_To_Reproduce
			(
    			type = textarea
				displayname = "Steps To Reproduce"
				height = 100px
	 			row = 1
	 			column = 1   
				width = 200px
			)
			Module_If_any
			(
    			type = text
				displayname = "Build (If any)"
	 			row = 1
	 			column = 1   
				width = 200px
			)
			Deadline
			(
    			type = date
				alloweddays = 0,1,2,3,4,5,6
	 			row = 1
	 			column = 1   
				width = 200px
			)
			must have Priority
			(
				type = radiobuttons
				values = {"High", "Medium", "Low"}
				layout = 3
	 			row = 1
	 			column = 1   
				width = medium
			)
			must have Status
			(
				type = picklist
				values = {"New", "Fixed", "Reopened", "Rejected", "Open", "Not Reproducible", "Not Fixing Now", "Review/Testing"}
				initial value = "New"
	 			row = 1
	 			column = 1   
				width = 200px
			)
			Reporter
			(
				type = picklist
				values  = Add_User.ID
    			displayformat = [Name1.first_name + " - " + Name1.last_name]
				sortorder = ascending
	 			row = 1
	 			column = 1   
				width = 200px
			)
			Assignees1
			(
				type = list
				displayname = "Assignees"
				values  = Add_User.ID
    			displayformat = [Name1.first_name + " - " + Name1.last_name]
				sortorder = ascending
				height = 60px
	 			row = 1
	 			column = 1   
				width = 200px
			)
			Auto_Number
			(
    			type = autonumber
				displayname = "Auto Number"
				start index = 1
	 			row = 1
	 			column = 1   
				width = medium
			)
			must have Screenshot
			(
    			type = image
				source  = public_url,file
				aspect ratio = original
				camera = primary
				show gallery = true
				switch camera = true
	 			row = 1
	 			column = 1   
				width = 200px
			)
			Comments
			(
				type = grid
				values  = Comments.ID
    			bidirectional = Submitted_Bug_Rec_ID
	 			row = 1
	 			column = 1   
				width = medium
			)
			Comment
			(
    			type = textarea
				height = 100px
	 			row = 1
	 			column = 1   
				width = 200px
			)
			must have adminEmail
			(
    			type = email
	 			row = 1
	 			column = 1   
				width = 200px
			)
			Submitted_Date
			(
    			type = date
				displayname = "Submitted Date"
				alloweddays = 0,1,2,3,4,5,6
	 			row = 1
	 			column = 1   
				width = medium
			)
	
			customize
			(
				icon = "ui-2-square-add-11"
			)
			actions
			{
				on add
				{
					submit
					(
   						type = submit
   						displayname = "Submit"
					)
					reset
					(
   						type = reset
   						displayname = "Reset"
					)
				}
				on edit
				{
