	functions
	{
 			string getVenues(list venuesList)
			{
				output = "";
				for each  venue in venuesList
				{
					output = output + Add_Venue[ID == venue.toLong()].Location;
				}
				return output;
			}
 			void bookEvent(Create_New_Event eventRow)
			{
				if(eventRow.Subscription_Type == "Paid")
				{
					openUrl("#Form:Book_Paid_Events?zc_Header=false&zc_BdrClr=transparent&Event=" + eventRow.ID,"Popup Window");
				}
				else
				{
					openUrl("#Form:Book_Free_Events?zc_Header=false&zc_BdrClr=transparent&Event=" + eventRow.ID,"Popup Window");
				}
			}
 			string timeCalculation(date addedTime)
			{
				curr = zoho.currenttime.subMinutes(20);
				days = days360(addedTime,zoho.currenttime);
				addedTimePlusHour = addedTime.addHour(days * 24);
				if(addedTimePlusHour > zoho.currenttime)
				{
					days = days - 1;
					hour = 24 - (addedTime.getHour() - zoho.currenttime.getHour());
					mins = 60 - addedTime.getMinutes() + zoho.currenttime.getMinutes();
					if(mins >= 60)
					{
						mins = mins - 60;
					}
					else
					{
						hour = hour - 1;
					}
					if(days == 0)
					{
						eventAddedTime = hour + " hour " + mins + " mins ago";
					}
					else
					{
						eventAddedTime = days + " days " + hour + " hours ago";
					}
				}
				else if(addedTimePlusHour < zoho.currenttime)
				{
					hour = zoho.currenttime.getHour() - addedTime.getHour();
					mins = 60 - addedTime.getMinutes() + zoho.currenttime.getMinutes();
					if(mins >= 60)
					{
						mins = mins - 60;
					}
					else
					{
						hour = hour - 1;
					}
					if(days == 0)
					{
						eventAddedTime = hour + " hour " + mins + " mins ago";
					}
					else
					{
						eventAddedTime = days + " days " + hour + " hours ago";
					}
				}
				info zoho.currenttime;
				return eventAddedTime;
			}
 			void cancelBooking(int eventRecId, int bookedEventID)
			{
				rec = Create_New_Event[ID == input.eventRecId];
				if(rec.Subscription_Type == "Paid")
				{
					bookedPaidEvent = Book_Paid_Events[ID == input.bookedEventID];
					rec.Available_Entries=rec.Available_Entries + bookedPaidEvent.Required_Entries;
					venu = Add_Venue[ID == bookedPaidEvent.Venue].Location;
					sendmail
					[
						from :zoho.adminuserid
						to :bookedPaidEvent.Contact_Email
						subject :"Registration for  " + rec.Event_Name + " Cancelled Successfully!"
						message :("<divstyle=\"text- align:left;\">Your Registration cancellation details are mentioned below</div>\n<br type=\"_moz\">\n<table>\n<tbody>\n<tr>\n<td align=\"right\">Event Name:</td>\n<td>" + rec.Event_Name + "</td>\n</tr>\n<tr>\n<td align='right'>Venues:</td>\n<td>" + venu) + "</td></tr>\n<tr>\n<td align=\"right\">Start Time:</td>\n<td>" + rec.Event_Start_Time + "</td>\n</tr>\n<tr>\n<td align=\"right\">End Time:</td>\n<td>" + rec.Event_End_Time + "</td>\n</tr>\n<tr>\n<td align=\"right\">Number of people:</td>\n<td>" + bookedPaidEvent.Required_Entries + "</td>\n</tr>\n</tbody>\n</table>"
					]
					delete from Add_Customer[Book_Paid_Event_Rec_ID == bookedPaidEvent.ID];
					delete from Book_Paid_Events[ID == input.bookedEventID];
				}
				if(rec.Subscription_Type == "Free")
				{
					bookedFreeEvent = Book_Free_Events1[ID == input.bookedEventID];
					rec.Available_Entries=rec.Available_Entries + bookedFreeEvent.Number_of_people;
					venu = Add_Venue[ID == bookedPaidEvent.Venue].Location;
					sendmail
					[
						from :zoho.adminuserid
						to :bookedFreeEvent.Contact_Email
						subject :"Registration for  " + rec.Event_Name + " Cancelled Successfully!"
						message :("<divstyle=\"text- align:left;\">Your Registration cancellation details are mentioned below</div>\n<br type=\"_moz\">\n<table>\n<tbody>\n<tr>\n<td align=\"right\">Event Name:</td>\n<td>" + rec.Event_Name + "</td>\n</tr>\n<tr>\n<td align='right'>Venues:</td>\n<td>" + venu) + "</td></tr>\n<tr>\n<td align=\"right\">Start Time:</td>\n<td>" + rec.Event_Start_Time + "</td>\n</tr>\n<tr>\n<td align=\"right\">End Time:</td>\n<td>" + rec.Event_End_Time + "</td>\n</tr>\n<tr>\n<td align=\"right\">Number of people:</td>\n<td>" + bookedFreeEvent.Number_of_people + "</td>\n</tr>\n</tbody>\n</table>"
					]
