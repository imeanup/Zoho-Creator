				}
			}
 			string timeDiffCalc(date addedTime)
			{
				timeDifInSec = 0;
				totMins = 0;
				totHours = 0;
				totDays = 0;
				timeDifInSec = floor(((zoho.currenttime - addedTime)) / 1000);
				totMins = floor(timeDifInSec / 60);
				totHours = floor(totMins / 60);
				remainingMinutes = totMins % 60;
				totDays = floor(totHours / 24);
				remainingHours = totHours % 24;
				if(totDays <= 0)
				{
					eventAddedTimeDiff = totHours + " hour " + remainingMinutes + " mins ago";
				}
				else
				{
					eventAddedTimeDiff = totDays + " days " + remainingHours + " hours ago";
				}
				return eventAddedTimeDiff;
			}
