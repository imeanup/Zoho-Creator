				}
			)
			quickview
			(
				layout
				(
		 			type = 1
					datablock1
					(
			 			layout type = 1
		     			title = "Overview"
						fields
						(
							Event_ID as "Event ID"
							Event_Name as "Event Name"
							Event_Category as "Event Category"
							Remarks as "Description"
