		form Book_Free_Events1
		{
			displayname = "Registration Form"
			success message = "Data Added Successfully!"
			field alignment = left
			feature property = 32
					hide = true
			Section
			(
				type = section
	 			row = 1
	 			column = 0   
				width = medium
			)
			must have Event
			(
				type = picklist
				values  = Create_New_Event.ID
    			displayformat = [Event_Name]
				searchable = true
	 			row = 1
	 			column = 1   
				width = medium
			)
			Venue
			(
				type = picklist
				values  = Add_Venue.ID
    			displayformat = [Location.address_line_12]
				sortorder = ascending
	 			row = 1
	 			column = 1   
				width = medium
			)
			must have Available_Entries
			(
				type = number
				displayname = "Available Entries"
				initial value = 0
	 			row = 1
	 			column = 1   
				width = medium
			)
			must have Number_of_people
			(
				type = number
				displayname = "Required Entries"
				initial value = 0
	 			row = 1
	 			column = 1   
				width = medium
			)
			must have I_agree_to_the_Terms_Conditions
			(
    			type = checkbox
				displayname = "I agree to the Terms & Conditions."
				initial value = false
	 			row = 1
	 			column = 1   
				width = medium
			)
			Add_Customer_First_Name
			(
				type = picklist
				displayname = "Add Customer Rec (First Name)"
				values  = Add_Customer.ID
    			bidirectional = Book_Free_Events_Rec_ID
    			displayformat = [Name.first_name]
	 			row = 1
	 			column = 1   
				width = medium
			)
			must have Name
			(
    			type = name
     			prefix
     			(
	     			 type = prefix
	     			 displayname ="Prefix"
	     			 visibility = false
	     			 value = {"Mr.","Mrs.","Ms."}
     			) 
     			first_name
     			(
	     			 type = first_name
	     			 displayname ="First Name"
     			) 
     			last_name
     			(
	     			 type = last_name
	     			 displayname ="Last Name"
     			) 
     			suffix
     			(
	     			 type = suffix
	     			 displayname ="Suffix"
	     			 visibility = false
     			) 
	 			row = 1
	 			column = 2   
				width = medium
			)
			must have Contact_Email
			(
    			type = email
				displayname = "Contact Email"
	 			row = 1
	 			column = 2   
				width = medium
			)
			must have Gender
			(
				type = radiobuttons
				values = {"Male", "Female"}
	 			row = 1
	 			column = 2   
				width = medium
			)
			must have Contact_Number
			(
    			type = phonenumber
				displayname = "Contact Number"
	 			row = 1
	 			column = 2   
				width = medium
			)
	
			actions
			{
				on add
				{
					submit
					(
   						type = submit
   						displayname = "Register"
					)
					reset
					(
   						type = reset
   						displayname = "Reset"
					)
				}
				on edit
				{
					update
					(
   						type = submit
   						displayname = "Update"
					)
					cancel
					(
   						type = cancel
   						displayname = "Cancel"
					)
				}
			}
		}
