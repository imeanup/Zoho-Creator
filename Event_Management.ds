 	 	}
		form Create_New_Event
		{
			displayname = "Create New Event"
			success message = "Event Created Successfully!"
			field alignment = left
			feature property = 32
			Section
			(
				type = section
	 			row = 1
	 			column = 0   
				width = medium
			)
			must have Event_Category
			(
				type = picklist
				displayname = "Event Category"
				values  = Add_Event_Category.ID
    			displayformat = [Event_Category]
				allow new entries = true
				[
					displayname = "Add Category"
				]
				searchable = true
	 			row = 1
	 			column = 1   
				width = medium
			)
			must have Event_Name
			(
    			type = text
				displayname = "Event Name"
	 			row = 1
	 			column = 1   
				width = medium
			)
			must have Venue
			(
				type = picklist
				values  = Add_Venue.ID
    			displayformat = [Location.address_line_12]
				allow new entries = true
				[
					displayname = "Add Venue"
				]
				sortorder = ascending
	 			row = 1
	 			column = 1   
				width = medium
			)
			must have Event_Start_Time
			(
    			type = datetime
				displayname = "Event Start Time"
				timedisplayoptions = "hh:mm:ss"
				alloweddays = 0,1,2,3,4,5,6
	 			row = 1
	 			column = 1   
				width = medium
			)
			must have Event_End_Time
			(
    			type = datetime
				displayname = "Event End Time"
				timedisplayoptions = "hh:mm:ss"
				alloweddays = 0,1,2,3,4,5,6
	 			row = 1
	 			column = 1   
				width = medium
			)
			Event_ID
			(
    			type = autonumber
				displayname = "Event ID"
				start index = 1
	 			row = 1
	 			column = 1   
				width = medium
			)
			must have Subscription_Type
			(
				type = picklist
				displayname = "Ticket Type"
				values = {"Free", "Paid"}
	 			row = 1
	 			column = 1   
				width = medium
			)
			must have Event_Image
			(
    			type = image
				displayname = "Event Image"
				source  = public_url,file
				aspect ratio = original
				camera = primary
				show gallery = true
				switch camera = true
	 			row = 1
	 			column = 1   
				width = medium
			)
			must have Entry_Fees
			(
				displayname = "Entry Fees"
				type = USD
				initial value = 0
	 			row = 1
	 			column = 1   
				width = medium
			)
			must have Number_of_entries
			(
				type = number
				displayname = "Number of entries"
				initial value = 0
	 			row = 1
	 			column = 1   
				width = medium
			)
			Available_Entries
			(
				type = number
				displayname = "Available Entries"
				initial value = 0
	 			row = 1
	 			column = 1   
				width = medium
			)
			must have Remarks
			(
    			type = textarea
				displayname = "Event Description"
				height = 100px
	 			row = 1
	 			column = 1   
				width = medium
			)
			Event_Status
			(
				type = radiobuttons
				displayname = "Event Status"
				values = {"Active", "Ended"}
	 			row = 1
	 			column = 1   
				width = medium
			)
	
			customize
			(
				icon = "ui-3-calendar-add"
			)
			actions
			{
				on add
				{
					submit
					(
   						type = submit
   						displayname = "Submit"
					)
					reset
					(
   						type = reset
   						displayname = "Reset"
					)
				}
				on edit
				{
					update
					(
   						type = submit
   						displayname = "Update"
					)
					cancel
					(
   						type = cancel
   						displayname = "Cancel"
					)
				}
			}
		}
		calendar Events_Calendar
		{
			displayName = "Events Calendar"
			show all rows from Create_New_Event    
			(
				Event_ID as "Event ID"
				Event_Name as "Event Name"
				Event_Category as "Event Category"
				Remarks as "Description"
				Venue
