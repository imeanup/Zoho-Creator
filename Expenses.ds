			Run_Scripts_On_click_of_A as "Run Scripts - On click of 'Approve' button"
			{
				type =  form
				form = Approve
				button = Approve
				on click
				{
					actions 
					{
					on click
					(
						expenseAmount = Add_Expense[ID == input.RecId].Amount;
										if(input.Approve1 == "Partial Amount" && input.Specify_Allowable_Amount == null)
										{
											alert "Please enter value for 'Allowable Amount'!";
										}
										else if(input.Specify_Allowable_Amount >= expenseAmount)
										{
											alert "Please provide a valid partial amount!";
										}
										else
										{
											expenseRecord = Add_Expense[ID == input.RecId];
											//Updating approved status and allowable amount to expense records
											if(input.Approve1 == "Partial Amount")
											{
												expenseRecord.Partially_Approved=true;
												expenseRecord.Approved_Amount=input.Specify_Allowable_Amount;
											}
											else
											{
												expenseRecord.Approved_Amount=expenseRecord.Amount;
											}
											expenseRecord.Status="Approved";
											//Maintaining status change timings in backend for showing recent activity
											reportRecord = Add_Report[ID == expenseRecord.Add_to_Report];
											reportRecord.StatusChangeTime=zoho.currenttime;
											//If all expenses in a report has been approved, the status of the corresponding report has to be changed to 'Approval Completed'
											statusList = Add_Expense[Add_to_Report == expenseRecord.Add_to_Report].Status.getAll();
											if(!statusList.contains("Submitted"))
											{
												reportRec = Add_Report[ID == expenseRecord.Add_to_Report];
												advanceAmount = Add_Advance[Link_Advance_To == reportRec.ID && Status == "Granted"].sum(Amount);
												reportRec.Reimbursable_Amount=Add_Expense[Add_to_Report == expenseRecord.Add_to_Report && Status == "Approved" && Reimbursable == "Yes"].sum(Approved_Amount) - advanceAmount;
												submitter = Add_User[ID == reportRecord.Submitted_By];
												approver = Add_User[Official_Email == reportRecord.Approver];
												orgDetail = Add_Your_Organization[ID != 0];
												if(reportRec.Reimbursable_Amount > 0)
												{
													reportRec.Status="Awaiting Reimbursement";
													sendmail
													[
														from :zoho.adminuserid
														to :submitter.Official_Email
														subject :"Your Expense report approval completion notification"
														message :(("Hello " + submitter.Name) + ",<div><br></div><div>The approval process of your expense report named '" + "<b>" + reportRecord.Report_Number + "-" + reportRecord.Report_Title + "</b>'" + ", which contains a set of " + statusList.size() + " expenses,&nbsp; is&nbsp;been officially completed and is awaiting for reimbursement in the hands of " + approver.Name) + "(" + reportRecord.Approver + ").</div><div><br></div><div>Best Regards,</div><div>" + zoho.adminuser + "</div><div>" + orgDetail.Organisation_Name + "<br></div>"
													]
												}
												else
												{
													reportRec.Status="Approval Completed";
													sendmail
													[
														from :zoho.adminuserid
														to :submitter.Official_Email
														subject :"Your Expense report approval completion - Notification"
														message :(("<div>Hello " + submitter.Name) + ",<div><br></div><div>The approval process of your expense report named '" + "<b>" + reportRecord.Report_Number + "-" + reportRecord.Report_Title + "</b>'" + ", which contains a set of " + statusList.size() + " expenses,&nbsp; is&nbsp;been officially completed by " + approver.Name) + "(" + reportRecord.Approver + ").</div><div><br></div><div>Best Regards,</div><div>" + zoho.adminuser + "</div><div>" + orgDetail.Organisation_Name + "</div></div>"
													]
												}
											}
										}
										openUrl("#Page:Pending?currentRow=" + input.currentRow,"same window");
					)
					}
				}
			}
