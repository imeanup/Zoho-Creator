	section Expense
	{
		displayname= "Expenses"
		icon = "business-money-bag"
		form Add_Expense
		{
			displayname = "Add Expense"
			success message = "New Expense Added Successfully!"
			field alignment = left
			feature property = 32
			Section
			(
				type = section
	 			row = 1
	 			column = 0   
				width = medium
			)
			must have Add_to_Report
			(
				type = picklist
				displayname = "Add to Report"
				values  = Add_Report[Status == "Unsubmitted"].ID
    			bidirectional = Expenses
    			displayformat = [Report_Title]
				sortorder = ascending
	 			row = 1
	 			column = 1   
				width = medium
			)
			must have Category
			(
				type = picklist
				values  = Add_Expense_Category.ID
    			displayformat = [Category_Name]
				sortorder = ascending
	 			row = 1
	 			column = 1   
				width = medium
			)
			must have Vendor
			(
				type = picklist
				displayname = " Vendor"
				values  = Add_Vendor.ID
    			displayformat = [Vendor_Name]
				allow new entries = true
				[
					displayname = "Add New"
				]
				sortorder = ascending
	 			row = 1
	 			column = 1   
				width = medium
			)
			must have Date_field
			(
    			type = date
				displayname = "Date"
				alloweddays = 0,1,2,3,4,5,6
	 			row = 1
	 			column = 1   
				width = medium
			)
			must have Amount
			(
				type = USD
	 			row = 1
	 			column = 1   
				width = medium
			)
			Expense_Purpose
			(
    			type = textarea
				displayname = "Expense Purpose"
				height = 100px
	 			row = 1
	 			column = 1   
				width = medium
			)
			must have Receipt
			(
    			type = image
				source  = file
				aspect ratio = original
				camera = primary
				show gallery = true
				switch camera = true
	 			row = 1
	 			column = 1   
				width = medium
			)
			must have Reimbursable
			(
				type = radiobuttons
				values = {"Yes", "No"}
				initial value = "Yes"
				layout = 2
	 			row = 1
	 			column = 1   
				width = 300px
			)
			Paid_through
			(
				type = picklist
				displayname = "Paid through (if non-reimbursable)"
				values = {"Petty Cash", "Bank Account", "Office Card"}
	 			row = 1
	 			column = 1   
				width = medium
			)
			Status
			(
				type = picklist
				values = {"Reported", "Submitted", "Approved", "Rejected"}
				initial value = "Reported"
	 			row = 1
	 			column = 1   
				width = medium
			)
			Policy_Violated
			(
    			type = checkbox
				displayname = "Policy Violated"
				initial value = false
	 			row = 1
	 			column = 1   
				width = 300px
			)
			Partially_Approved
			(
    			type = checkbox
				displayname = "Partially Approved"
				initial value = false
	 			row = 1
	 			column = 1   
				width = 300px
			)
			Approved_Amount
			(
				displayname = "Approved Amount"
				type = USD
	 			row = 1
	 			column = 1   
				width = medium
			)
	
			customize
			(
				icon = "ui-1-bold-add"
			)
			actions
			{
				on add
				{
					submit
					(
   						type = submit
   						displayname = "Submit"
					)
					reset
					(
   						type = reset
   						displayname = "Reset"
					)
				}
				on edit
				{
					update
