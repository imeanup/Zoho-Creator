 			void CustomAction.submitReport(Add_Report recObj)
			{
				//Changing both report and expense satus to 'Submitted'
				recObj.Status="Submitted";
				recObj.StatusChangeTime=zoho.currenttime;
				recObj.Submitted_Date=zoho.currentdate;
				allExpenses = Add_Expense[Add_to_Report == recObj.ID];
				for each  expense in allExpenses
				{
					expense.Status="Submitted";
				}
				submitter = Add_User[ID == recObj.Submitted_By];
				approver = Add_User[Official_Email == recObj.Approver];
				orgDetail = Add_Your_Organization[ID != 0];
				sendmail
				[
					from :zoho.adminuserid
					to :submitter.Official_Email
					subject :"Expense Report Submission Notification"
					message :(("Hello " + submitter.Name) + ",<div><br></div><div>The expense report named '" + "<b>" + recObj.Report_Number + "-" + recObj.Report_Title + "</b>'" + ", which contains a set of&nbsp;&nbsp;" + allExpenses.count() + " expenses has been successfully submitted to " + approver.Name) + "(" + recObj.Approver + ").</div><div><br></div><div>Best Regards,</div><div>" + zoho.adminuser + "</div><div>" + orgDetail.Organisation_Name + "<br></div>"
				]
			}
 			void CustomAction.denyAdvance(Add_Advance recObj)
			{
				recObj.Status="Denied";
			}
 			void CustomAction.grantAdvance(int Id)
			{
				openUrl("#Form:Add_Advance?recLinkID=" + Id + "&OpenedFromAdvanceGrant=true&Status=Granted&viewLinkName=Team_Advance_Requests","popup window");
			}
 			string PageData.addCommasInCurrency(float currencyValue)
			{
				if(input.currencyValue != null)
				{
					currencyValue = currencyValue.round(2);
					currencyStr = currencyValue.toString().replaceAll("(?<!\.\d)(?<=\d)(?=(?:\d\d\d)+\b)",",");
				}
				return currencyStr;
			}
	}



