				form = Add_User
				record event = on add

	on success
	{
					actions 
					{
						custom deluge script
						(
											//Sharing application and sending invitation mail to users based on their role
										orgDetail = Add_Your_Organization[ID != 0];
										thisapp.permissions.assignUserInProfile(input.Official_Email,input.Role);
										if(input.Role == "Submitter")
										{
											sendmail
											[
												from :zoho.adminuserid
												to :input.Official_Email
												subject :"Invitation to access Expenses software"
												message :("Hello&nbsp;" + input.Name) + ",<div><br></div><div>I as a part of " + orgDetail.Organisation_Name + ", cordially invites you to access Expenses application. Hereafter you need to submit all your expense reports in this portal.<br></div><div><br></div><div>Click <a href=\"" + "https://app.zohocreator.com" + zoho.appuri + "\"" + " target=\"_blank\">here</a> to access the application.</div><div><br></div><div>Best Regards,</div><div>" + zoho.adminuser + ",</div><div>" + orgDetail.Organisation_Name + "</div>"
											]
										}
										else if(input.Role == "Approver")
										{
											sendmail
											[
												from :zoho.adminuserid
												to :input.Official_Email
												subject :"Invitation to access Expenses application"
												message :("Hello&nbsp;" + input.Name) + ",<div><br></div><div>I as a part of " + orgDetail.Organisation_Name + ", cordially invites you to access Expenses application. Hereafter you need to submit all your expense reports in this portal. You can also review your subordinates expense reports from here itself.<br></div><div><br></div><div>Click&nbsp;<a href=\"" + "https://app.zohocreator.com" + zoho.appuri + "\"" + " target=\"_blank\">here</a>&nbsp;to access the application.</div><div><br></div><div>Best Regards,</div><div>" + zoho.adminuser + ",</div><div>" + orgDetail.Organisation_Name + "</div>"
											]
										}
										if(Add_User[ID != 0].count() == 1)
										{
											openUrl("#Form:Add_Report","same window");
										}
						)
					}
	}

			}
