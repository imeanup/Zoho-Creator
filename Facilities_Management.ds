	functions
	{
 			void custom_action.add_asset_to_gateway(int gateway)
			{
				openUrl(zoho.appuri + "#Form:Add_Asset_To_Gateway?Gateway=" + gateway,"popup window");
			}
 			void custom_action.open_add_gateway_form(int building)
			{
				openUrl(zoho.appuri + "#Form:Add_Gateway?Building=" + building,"popup window");
			}
 			void custom_action.open_add_building_form(int site)
			{
				openUrl(zoho.appuri + "#Form:Add_Building?Associated_Site=" + site,"popup window");
			}
 			void custom_action.update_asset_quantity_in_inventory(int asset_in_inventory)
			{
				openUrl(zoho.appuri + "Assets_In_inventory/record-edit/Assets_In_Stock/" + asset_in_inventory,"popup window");
			}
 			void notification_nction.send_mail(string toMailId, string sub, string msg)
			{
				sendmail
				[
					from :zoho.adminuserid
					to :toMailId
					subject :sub
					message :msg
				]
			}
 			void custom_action.assign_employee(int issueID)
			{
				openUrl(zoho.appuri + "#Form:Assign_Employee?Issue=" + issueID,"popup window");
			}
 			void custom_action.accept_Issue(int issue_id)
			{
				issue = Add_An_Incident[ID == issue_id];
				issue.Troubleshoot_Status="In Progress";
			}
 			void custom_action.close_issue(int issue_id)
			{
				issue = Add_An_Incident[ID == issue_id];
				issue.Troubleshoot_Status="Closed";
			}
 			void custom_action.order_to_vendor(int request_ID)
			{
				openUrl(zoho.appuri + "#Form:Order_To_Vendor?For_Request=" + request_ID,"popup window");
			}
 			void custom_action.move_to_gateway(int order_ID)
			{
				request = Raise_Asset_Request[ID = order_ID];
				request = "Moved To Gateway";
			}
 			void custom_action.receive_asset(int order_ID)
			{
				order = Order_To_Vendor[ID == order_ID];
				order.Order_Status="Received To Inventory";
				asset_in_inverntory = Assets_In_inventory[Asset == order.Asset];
				asset_in_inverntory.Quantity=order.Quantity;
			}
	}



	workflow
	{
		form
		{
			Run_Scripts_On_click_a_bu as "Run Scripts - On click a button"
			{
				type =  form
				form = Assign_Employee
				button = Submit
				on click
				{
					actions 
					{
					on click
					(
						issue = Add_An_Incident[ID == input.Issue];
										issue.Assigned_To=input.Employee;
										issue.Troubleshoot_Status="Assigned";
										issue.Status_Updated_On=zoho.currenttime;
										msg = "There is an issue has been assigned to you. <br><br><br> Issue Details below here, <br><br> <b>Issue Name: </b>" + issue.Issue_Name + "(<span style='color:#58CFFB'>Priority: " + issue.Priority + "</span>)<br><b>Site: </b>" + issue.Site.Branch_Site_Name + "<br><b>Building: </b>" + issue.Site.Branch_Site_Name + "<br><b>Gateway: </b>" + issue.Site.Branch_Site_Name;
										if(issue.Failure_Object != null)
										{
											msg = msg + "<br><b>Failure Object: </b>" + issue.Failure_Object.Asset_Name + "<br><b>Description: </b>" + issue.Description + "<br><b>Deadline: </b>" + issue.Deadline + "<br><br>To track your work oreder click <a href='https://app.zohocreator.com/" + zoho.appuri + "#Report:My_Work_Orders'>here</a>";
										}
										msg = msg + "<br><b>Failure Object: </b>" + issue.Failure_Object.Asset_Name + "<br><b>Deadline: </b>" + issue.Deadline;
										sub = zoho.appname + "Issue Assinged";
										toMailID = input.Employee.Contact_Email;
										thisapp.notification_nction.send_mail(toMailID,sub,msg);
					)
					}
				}
			}
