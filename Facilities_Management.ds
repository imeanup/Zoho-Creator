			Run_scripts_While_adding_4 as "Run scripts - While adding a record"
			{
				type =  form
				form = Add_asset_to_Inventory
				record event = on add

	on success
	{
					actions 
					{
						custom deluge script
						(
											asset_in_stock = Assets_In_inventory[Asset == input.Asset];
										if(asset_in_stock.count() == 0)
										{
											insert into Assets_In_inventory
											[
												Quantity=input.Quantity
												Asset=input.Asset
												Last_Update_Date=input.Asset_Refill_Date
												Added_User=zoho.loginuser
											]
										}
										else
										{
											asset_in_stock.Quantity=asset_in_stock.Quantity + input.Quantity;
											asset_in_stock.Last_Update_Date=(asset_in_stock.Last_Update_Date + input.Asset_Refill_Date);
										}
										if(input.Expense_Amount != null)
										{
											user = Add_Employee[Contact_Email == zoho.loginuserid];
											insert into Add_Expense
											[
												Expense_Type="Asset Purchase"
												Added_User=zoho.loginuser
												Expense_Amount=input.Expense_Amount
												Expense_Date=input.Asset_Refill_Date
												Notes=input.Notes
												Receipt=input.Receipt
												Submitted_By=user
											]
										}
						)
					}
	}

			}
