	functions
	{
 			list leaveTypes.calculateLeaveTypes()
			{
				employee = Add_Employee[Official_Email == zoho.loginuserid];
				leaveTypeList = List:String();
				for each  Rec in Add_Leave_Type[Name != "Compensatory Off"]
				{
					if(Rec.Applicable = "All Employees")
					{
						leaveTypeList.add(Rec.ID);
					}
					else if(Rec.Applicable == "Specific Employees")
					{
						if(Rec.Department.size() == 0)
						{
							departmentList = Add_Department[ID != 0].ID.getAll();
						}
						else
						{
							departmentList = Rec.Department;
						}
						if(Rec.Designation.size() == 0)
						{
							designationList = Add_Designation[ID != 0].ID.getAll();
						}
						else
						{
							designationList = Rec.Designation;
						}
						if(Rec.Location.size() == 0)
						{
							locationList = Add_Location[ID != 0].ID.getAll();
						}
						else
						{
							locationList = Rec.Location;
						}
						if(departmentList.contains(employee.Department) && designationList.contains(employee.Designation) && locationList.contains(employee.Location))
						{
							leaveTypeList.add(Rec.ID);
						}
					}
				}
				return leaveTypeList;
			}
 			void CustomAction.markInactive(Add_Leave_Type recObj)
			{
				recObj.Status="Inactive";
			}
 			void CustomAction.rejectLeave(Apply_Leave recObj)
			{
				recObj.Status="Rejected";
				employeeRec = Add_Employee[ID != 0];
				sendmail
				[
					from :zoho.adminuserid
					to :employeeRec.Official_Email
					subject :"Leave Rejection Notification"
					message :("Hi&nbsp;" + employeeRec.Name) + ",<div><br></div><div>Sorry! Your leave request has been rejected by your manager.</div><div><br></div><div>Leave Details is as follows,</div><div><br></div><div><b>Half/Half Day :&nbsp;</b>" + recObj.Type + "</div><div><b><br></b></div><div><b>Leave Type &nbsp; &nbsp;:&nbsp;</b>" + recObj.Leave_Type.Name + "</div><div><b><br></b></div><div><b>From &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp;:&nbsp;</b>" + recObj.From + "</div><div><b><br></b></div><div><b>To &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp;: &nbsp;</b>" + recObj.To + "</div><div>&nbsp;&nbsp;</div><div><br></div><div>Regards,</div><div>" + zoho.adminuser + "</div><div><br></div>"
				]
			}
 			void CustomAction.approveLeave(Apply_Leave recObj)
			{
				recObj.Status="Approved";
				if(Add_Leave_Type[ID == recObj.Leave_Type].Name != "Compensatory Off")
				{
					availableLeaves = Available_Leaves[Employee == recObj.Employee_ID && Leave_Type == recObj.Leave_Type];
					availableLeaves.Available=availableLeaves.Available - recObj.Total_Day_s.toDecimal();
					availableLeaves.Taken=availableLeaves.Available + recObj.Total_Day_s.toDecimal();
				}
				employeeRec = Add_Employee[ID != 0];
				sendmail
				[
					from :zoho.adminuserid
					to :employeeRec.Official_Email
					subject :"Leave Approval Notification"
					message :("Hi&nbsp;" + employeeRec.Name) + ",<div><br></div><div>Your leave request has been approved by your manager.</div><div><br></div><div>Leave Details is as follows,</div><div><br></div><div><b>Half/Half Day :&nbsp;</b>" + recObj.Type + "</div><div><b><br></b></div><div><b>Leave Type &nbsp; &nbsp;:&nbsp;</b>" + recObj.Leave_Type.Name + "</div><div><b><br></b></div><div><b>From &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp;:&nbsp;</b>" + recObj.From + "</div><div><b><br></b></div><div><b>To &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp;: &nbsp;</b>" + recObj.To + "</div><div>&nbsp;&nbsp;</div><div><br></div><div>Regards,</div><div>" + zoho.adminuser + "</div><div><br></div>"
				]
			}
 			list leaveTypes.calculateLeaveSubLists(int startfrom)
			{
				leaveTypesList = thisapp.leaveTypes.calculateLeaveTypes();
				leaveTypeCount = leaveTypesList.size();
				start = startfrom;
				end = startfrom + 5;
				leaveTypesSubList = leaveTypesList.subList(start - 1,if(end < leaveTypeCount,end,leaveTypeCount));
				return leaveTypesSubList;
			}
	}



