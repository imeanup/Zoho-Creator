			Run_Scripts_While_Adding_20 as "Run Scripts - While Adding a Record"
			{
				type =  form
				form = Apply_Leave
				record event = on add

	on load
	{
					actions 
					{
						custom deluge script
						(
											hide Worked_Date;
										hide availableDays;
										employee = Add_Employee[Official_Email == zoho.loginuserid];
										input.Employee_ID = employee.ID;
										disable Employee_ID;
										leaveTypesList = List:String();
										//Adding current user's available leaves to the leave type field
										for each  leaveType in Add_Leave_Type[Status = "Active"]
										{
											if(leaveType.Applicable = "All Employees")
											{
												leaveTypesList.add(leaveType.ID);
											}
											else if(leaveType.Applicable == "Specific Employees")
											{
												if(leaveType.Department.size() == 0)
												{
													departmentList = Add_Department[ID != 0].ID.getAll();
												}
												else
												{
													departmentList = leaveType.Department;
												}
												if(leaveType.Designation.size() == 0)
												{
													designationList = Add_Designation[ID != 0].ID.getAll();
												}
												else
												{
													designationList = leaveType.Designation;
												}
												if(leaveType.Location.size() == 0)
												{
													locationList = Add_Location[ID != 0].ID.getAll();
												}
												else
												{
													locationList = leaveType.Location;
												}
												if(departmentList.contains(employee.Department) && designationList.contains(employee.Designation) && locationList.contains(employee.Location))
												{
													leaveTypesList.add(leaveType.ID);
												}
											}
										}
										input.Leave_Type:ui.add(leaveTypesList);
						)
					}
	}

			}
			Run_Scripts_While_Adding_21 as "Run Scripts - While Adding a Record"
			{
				type =  form
				form = Apply_Leave
				record event = on add

	on validate
	{
					actions 
					{
						custom deluge script
						(
											//Updating Total day value in case of Half day
										if(input.Type == "Half Day")
										{
											input.Total_Day_s = 0.5.toString();
										}
										//Updating Total day value in case of Compensatory Off
										else if(input.Type == "Full Day" && Add_Leave_Type[ID == input.Leave_Type].Name == "Compensatory Off")
										{
											input.Total_Day_s = 1.toString();
										}
										if(Add_Leave_Type[ID == input.Leave_Type].Name == "Compensatory Off")
										{
											//Validating 'Compensate with' field in case of Compensatory Off
											if(input.Worked_Date = null)
											{
												alert "Please enter date for 'Compensate with' field !";
												cancel submit;
											}
											if(input.Worked_Date > zoho.currentdate)
											{
												alert "Please enter valid past date for 'Compensate with' field !";
												cancel submit;
											}
										}
										//Check to ensure that leaves are applied only on future dates
										if(input.From < zoho.currentdate || input.To < zoho.currentdate)
										{
											alert "Plaese enter valid future date !";
											cancel submit;
										}
										//Check to ensure that from date is less than to date
										if(input.From > input.To)
										{
											alert "Please enter valid 'From' and 'To' date !";
											cancel submit;
										}
										//Check to ensure that requested no of days is less than or equal to available no of days
										if(Add_Leave_Type[ID == input.Leave_Type].Name != "Compensatory Off")
										{
											availableLeave = Available_Leaves[Employee == input.Employee_ID && Leave_Type == input.Leave_Type].Available;
											if(availableLeave < Total_Day_s.toDecimal())
											{
												alert "Sorry! Can't process your leave request since availablility of " + Add_Leave_Type[ID == input.Leave_Type].Name + " is only " + availableLeave + " day(s)";
												cancel submit;
											}
										}
										//Check to prevent applied date overlapping with existing leave date
										if(Apply_Leave[(From <= input.From && To >= input.From) || (From >= input.From && From <= input.To)].count() > 0)
										{
											alert "Sorry! You have already applied leave for this date.";
											cancel submit;
										}
						)
					}
	}

			}
			Run_Scripts_While_Adding_22 as "Run Scripts - While Adding a Record"
			{
				type =  form
				form = Apply_Leave
				record event = on add

	on success
	{
					actions 
					{
						custom deluge script
						(
											employeeRec = Add_Employee[ID == input.Employee_ID];
										if(employeeRec.Reporting_To != "" && employeeRec.Reporting_To != null)
										{
											//Notification mail to Reporting Manager
											sendmail
											[
												from :zoho.adminuserid
												to :employeeRec.Reporting_To
												subject :"New Leave Request by " + employeeRec.Name
												message :("Hi,<div><br></div><div>A new leave request has been raised by&nbsp;" + employeeRec.Name) + " and it is waiting for your approval.</div><div><br></div><div>Please click <a href=\"" + "https://app.zohocreator.com" + zoho.appuri + "#Report:Leave_Requests?Employee_ID=" + " target=\"_blank\">here</a> to view the leave details.</div><div><br></div><div>Regards</div><div>" + zoho.adminuser + "</div>"
											]
										}
										else
										{
											//If there is no reporting manager for the applied employee, the leave will be auto approved
											input.Status = "Approved";
											if(Add_Leave_Type[ID == input.Leave_Type].Name != "Compensatory Off")
											{
												availableLeaveRec = Available_Leaves[Employee == input.Employee_ID && Leave_Type == input.Leave_Type];
												availableLeaveRec.Taken=availableLeaveRec.Taken + input.Total_Day_s.toDecimal();
												availableLeaveRec.Available=availableLeaveRec.Available - input.Total_Day_s.toDecimal();
											}
										}
										openUrl("#Script:page.refresh","same window");
						)
					}
	}

			}
