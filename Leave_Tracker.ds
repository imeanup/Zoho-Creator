		schedule
		{
			Yearly_Leave_Updation as "Yearly Leave Updation"
			{
				type =  schedule
				start = "2017-01-01 12:00:00"
				frequency = yearly
				time zone = "PST8PDT"
				on start
				{
					actions 
					{
					on load
					(
						for each  employee in Add_Employee[ID != 0]
										{
											for each  leaveType in Add_Leave_Type[Cycle_Period == "Yearly(Jan to Dec)" && Name != "Compensatory Off"]
											{
												carryforwardCount = 0.0;
												availableLeaveRec = Available_Leaves[Employee == employee.ID && Leave_Type == leaveType.ID];
												if(leaveType.Enable_Carry_Forward)
												{
													carryforwardCount = if(availableLeaveRec.Available > leaveType.Maximum_Roll_Over_Accumulation_Count,leaveType.Maximum_Roll_Over_Accumulation_Count.toDecimal(),availableLeaveRec.Available);
												}
												availableLeaveRec.Available=leaveType.Leave_Count + carryforwardCount;
												availableLeaveRec.Permitted=leaveType.Leave_Count + carryforwardCount;
												availableLeaveRec.Taken=0.0;
											}
										}
					)
					}
				}
			}
			Monthly_Leave_Updation as "Monthly Leave Updation"
			{
				type =  schedule
				start = "2017-01-01 12:00:00"
				frequency = monthly
				time zone = "PST8PDT"
				on start
				{
					actions 
					{
					on load
					(
						for each  employee in Add_Employee
										{
											for each  leaveType in Add_Leave_Type[Cycle_Period == "Monthly" && Name != "Compensatory Off"]
											{
												availableLeaveRec = Available_Leaves[Employee == employee.ID && Leave_Type == leaveType.ID];
												carryforwardCount = 0.0;
												availableLeaveRec = Available_Leaves[Employee == employee.ID && Leave_Type == leaveType.ID];
												if(leaveType.Enable_Carry_Forward)
												{
													carryforwardCount = if(availableLeaveRec.Available > leaveType.Maximum_Roll_Over_Accumulation_Count,leaveType.Maximum_Roll_Over_Accumulation_Count.toDecimal(),availableLeaveRec.Available);
												}
												availableLeaveRec.Available=leaveType.Leave_Count + carryforwardCount;
												availableLeaveRec.Permitted=leaveType.Leave_Count + carryforwardCount;
												if(zoho.currentdate.getMonth() == 1)
												{
													availableLeaveRec.Taken=0.0;
												}
											}
										}
					)
					}
				}
			}
		}
