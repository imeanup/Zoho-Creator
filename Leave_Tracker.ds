				form = Add_Leave_Type
				record event = on add

	on success
	{
					actions 
					{
						custom deluge script
						(
											//Adding the leave type to respective employees 
										if(input.Name != "Compensatory Off")
										{
											if(input.Applicable = "All Employees")
											{
												for each  employee in Add_Employee[ID != 0]
												{
													insert into Available_Leaves
													[
														Employee=employee.ID
														Leave_Type=input.ID
														Available=input.Leave_Count
														Permitted=input.Leave_Count
														Taken=0
														Added_User=zoho.loginuser
													]
												}
											}
											else
											{
												if(Department.size() == 0)
												{
													departmentList = Add_Department[ID != 0].ID.getAll();
												}
												else
												{
													departmentList = input.Department;
												}
												if(Designation.size() == 0)
												{
													designationList = Add_Designation[ID != 0].ID.getAll();
												}
												else
												{
													designationList = input.Designation;
												}
												if(Location.size() == 0)
												{
													locationList = Add_Location[ID != 0].ID.getAll();
												}
												else
												{
													locationList = input.Location;
												}
												for each  employee in Add_Employee[Department in departmentList && Designation in designationList && Location in locationList]
												{
													insert into Available_Leaves
													[
														Employee=employee.ID
														Leave_Type=input.ID
														Available=input.Leave_Count
														Permitted=input.Leave_Count
