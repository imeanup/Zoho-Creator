			Field_scripts4 as "Field scripts"
			{
				type =  form
				form = Add_Resource
				record event = on add or edit

	on user input of ISBN_Barcode
	{
					actions 
					{
						custom deluge script
						(
											if(input.ISBN_Barcode != "")
										{
											exist_resource = Resource[ISBN_Barcode == input.ISBN_Barcode];
											//If ISBN already exists in the resource
											if(exist_resource.count() > 0)
											{
												input.Type = exist_resource.Resource_Type;
												disable Type;
												input.Title = exist_resource.Title;
												disable Title;
												input.Edition = exist_resource.Edition;
												disable Edition;
												input.Author = exist_resource.Author;
												disable Author;
												input.Category = exist_resource.Category;
												disable Category;
												input.Publisher = exist_resource.Publisher;
												disable Publisher;
												input.Image = exist_resource.Image;
												disable Image;
												input.Book_Show = thisapp.NoteField.makeBookView(input.ISBN_Barcode);
												show Book_Show;
											}
											else
											{
												//Gather the book datails by using API.
												book_details = thisapp.usingAPI.getBookDetails(input.ISBN_Barcode);
												input.Book_Show = "<style>.content{margin-top:12px}</style><div style ='width:350px;text-align:left;margin=10px;'><img style ='width:230px;height:320px;' src=" + book_details.get("image") + " alt = 'No Preview Available'/><div style = 'margin-left:2px'><div class = 'content'><b>Author : </b><span>" + book_details.get("author") + "</span></div><div class = 'content'><b>Category : </b><span>" + book_details.get("category") + "</span></div><div class = 'content'><b>Publisher : </b><span>" + book_details.get("publisher") + "</span></div></div>";
												show Book_Show;
												//Auto fill the  Data fetched by API
												if(book_details.size() != 0)
												{
													exist_authorList = List:String();
													if(book_details.get("author") != "---")
													{
														authorList = book_details.get("author").toList();
														for each  rec in authorList
														{
															exist_author = Add_Author[Author_Name == rec];
															if(exist_author.count() == 0)
															{
																insert into Add_Author
																[
																	Author_Name=rec
																	Added_User=zoho.loginuser
																]
																new_author = Add_Author[Author_Name == rec];
																exist_authorList.add(new_author.ID);
															}
															else
															{
																exist_authorList.add(exist_author.ID);
															}
														}
													}
													toAddAuthorList = exist_authorList.distinct();
													exist_categoryList = List:String();
													if(book_details.get("category") != "---")
													{
														categoryList = book_details.get("category").toList();
														for each  rec in categoryList
														{
															exist_category = Add_Category[Category == rec];
															if(exist_category.count() == 0)
															{
																insert into Add_Category
																[
																	Added_User=zoho.loginuser
																	Category=rec
																]
																new_category = Add_Category[Category == rec];
																exist_categoryList.add(new_category.ID);
															}
															else
															{
																exist_categoryList.add(exist_category.ID);
															}
														}
													}
													toAddCategoryList = exist_categoryList.distinct();
													exist_PublisherList = List:String();
													if(book_details.get("publisher") != "---")
													{
														publisherList = book_details.get("publisher").toList();
														for each  rec in publisherList
														{
															exist_Publisher = Add_Publisher[Publisher_Name == rec];
															if(exist_Publisher.count() == 0)
															{
																insert into Add_Publisher
																[
																	Publisher_Name=rec
																	Added_User=zoho.loginuser
																]
																new_pulisher = Add_Publisher[Publisher_Name == rec];
																exist_PublisherList.add(new_pulisher.ID);
															}
															else
															{
																exist_PublisherList.add(exist_category.ID);
															}
														}
													}
													toAddPublisherList = exist_PublisherList.distinct();
													input.Title = book_details.get("title");
													disable Title;
													input.Edition = "";
													enable Edition;
													input.Author = toAddAuthorList;
													enable Author;
													input.Category = toAddCategoryList;
													enable Category;
													input.Publisher = toAddPublisherList;
													enable Publisher;
													input.Language = null;
													enable Language;
													input.Type = null;
													enable Type;
													input.Image = "<img  src='" + book_details.get("image") + "'/>";
													disable Image;
													input.Searching_Url = "<a href = \"" + book_details.get("valumeInfoURL") + "\">See more</a>";
													//show Searching_Url ;
													//alert Image;
												}
												else
												{
													input.Title = "";
													enable Title;
													input.Category = null;
													enable Author;
													input.Category = null;
													enable Category;
													input.Category = null;
													enable Publisher;
													input.Edition = null;
													enable Edition;
													input.Language = null;
													enable Language;
													input.Type = null;
													enable Type;
													input.Image = null;
													enable Image;
													input.Quantity = null;
													enable Quantity;
													input.Searching_Url = null;
													hide Book_Show;
												}
											}
										}
										else
										{
											reload;
										}
						)
					}
	}

			}
