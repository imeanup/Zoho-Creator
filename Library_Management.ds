		schedule
		{
			Alert_message_for_User as "Alert_message_for_User"
			{
				type =  schedule
				start = "2017-08-27 08:00:00"
				frequency = daily
				time zone = "Asia/Kolkata"
				on start
				{
					actions 
					{
					on load
					(
						oldRequests = Place_Order[Status == "Pending" || Status == "Approved" && To_Date < zoho.currentdate];
										for each  oldRequest in oldRequests
										{
											oldRequests.Status="Did not respect";
										}
										//Give alert to the user to return the resource if the till date is today
										rentTodaylast = Place_Order[Status == "Hired" && (To_Date == zoho.currentdate && Renewal_Date is null) || Renewal_Date == zoho.currentdate];
										for each  order in rentTodaylast
										{
											bookname = order.Item.Title;
											sendmail
											[
												from :zoho.adminuserid
												to :order.Member.Email
												subject :"Reminder Notificarion"
												message :"Hi,<br><br>You have rented the resource <b>" + bookname + " from " + order.From_Date + "  to " + order.To_Date + " </b>. Today is last date.<br><br> Plaese Return that.<br><br>Regards,<br>" + zoho.adminuser
											]
										}
										//Give alert to the user to return the resource if user have not return the resource
										rentPassedDate = Place_Order[Status == "Hired" && (To_Date < zoho.currentdate && Renewal_Date is null) || Renewal_Date < zoho.currentdate];
										for each  passedorOrder in rentPassedDate
										{
											bookname = passedorOrder.Item.Title;
											sendmail
											[
												from :zoho.adminuserid
												to :passedorOrder.Member.Email
												subject :"Reminder Notification"
												message :"Hi,<br><br> You have Rented the resource <b>" + bookname + " from " + order.From_Date + "  to " + order.To_Date + "</b> <br><br>You have passed the day to Return. <br><br>Plaese Return that.<br><br>Regards,<br>" + zoho.adminuser
											]
										}
					)
					}
				}
			}
