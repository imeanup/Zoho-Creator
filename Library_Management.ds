 			map usingAPI.getBookDetails(string isbn)
			{
				book_datails = Map();
				//get  the book details for by thhe API calling
				resource = getUrl("https://www.googleapis.com/books/v1/volumes?q=isbn:" + isbn).toMap();
				//If API gives data with not null value the book details will be build
				if(resource.get("totalItems").toLong() != 0)
				{
					items = resource.get("items").toJSONList();
					volumeInfo = items.get(0).getJSON("volumeInfo").toMap();
					//some datas are not properly in API Responce. the build as proper map type data
					authorList = "---".toList();
					if(volumeInfo.containKey("authors"))
					{
						author = volumeInfo.get("authors").removeFirstOccurence("[").removeLastOccurence("]").replaceAll("\"","");
						authorList = author.toList();
					}
					categoryList = "---".toList();
					if(volumeInfo.containKey("categories"))
					{
						category = volumeInfo.get("categories").removeFirstOccurence("[").removeLastOccurence("]").replaceAll("\"","");
						categoryList = category.toList();
					}
					publisherList = "---".toList();
					if(volumeInfo.containKey("publisher"))
					{
						publisher = volumeInfo.get("publisher").removeFirstOccurence("[").removeLastOccurence("]").replaceAll("\"","");
						publisherList = publisher.toList();
					}
					image = volumeInfo.get("imageLinks").toMap();
					book_datails.put("title",volumeInfo.get("title"));
					book_datails.put("author",authorList);
					book_datails.put("category",categoryList);
					book_datails.put("publisher",publisherList);
					book_datails.put("image",image.get("thumbnail"));
					book_datails.put("valumeInfoURL",volumeInfo.get("infoLink"));
				}
				//The Map type data is created by using API.Then that is returned.
				return book_datails;
			}
