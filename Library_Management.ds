 			void CustomActions.allowToTakeResource(Place_Order thisRequest)
			{
				//If the From date is today or before today the request status will be updated as the "Hired"
				if(thisRequest.From_Date <= zoho.currentdate)
				{
					thisReq = Place_Order[ID == thisRequest.ID];
					thisReq.Status="Hired";
					requestedResource = Resource[ID == thisRequest.Item];
					requestedResource.Available_Quantity=requestedResource.Available_Quantity - 1;
					//Notification maiil will be send to the Member
					sendmail
					[
						from :zoho.adminuserid
						to :thisRequest.Member.Email
						subject :"Conformation mail"
						message :"Hi,<br> The resource has been handled to you .<br><br><b>Your request details:</b><br><br><table style='border-collapse: collapse;border: 1px solid black;padding: 6px;color: brown;'><tr><td style='border: 1px solid black;padding: 6px;color: brown;'><b>Order Number</b></td><td style='border: 1px solid black;padding: 6px;color: brown;'>" + thisRequest.Order_Number + "</td></tr><tr><td style='border: 1px solid black;padding: 6px;color: brown;'><b>Resources</b></td><td style='border: 1px solid black;padding: 6px;color: brown;'>" + thisRequest.Item.Title + "</td></tr><tr><td style='border: 1px solid black;padding: 6px;color: brown;'><b>Period</b></td><td style='border: 1px solid black;padding: 6px;color: brown;'>" + thisRequest.From_Date + " To " + thisRequest.To_Date + "</td></table><br><br>Regards,<br>" + zoho.adminuser
					]
				}
				//If the From date after today this code is not allow to hire for the member.
				else
				{
					info "Not allow before From date";
				}
			}
 			void CustomActions.aproveRequest(Place_Order thisReq)
			{
				toAproveReq = Place_Order[ID == thisReq.ID];
				//Fetching the Resource for this request.
				resource = Resource[ID == toAproveReq.Item];
				resource.Request_Quantity=resource.Request_Quantity - 1;
				if(resource.Request_Quantity > 1)
				{
					//If resource is available to approve the request will be approved.Status will be updated as "Approved".
					toAproveReq.Status="Approved";
					if(toAproveReq.Member.Email != null)
					{
						//The notification mail will be send to the requester
						sendmail
						[
							from :zoho.adminuserid
							to :thisReq.Member.Email
							subject :"Resource is available"
							message :"Hi,<br> You have been Requested the resource.<br><br><b>Your request details:</b><br><br><table style='border-collapse: collapse;border: 1px solid black;padding: 6px;color: brown;'><tr><td style='border: 1px solid black;padding: 6px;color: brown;'><b>Order Number</b></td><td style='border: 1px solid black;padding: 6px;color: brown;'>" + thisReq.Order_Number + "</td></tr><tr><td style='border: 1px solid black;padding: 6px;color: brown;'><b>Resources</b></td><td style='border: 1px solid black;padding: 6px;color: brown;'>" + thisReq.Item.Title + "</td></tr><tr><td style='border: 1px solid black;padding: 6px;color: brown;'><b>Period</b></td><td style='border: 1px solid black;padding: 6px;color: brown;'>" + thisReq.From_Date + " To " + thisReq.To_Date + "</td></table><br><br>That is available today. So please get the resource.<br><br>Regards,<br>" + zoho.adminuser
						]
					}
				}
				if(resource.Request_Quantity == 1)
				{
					//If only one resource will be availble the Pop up window will be opened for the conformation.
					openUrl(zoho.appuri + "#From:Approve_Restriction_form?Order_Number=" + thisReq,"popup window");
				}
			}
 			void CustomActions.cancelOrder(Place_Order thisRequest)
			{
				//Cancell the Request
				if(zoho.loginuserid == zoho.adminuserid)
				{
					//update the availability in rsource stock
					if(thisRequest.Status == "Approved")
					{
						resource = Resource[ID == thisRequest.Item];
						resource.Request_Quantity=resource.Request_Quantity + 1;
					}
					//Request status will update as "Denied" when Request is denied by the admin 
					thisRequest.Status="Denied";
					if(thisRequest.Member.Email != null)
					{
						//Notification mail
						sendmail
						[
							from :zoho.adminuserid
							to :thisRequest.Member.Email
							subject :"Request denied"
							message :"Hi, You have been resquested for te resource.<br><br><b>Your request details:</b><br><br><table style='border-collapse: collapse;border: 1px solid black;padding: 6px;color: brown;'><tr><td style='border: 1px solid black;padding: 6px;color: brown;'><b>Order Number</b></td><td style='border: 1px solid black;padding: 6px;color: brown;'>" + thisRequest.Order_Number + "</td></tr><tr><td style='border: 1px solid black;padding: 6px;color: brown;'><b>Resources</b></td><td style='border: 1px solid black;padding: 6px;color: brown;'>" + thisRequest.Item.Title + "</td></tr><tr><td style='border: 1px solid black;padding: 6px;color: brown;'><b>Period</b></td><td style='border: 1px solid black;padding: 6px;color: brown;'>" + thisRequest.From_Date + " To " + thisRequest.To_Date + "</td></table><br><br> Your request has been denied. Because That resource is not available now.<br><br>Regards,<br>" + zoho.adminuser
						]
					}
				}
				else
				{
					if(thisRequest.Status == "Approved")
					{
						//update the availability in rsource stock
						resource = Resource[ID == thisRequest.Item];
						resource.Request_Quantity=resource.Request_Quantity + 1;
					}
					//Request status will be updtate as "Cancelled" when Request is denied by the admin.
					thisRequest.Status="Cancelled";
					//Notification mail
					sendmail
					[
						from :zoho.adminuserid
						to :thisRequest.Member.Email
						subject :"cancel conformation mail"
						message :"Hi,<br> You has been cancelled the request .<br><br><b>Your request details:</b><br><br><table style='border-collapse: collapse;border: 1px solid black;padding: 6px;color: brown;'><tr><td style='border: 1px solid black;padding: 6px;color: brown;'><b>Order Number</b></td><td style='border: 1px solid black;padding: 6px;color: brown;'>" + thisRequest.Order_Number + "</td></tr><tr><td style='border: 1px solid black;padding: 6px;color: brown;'><b>Resources</b></td><td style='border: 1px solid black;padding: 6px;color: brown;'>" + thisRequest.Item.Title + "</td></tr><tr><td style='border: 1px solid black;padding: 6px;color: brown;'><b>Period</b></td><td style='border: 1px solid black;padding: 6px;color: brown;'>" + thisRequest.From_Date + " To " + thisRequest.To_Date + "</td></table><br><br>Regards,<br>" + zoho.adminuser
					]
				}
			}
