				openUrl("#Form:Add_Users?recID=" + recID + "&User_Type=Members","popup window");
			}
 			void CustomAction.toActive(int recID)
			{
				openUrl("#Form:Add_Users?recID=" + recID + "&User_Type=Project Manager","popup window");
			}
 			void CustomAction.toArchive(int recID)
			{
				projectRec = New_Project[ID == input.recID];
				projectRec.Status="Archived";
				staffRec = Add_Staff[ID == projectRec.Project_Manager];
				staffRec.Availability="Available";
				userList = List:String();
				userList = projectRec.Users.toList();
				userList.removeelement(projectRec.Project_Manager.Email);
				projectRec.Users=userList.toString();
				projectRec.Project_Manager=null;
			}
 			void CustomAction.completeProject(int recID)
			{
				openUrl("#Form:Completion_Details?recID=" + recID + "&&Complete=Project","popup window");
			}
 			int ProjectPageView.getNextID(int currentID)
			{
				projectIDList = (New_Project[Status == "Active"].ID sort by Added_Time desc).getAll();
				nextIndex = projectIDList.indexOf(currentID) + 1;
				if(nextIndex < projectIDList.size())
				{
					nextID = projectIDList.get(nextIndex);
				}
				else
				{
					nextID = null;
				}
				return nextID;
			}
 			int ProjectPageView.getPrevID(int currentID)
			{
				projectIDList = (New_Project[Status == "Active"].ID sort by Added_Time desc).getAll();
				prevIndex = projectIDList.indexOf(currentID) - 1;
				if(prevIndex >= 0)
				{
					prevID = projectIDList.get(prevIndex);
				}
				else
				{
					prevID = null;
				}
				return prevID;
			}
	}



	workflow
	{
		form
		{
			Run_Scripts_On_Button_Cli3 as "Run Scripts - On Button Click"
			{
				type =  form
				form = Assign
				button = Submit
				on click
				{
					actions 
					{
					on click
					(
						if(input.recID != null)
										{
											if(input.Assignee_Job_Title == "Architect")
											{
												designRec = Add_Design_Documents[ID == input.recID];
												designRec.Architect=input.Assignee;
												designRec.Review_Status="Assigned To Architect";
												assigneeRec = Add_Staff[ID = input.Assignee].Email;
												//send mail to Assignee about design work assignment from Assign_Architect
												sendmail
												[
													from :zoho.loginuserid
													to :assigneeRec
