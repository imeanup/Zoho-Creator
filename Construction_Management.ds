			Run_Scripts_While_Editing1 as "Run Scripts - While Editing a Record"
			{
				type =  form
				form = New_Project
				record event = on edit

	on validate
	{
					actions 
					{
						custom deluge script
						(
											if(input.Start_Date > input.End_Date)
										{
											alert "Please check the project's date range.";
											cancel submit;
										}
										subContractorsChanged = false;
										oldSubcontractorsSize = old.Subcontractors;
										inputSubcontractorsSize = input.Subcontractors;
										if(oldSubcontractorsSize.size() == inputSubcontractorsSize.size())
										{
											for each  eachOldSubContractor in old.Subcontractors
											{
												for each  eachInputSubContractor in input.Subcontractors
												{
													if(eachInputSubContractor != eachOldSubContractor)
													{
														subContractorsChanged = true;
													}
												}
											}
										}
										else
										{
											subContractorsChanged = true;
										}
										siteInspectorsChanged = false;
										if(old.Site_Inspectors.size() == Site_Inspectors.size())
										{
											for each  eachOldSiteInspector in old.Site_Inspectors
											{
												for each  eachInputSiteInspector in input.Site_Inspectors
												{
													if(eachInputSiteInspector != eachOldSiteInspector)
													{
														siteInspectorsChanged = true;
													}
												}
											}
										}
										else
										{
											siteInspectorsChanged = true;
										}
										if(old.Client != input.Client || old.Project_Manager != input.Project_Manager || subContractorsChanged || siteInspectorsChanged)
										{
											userList = List:String();
											userList.add(zoho.adminuserid);
											userList.add(Add_Client[ID == input.Client].Email);
											userList.add(Add_Staff[ID == input.Project_Manager].Email);
