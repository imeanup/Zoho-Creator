				record event = on add

	on load
	{
					actions 
					{
						custom deluge script
						(
											input.Date_field = zoho.currentdate;
										if(zoho.loginuserid == zoho.adminuserid)
										{
											//Redirects admin to add the company detail
											detail_count = Add_Company_Detail[ID != 0].count(ID);
											if(detail_count == 0)
											{
												openUrl("#Form:Add_Company_Detail","popup window");
											}
											//Adding admin as an Inspector by default
											admin_count = Add_Site_Inspector[Email_Id == zoho.adminuserid].count(ID);
											if(admin_count == 0)
											{
												insert into Add_Site_Inspector
												[
													Added_User=zoho.loginuser
													Name.first_name="Admin"
													Email_Id=zoho.adminuserid
													Status="Active"
												]
											}
											//Alerts admin to add sites	
											total_sites = Add_Your_Site[ID != 0].count(ID);
											if(total_sites == 0 && detail_count != 0)
											{
												alert "Add Sites to start Inspections";
											}
											active_sites = Add_Your_Site[Work_Status == "Active"].ID.getAll();
											if(active_sites.size() == 0 && total_sites != 0)
											{
												alert "There are no active sites to start Inspection";
											}
											input.Site_Name:ui.add(active_sites);
											hide Send_Email_Immediately;
											input.Inspector_Name = Add_Site_Inspector[Email_Id == zoho.loginuserid].ID;
										}
										else
										{
											input.Inspector_Name = Add_Site_Inspector[Email_Id == zoho.loginuserid].ID;
											//Alerts about the site details to the Inspector 
											status = Add_Site_Inspector[Email_Id == zoho.loginuserid].Status;
											if(status != "Archive")
											{
												available_sites = Add_Your_Site[Select_Site_Inspector == input.Inspector_Name && Work_Status == "Active"].count(ID);
												if(available_sites == 0)
												{
													alert available_sites;
													alert "There is no sites assigned to you please contact admin";
												}
											}
											//Alerts the Inspector if he is in archive mode	
											else
											{
												alert "You are now in archive mode, so you can't do any Inspections. Please contact the admin for further details!";
											}
										}
						)
					}
	}

