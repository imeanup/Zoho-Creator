				]
			}
 			string Notification.Send_Mail()
			{
				//Constructing the table for inspection report and mail it to admin
				Mail_content = "Summary<br>";
				Mail_content = Mail_content + "<table style='border:1px solid black;border-collapse:collapse;width:100%;text-align:center;'><thead><tr style='border:1px solid black;border-collapse:collapse;background-color:#eaeaea;height:30px;'><th style='border:1px solid black;border-collapse:collapse;' rowspan='2'>Site Name</th><th style='border:1px solid black;border-collapse:collapse;' colspan='3'>Severity</th><th style='border:1px solid black;border-collapse:collapse;' rowspan='2'>Inspector Name</th><th rowspan='2'>Report</th></tr><tr style='border:1px solid black;border-collapse:collapse;background-color:#eaeaea;height:30px;'><th style='border:1px solid black;border-collapse:collapse;'>High</th><th style='border:1px solid black;border-collapse:collapse;'>Medium</th><th style='border:1px solid black;border-collapse:collapse;'>Low</th></tr></thead><tbody>";
				default_mailcontent = Mail_content;
				today_date = Add_Company_Detail[ID != 0].Schedule_Time;
				yesterday_date = today_date.subDay(1);
				for each  rec in Add_Your_Site[Work_Status == "Active"]
				{
					site_name = encodeurl(rec.Site_Name);
					query_url = "Site_Name=" + site_name + "&Date_field>" + yesterday_date;
					url = "https://app.zohocreator.com" + zoho.appuri + "#Report:All_Inspections?" + encodeUrl(query_url);
					info url;
					inspector_name = Add_Site_Inspector[ID == rec.Select_Site_Inspector].Name;
					high_count = Add_New_Inspection[Site_Name == rec.ID && Added_Time > yesterday_date && Added_Time < today_date && Severity == "High"].count(ID);
					mid_count = Add_New_Inspection[Site_Name == rec.ID && Added_Time > yesterday_date && Added_Time < today_date && Severity == "Medium"].count(ID);
					low_count = Add_New_Inspection[Site_Name == rec.ID && Added_Time > yesterday_date && Added_Time < today_date && Severity == "Low"].count(ID);
					if(low_count != 0 || mid_count != 0 || high_count != 0)
					{
						Mail_content = Mail_content + ("<tr style='border:1px solid black;height:40px;'><td style='border:1px solid black;border-collapse:collapse;'>" + rec.Site_Name + "</td><td style='border:1px solid black;border-collapse:collapse;'>" + high_count + "</td><td style='border:1px solid black;border-collapse:collapse;'>" + mid_count + "</td><td style='border:1px solid black;border-collapse:collapse;'>" + low_count + "</td><td style='border:1px solid black;border-collapse:collapse;'>" + inspector_name) + "</td><td style='border:1px solid black;border-collapse:collapse;'><a href=" + url + ">View Details</a></td></tr>";
					}
				}
				Mail_content = Mail_content + "</tbody></table>";
				default_mailcontent = default_mailcontent + "</tbody></table>";
				schedule_time = Add_Company_Detail[ID != 0];
				schedule_time.Schedule_Time=schedule_time.Schedule_Time.addDay(1);
				if(default_mailcontent != Mail_content)
				{
					return Mail_content;
				}
				else
				{
					return "No Mail";
				}
			}
	}



