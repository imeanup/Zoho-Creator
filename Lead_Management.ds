	functions
	{
 			void Complete(int Id)
			{
				activityRec = Add_Activity[ID == input.Id];
				activityRec.Status="Completed";
			}
 			void Cancel(int Id)
			{
				activityRec = Add_Activity[ID == input.Id];
				activityRec.Status="Cancelled";
			}
