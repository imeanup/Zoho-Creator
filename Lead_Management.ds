		schedule
		{
			Call_Mail_Schedule as "Call & Mail Schedule"
			{
				type =  schedule
				form = Add_Activity
				start = before 1 hours from From
				time zone = "PST8PDT"
				on start
				{
					actions 
					{
					on load
					(
						leadRec = Add_New_Lead[ID == input.Lead];
										RepRec = Add_Sales_Rep[ID == leadRec.Lead_Owner];
										productList = List:String();
										for each  product in leadRec.Interested_Product
										{
											productList.add(Add_Product[ID == product].Name);
										}
										sendmail
										[
											from :zoho.adminuserid
											to :RepRec.Email
											subject :"Gentle Reminder - " + input.Activity_Type + " at " + input.From
											message :"Hi " + RepRec.Name.first_name + ",<div><br></div><div>You have a&nbsp;" + input.Activity_Type + " with " + leadRec.Name.first_name + " &nbsp;" + leadRec.Name.last_name + "(" + leadRec.Company + ") in an hour.</div><div><br></div><div><b>Lead Details</b></div><div><b><br></b></div><div>Mobile No : " + leadRec.Mobile_No + "</div><div><br></div><div>Status : " + leadRec.Lead_Status + "<br></div><div><br></div><div>Interested Product(s) : " + productList + "</div><div><br></div><div><br></div><div>Regards,</div><div>" + zoho.adminuser + "</div>"
										]
					)
					}
				}
			}
