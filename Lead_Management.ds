										[
											from :zoho.adminuserid
											to :RepRec.Email
											subject :"Gentle Reminder - Email at " + input.Date_Time
											message :"Hi " + RepRec.Name.first_name + ",<div><br></div><div>You have to send an&nbsp;Email to " + leadRec.Name.first_name + "(" + leadRec.Company + ") in an hour.</div><div><br></div><div><b>Lead Details</b></div><div><b><br></b></div><div>Mobile No : " + leadRec.Mobile_No + "</div><div><br></div><div>Status : " + leadRec.Lead_Status + "<br></div><div><br></div><div>Interested Product(s) : " + productList + "</div><div><br></div><div><br></div><div>Regards,</div><div>" + zoho.adminuser + "</div>"
