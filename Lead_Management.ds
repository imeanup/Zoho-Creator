						leadRec = Add_New_Lead[ID == input.Lead];
										RepRec = Add_Sales_Rep[ID == leadRec.Lead_Owner];
										productList = List:String();
										for each  product in leadRec.Interested_Product
										{
											productList.add(Add_Product[ID == product].Name);
										}
