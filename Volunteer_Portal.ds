			Run_Scripts_On_Always10 as "Run Scripts - On Always"
			{
				type =  form
				form = Add_Log
				record event = on add or edit

	on success
	{
					actions 
					{
						custom deluge script
						(
											// fetching volunteer detail
										getvolDetail = Volunteer_Registration[ID == input.Volunteer_Email];
										// fetching event detail
										getEventDetail = Create_Event[ID == input.Event_Name];
										// fetching the event registration record to log the volunteer's point
										participationRec = Event_Registration[Email == getvolDetail.Email && Event_Name == input.Event_Name];
										// calculating no of days in the event for point calculation
										eventDays = days360(getEventDetail.Starting_Time,getEventDetail.Till_Date) + 1;
										// calculating points earned by the volunteer
										pointsEarned = ceil((ifnull(input.Total_Volunteered_minutes,1) * eventDays) / 100);
										participationRec.Status="Atteneded";
										participationRec.Volunteered_Minutes=input.Total_Volunteered_minutes;
										participationRec.Points_Earned=pointsEarned;
										getvolDetail.Total_Points_Earned=ifnull(getvolDetail.Total_Points_Earned,0) + pointsEarned;
						)
					}
	}

			}
