			Run_Scripts_On_Always9 as "Run Scripts - On Always"
			{
				type =  form
				form = Add_Log
				record event = on add or edit

	on validate
	{
					actions 
					{
						custom deluge script
						(
											for each  eachrow in Volunteered_Log
										{
											if(eachrow.Event_Dates > zoho.currentdate)
											{
												alert "Sorry Logs can't be added to Future Dates";
