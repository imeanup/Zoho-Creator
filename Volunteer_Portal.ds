		form Create_Event
		{
			displayname = "Create Event"
			success message = "Event Created Successfully!"
			field alignment = right
			feature property = 32
			Section
			(
				type = section
	 			row = 1
	 			column = 0   
				width = medium
			)
			must have event_name
			(
    			type = text
				displayname = "Event Name"
	 			row = 1
	 			column = 1   
				width = medium
			)
			must have Campaign
			(
				type = list
				values  = Add_Campaign.ID
    			displayformat = [Campaign]
				allow new entries = true
				[
					displayname = "Add New Campaign"
				]
				sortorder = ascending
				height = 60px
	 			row = 1
	 			column = 1   
				width = medium
			)
			goal
			(
    			type = textarea
				displayname = "Description about this event"
				height = 100px
	 			row = 1
	 			column = 1   
				width = medium
			)
			must have Venue_Address
			(
    			type = address
				displayname = "Venue Address"
     			capture_coordinates = true
     			adjust_using_map = false
     			address_line_12
     			(
	     			  type = address_line_1
	     			  displayname = "Address Line 1"
     			) 
     			address_line_22
     			(
	     			  type = address_line_2
	     			  displayname = "Address Line 2"
     			) 
     			district_city2
     			(
	     			  type = district_city
	     			  displayname = "City / District"
     			) 
     			state_province2
     			(
	     			  type = state_province
	     			  displayname = "State / Province"
     			) 
     			postal_Code2
     			(
	     			  type = postal_Code
	     			  displayname = "Postal Code"
     			) 
     			country2
     			(
	     			  type = country
	     			  displayname = "Country"
     			) 
     			latitude2
     			(
	     			  type = latitude
	     			  displayname = "latitude"
	     			 visibility = false
     			) 
     			longitude2
     			(
	     			  type = longitude
	     			  displayname = "longitude"
	     			 visibility = false
     			) 
	 			row = 1
	 			column = 1   
				width = medium
			)
			must have City
			(
				type = picklist
				values  = Add_City.ID
    			displayformat = [City]
				allow new entries = true
				[
					displayname = "Add New City"
				]
				sortorder = ascending
	 			row = 1
	 			column = 1   
				width = medium
			)
			must have Starting_Time
			(
    			type = datetime
				displayname = "Starting Time"
				timedisplayoptions = "hh:mm:ss"
				alloweddays = 0,1,2,3,4,5,6
	 			row = 1
	 			column = 1   
				width = medium
			)
			must have Till_Date
			(
    			type = datetime
				displayname = "Till Date"
				timedisplayoptions = "hh:mm:ss"
				alloweddays = 0,1,2,3,4,5,6
	 			row = 1
	 			column = 1   
				width = medium
			)
			must have Volunteers_Required
			(
				type = radiobuttons
				displayname = "Volunteers Required"
				values = {"Limited", "Unlimited"}
				layout = 3
	 			row = 1
	 			column = 1   
				width = medium
			)
			Volunteers_Count
			(
				type = number
				displayname = "Volunteers Count"
	 			row = 1
	 			column = 1   
				width = medium
			)
			Registered_Volunteers
			(
				type = list
				displayname = "Registered Volunteers"
				values  = Event_Registration.ID
    			bidirectional = Event_Name
    			displayformat = [Email]
				height = 60px
	 			row = 1
	 			column = 1   
				width = medium
			)
			Person_Of_Contact
			(
				type = grid
				displayname = "Person Of Contact"
	    
				must have Name
				(
    				type = name
     				prefix
     				(
	     				 type = prefix
	     				 displayname ="Prefix"
	     				 visibility = false
	     				 value = {"Mr.","Mrs.","Ms."}
     				) 
     				first_name
     				(
	     				 type = first_name
	     				 displayname ="First Name"
     				) 
     				last_name
     				(
	     				 type = last_name
	     				 displayname ="Last Name"
     				) 
     				suffix
     				(
	     				 type = suffix
	     				 displayname ="Suffix"
	     				 visibility = false
     				) 
					width = medium
				)
				must have E_Mail
				(
    				type = text
					displayname = "E-Mail"
					width = medium
				)
				must have Phone_Number
				(
    				type = phonenumber
					displayname = "Phone Number"
					width = medium
				)
	 			row = 2
	 			column = 1   
				width = medium
			)
	
			customize
			(
				icon = "ui-3-calendar-add"
			)
			actions
			{
				on add
				{
					submit
					(
   						type = submit
   						displayname = "Submit"
					)
					reset
					(
   						type = reset
   						displayname = "Reset"
					)
				}
				on edit
				{
					update
					(
   						type = submit
   						displayname = "Update"
					)
					cancel
					(
   						type = cancel
   						displayname = "Cancel"
					)
				}
			}
		}
	}
