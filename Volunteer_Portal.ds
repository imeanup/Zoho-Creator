											{
												getRec = Add_Log[Event_Name == input.Event_Name && Volunteer_Email == input.Volunteer_Email];
												// if already data is logged for this particular event, redirecting to edit url of the record
												if(getRec.count() != 0)
												{
													openUrl("#Form:Add_Log?recLinkID=" + getRec.ID + "&viewLinkName=My_Logs","same window");
												}
											}
										}
						)
					}
	}

