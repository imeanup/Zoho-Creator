			Run_Scripts_On_Always12 as "Run Scripts - On Always"
			{
				type =  form
				form = Add_Log
				record event = on add or edit

	on user input of Volunteered_Log.Check_Out
	{
					actions 
					{
						custom deluge script
						(
											if(row.Event_Dates != null)
										{
											rowMins = 0;
											if(row.Check_In != null && row.Check_In != "" && row.Check_Out != null && row.Check_Out != "")
											{
												checkIntime = (row.Event_Dates + " " + row.Check_In + ":00").toTime();
												checkOutTime = (row.Event_Dates + " " + row.Check_Out + ":00").toTime();
												if(checkOutTime.getMinutes() == 59)
												{
													checkOutTime = checkOutTime.addMinutes(1);
												}
												if(checkIntime > checkOutTime)
												{
													alert "Check out Time is less then Check In time";
													row.Check_Out="";
												}
												else if(checkOutTime > zoho.currenttime)
												{
													alert "Check out can't be added for future time";
													row.Check_Out="";
												}
												else
												{
													row.Volunteered_Minutes=floor(((checkOutTime - checkIntime)) / 60000);
													rowMins = ifnull(row.Volunteered_Minutes,0);
													totalMins = ifnull(input.Total_Volunteered_minutes,0);
													input.Total_Volunteered_minutes = totalMins + rowMins;
												}
											}
											else
											{
												row.Volunteered_Minutes=null;
											}
										}
						)
					}
	}

			}
