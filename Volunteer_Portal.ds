	functions
	{
 			void CustomAction.eventRegister(int eventId)
			{
				eventRecord = Create_Event[ID == input.eventId];
				if(eventRecord.Volunteers_Required == "Limited")
				{
					volRegs = Event_Registration[Event_Name == input.eventId];
					if(volRegs.count() == eventRecord.Volunteers_Count)
					{
						info "Sorry, Registration is closed";
					}
				}
				openUrl(zoho.appuri + "#Form:Event_Registration?Event_Name=" + eventId,"same window");
			}
 			void CustomAction.addLog(int eventRecID)
			{
				openUrl("#Form:Add_Log?Event_Name=" + eventRecID,"popup window");
			}
 			int Util.gettotalEvents()
			{
				return Create_Event[ID != 0].count();
			}
 			int Util.gettotalVolunteers()
			{
				return Volunteer_Registration[ID != 0].count();
			}
 			void CustomAction.cancelRegisteration(int Recid)
			{
				getRecDet = Event_Registration[ID == input.Recid];
				getEveDet = Create_Event[ID == getRecDet.Event_Name];
				getVolDet = Volunteer_Registration[Email == getRecDet.Email];
				sendmail
				[
					from :zoho.adminuserid
					to :getRecDet.Email,zoho.adminuserid
					subject :"Cancelled the Registration"
					message :("<div> Dear " + getVolDet.Name) + "</div>, <div> we are sorry to see you leaving the event <b>" + getEveDet.event_name + "</b>.</div><div>hope, we will meet you on upcoming opportunities</div><div>Regards,<div>" + zoho.appname + " Team</div></div>"
				]
				getRecDet.Status="Cancelled";
			}
 			void Sharing.sharetoVolunteers(string volEmail)
			{
				sharingResponse = thisapp.permissions.assignUserInProfile(volEmail,"Volunteer");
				// 	Sending Email notfication to the user for the app access
				sendmail
				[
					from :zoho.adminuserid
					to :volEmail
					subject :"Invitation to access '" + zoho.appname + "'"
					message :"<div style=\"width:100%;display:inline-block;box-sizing:border-box;border:1px solid #ddd;\"><div style=\"padding:2%;text-align:center;\">You've been invited to use an application built on Zoho Creator by \"" + zoho.adminuserid + "\"" + ".</div><div style=\"background: #efefef; padding: 20.0px;margin: 20.0px 0 10.0px;text-align:center;\"><h2>" + zoho.appname.toUpperCase() + "</h2><div style=\"margin: 20.0px 0 10.0px;\"><a style=\"padding:10px;background-color: rgb(62,149,205);text-decoration: none;color: rgb(255,255,255);font-size: 14.0px;\" href=\"" + "https://creator.zoho.com" + zoho.appuri + "\"" + " target=\"_blank\"> Go to application </a></div></div></div>"
				]
			}
 			void Unsharing.unsharetoVolunteers(string volEmail)
			{
				thisapp.permissions.deleteuser(volEmail);
			}
 			void Notifications.acknowledgementMail(int eventRegID)
			{
				getEventReg = Event_Registration[ID == input.eventRegID];
				getVolDet = Volunteer_Registration[Email == getEventReg.Email];
				sendmail
				[
					from :zoho.adminuserid
					to :getEventReg.Email
					subject :"Thanks for Registering the Event " + getEventReg.Event_Name
					message :("<div style='word-spacing:3px;font-size:15px;'><div> Dear " + getVolDet.Name) + ",</div><br><div> Thanks for showing your interest to volunteer in the Event " + getEventReg.Event_Name + ".</div><div>Stay connected with us for future updates.</div></div><br><div> Thanks and regards,</div><br> " + zoho.adminuser
				]
			}
 			string Util.recentUpdates()
			{
				volReg = (Volunteer_Registration[ID != 0].Added_Time sort by Added_Time desc).getAll();
				eveCreate = (Create_Event[ID != 0].Added_Time sort by Added_Time desc).getAll();
				tempList = List();
				tempList.addall(volReg);
				tempList.addall(eveCreate);
				tempList = tempList.distinct().sort(false);
				if(tempList.size() == 0)
				{
					returnStr = "<div class='noUpdates'><center class='noUpdates_Text'> No Recent Activity </center></div>  ";
				}
				else
				{
					// limiting list upto 10 items, to show recent 10 activities
					if(tempList.size() >= 13)
					{
						subList = tempList.subList(0,12);
					}
					else
					{
						subList = tempList;
					}
					// constructing a table
					returnStr = "<table class='tablestyle'>";
					for each  rec in subList
					{
						getVolunteerRec = Volunteer_Registration[Added_Time == rec.toTime()];
						getEventRec = Create_Event[Added_Time == rec.toTime()];
						if(getVolunteerRec.count() != 0)
						{
							returnStr = (returnStr + "<tr> <td class=''> <a href= '" + zoho.appuri + "record-summary/Volunteer_Details/" + getVolunteerRec.ID + "' target='_blank'> " + getVolunteerRec.Name) + " </a> is registered as new volunteer </td> </tr>  ";
						}
						if(getEventRec.count() != 0)
						{
							returnStr = returnStr + "<tr> <td class=''> <a href= '" + zoho.appuri + "record-summary/All_Events/" + getEventRec.ID + "' target='_blank'> " + getEventRec.event_name + " </a> event is created.</td> </tr>  ";
						}
					}
					returnStr = returnStr + "</table>";
				}
				return returnStr;
			}
	}



