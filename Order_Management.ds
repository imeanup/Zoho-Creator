	section New_Order
	{
		displayname= "Orders"
		icon = "shopping-basket-edit"
		form New_Order
		{
			displayname = "New Order"
			success message = "New order has been placed!"
			field alignment = left
			feature property = 32
			Section
			(
				type = section
	 			row = 1
	 			column = 0   
				width = medium
			)
			Order_ID
			(
    			type = autonumber
				displayname = "Order ID"
				start index = 1
	 			row = 1
	 			column = 1   
				width = medium
			)
			must have Sales_Owner
			(
				type = picklist
				displayname = "Sales Owner"
				values  = Add_Employee.ID
    			displayformat = [Employee_ID + " - " + Name.first_name + Name.last_name]
				sortorder = ascending
	 			row = 1
	 			column = 1   
				width = medium
			)
			must have Customer
			(
				type = picklist
				values  = Add_Customer.ID
    			displayformat = [Customer_ID + " - " + Name.first_name + " " + Name.last_name]
				allow new entries = true
				[
					displayname = "Add New Customer"
				]
				sortorder = ascending
	 			row = 1
	 			column = 1   
				width = medium
			)
			must have Order_Date
			(
    			type = date
				displayname = "Order Date"
				alloweddays = 0,1,2,3,4,5,6
	 			row = 1
	 			column = 1   
				width = medium
			)
			Delivery_Agency
			(
				type = picklist
				displayname = "Delivery Agency"
				values  = Add_Delivery_Agency.ID
    			displayformat = [Agency_Name]
				sortorder = ascending
	 			row = 1
	 			column = 1   
				width = medium
			)
			must have Estimated_Delivery_Date
			(
    			type = date
				displayname = "Estimated Delivery Date"
				alloweddays = 0,1,2,3,4,5,6
	 			row = 1
	 			column = 1   
				width = medium
			)
			Actual_Delivery_Date
			(
    			type = date
				displayname = "Actual Delivery Date"
				alloweddays = 0,1,2,3,4,5,6
	 			row = 1
	 			column = 1   
				width = medium
			)
			must have Products
			(
				type = grid
				values  = Orders_Details.ID
    			bidirectional = Order_ID
	 			row = 2
	 			column = 1   
				width = medium
			)
			Section1
			(
				type = section
				displayname = "Section"
	 			row = 3
	 			column = 0   
				width = medium
			)
			Sub_Total
			(
				displayname = "Sub Total"
				type = USD
				initial value = 0.00
	 			row = 3
	 			column = 1   
				width = medium
			)
			must have Tax
			(
    			type = percentage
				initial value = 0%
	 			row = 3
	 			column = 1   
				width = medium
			)
			Net_Total
			(
				displayname = "Net Total"
				type = USD
				initial value = 0.00
	 			row = 3
	 			column = 1   
				width = medium
			)
			Reason_For_Cancellation
			(
    			type = textarea
				displayname = "Reason For Cancellation"
				height = 100px
	 			row = 3
	 			column = 1   
				width = medium
			)
			Status
			(
				type = picklist
				values = {"Pending", "Ready For Delivery", "Delivered", "Cancelled"}
				initial value = "Pending"
	 			row = 3
	 			column = 1   
				width = medium
			)
	
			customize
			(
				icon = "shopping-cart-modern-add"
			)
			actions
			{
				on add
				{
					submit
					(
   						type = submit
   						displayname = "Submit"
					)
					reset
					(
   						type = reset
   						displayname = "Reset"
					)
				}
				on edit
				{
					update
					(
   						type = submit
   						displayname = "Update"
					)
					cancel
					(
   						type = cancel
   						displayname = "Cancel"
					)
				}
			}
		}
		list Pending_Orders
		{
			displayName = "Pending Orders"
			show all rows from New_Order  [Status == "Pending"]  
			(
				Products
				(
					field = Products.Order_ID + " " + Products.Product
				)
				Reason_For_Cancellation as "Reason For Cancellation"
				custom action "Ready To Delivery"
				(
	  			workflow = Ready_For_Delivery
				show action in view header = true
				column header = "Ready To Delivery"
				)
				Status
				Sub_Total as "Sub Total"
				Tax
				Net_Total as "Net Total"
				Order_Date as "Order Date"
				Delivery_Agency as "Delivery Agency"
				Customer
				Order_ID as "Order ID"
				Estimated_Delivery_Date as "Estimated Delivery Date"
				Sales_Owner as "Sales Owner"
			)
			group by
			(
				Order_Date ascending
			)
			custom actions
			(
				"Cancel"
				(
	  			workflow = Cancel
				show action in view header = true
				)
			)
			conditional formatting
			(
				"overDueAlert"
				{
				condition = (Estimated_Delivery_Date < zoho.currentdate)
				fields = [Estimated_Delivery_Date]
				format = "color:#e64d3b;background-color:#ffffff;"
				}
			)
			quickview
			(
				layout
				(
		 			type = -1
					datablock1
					(
			 			layout type = -1
		     			title = "Overview"
						fields
						(
							Order_ID as "Order ID"
							Order_Date as "Order Date"
							Sales_Owner as "Sales Owner"
							Customer
							Products.Order_ID as "Products"
							Estimated_Delivery_Date as "Estimated Delivery Date"
				"Ready To Delivery"
						)
					)
				)

				menu
    			(
    	 			header
    	 			(
    		 			Edit 
    		 			Duplicate 
    		 			Delete 
    	 			)
							record
							(
								Edit   	   
								Duplicate   	   
								Delete   	   
								"Ready To Delivery" as "Ready For Delivery"
								"Cancel"
    						)
    			)
    			action
    			(
							on click
							(
								View Record   	   
    						)
							on right click
							(
								Edit   	   
								Delete   	   
								Duplicate   	   
								View Record   	   
    						)
     			)
			)
			detailview
			(
				layout
				(
		 			type = 2
					datablock1
					(
			 			layout type = -2
		     			title = "Overview"
						fields
						(
							Order_ID as "Order ID"
							Order_Date as "Order Date"
							Sales_Owner as "Sales Owner"
							Delivery_Agency as "Delivery Agency"
							Estimated_Delivery_Date as "Estimated Delivery Date"
						)
					)
					datablock2
					(
			 			layout type = -2
		     			title = "Customer"
		     			related field =  Customer[Status == "Pending"]
						fields
						(
							Customer_ID as "Customer ID"
							Name
							Phone_Number as "Phone Number"
							Email
							Address
						)
					)
					datablock3
					(
			 			layout type = -1
		     			title = "Products"
		     			related field =  Products[Status == "Pending"]
						fields
						(
							Product
							Unit_Of_Measure as "Unit Of Measure"
							Quantity
							Unit_Price as "Unit Price"
							Total
						)
					)
					datablock4
					(
			 			layout type = -2
		     			title = "Invoice Details"
						fields
						(
							Sub_Total as "Sub Total"
							Tax
							Net_Total as "Net Total"
						)
					)
				)

				menu
    			(
    	 			header
    	 			(
			 			"Ready To Delivery" as "Ready For Delivery"
    		 			Edit 
    		 			Duplicate 
    		 			Delete 
    	 			)
    			)
			)
		}
