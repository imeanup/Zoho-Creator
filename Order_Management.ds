	functions
	{
 			void CustomAction.addStock(Add_Invoice invoiceDetails)
			{
				//Updating each inventory item stock level
				for each  eachRow in invoiceDetails.Products
				{
					inventoryProducts = Add_Product[ID == eachRow.Product];
					inventoryProducts.In_Stock=inventoryProducts.In_Stock + eachRow.Quantity;
					inventoryProducts.Selling_Price=eachRow.Selling_Price;
				}
				invoiceDetails.Add_products_to_inventory=true;
			}
 			void CustomAction.readyForDelivery(New_Order orderDetails)
			{
				//Validating each product availability here
				productsAvailable = true;
				for each  eachRow in orderDetails.Products
				{
					productDetails = Add_Product[ID == eachRow.Product];
					if(eachRow.Quantity > productDetails.In_Stock)
					{
						productsAvailable = false;
					}
				}
				if(productsAvailable)
				{
					for each  eachRow in orderDetails.Products
					{
						productDetails = Add_Product[ID == eachRow.Product];
						productDetails.In_Stock=productDetails.In_Stock - eachRow.Quantity;
						if(productDetails.In_Stock <= productDetails.Reorder_Level)
						{
							//Send notification mail to admin regarding stock level
							sendmail
							[
								from :zoho.loginuserid
								to :zoho.adminuserid
								subject :"Stock Level Alert"
								message :"Hi,<div><br></div><div>The stock level for the item '" + productDetails.Product_Code + "'" + " have reached the reorder level.</div><div><br></div><div>Please replenish the item as soon as possible.</div><div><br></div><div><a href=\"" + "https://app.zohocreator.com" + zoho.appuri + "#Report:Inventory?Product_Code=" + productDetails.Product_Code + "\"" + " target=\"_blank\">Click here</a> to view the more details.</div><div><br></div><div>Regards,</div><div>" + zoho.loginuser + "<br></div>"
							]
						}
					}
					orderDetails.Status="Ready For Delivery";
				}
				else
				{
					openUrl("#Form:Alert_Message?Order_ID=" + orderDetails.ID + "&zc_Header=false","Popup Window");
				}
			}
 			void CustomAction.delivered(New_Order orderDetails)
			{
				orderDetails.Status="Delivered";
				orderDetails.Actual_Delivery_Date=zoho.currentdate;
				sendmail
				[
					from :zoho.loginuserid
					to :orderDetails.Customer.Email
					subject :"[Order ID-" + orderDetails.Order_ID + "]Order Delivered"
					message :"Hi,<div><br></div><div>Your order[" + orderDetails.Order_ID + "] is out for delivery.</div><div><br></div><div>Thank you for your order.</div><div><br></div><div>Regards,</div><div>" + zoho.loginuser + "</div><div><br></div><div><br></div>"
				]
			}
 			void CustomAction.cancelOrder(int recID)
			{
				openUrl("#Form:Add_Reason?Order_ID=" + recID,"popup window");
			}
	}



	workflow
	{
		form
		{
			Run_Scripts_On_Click_of_C as "Run Scripts - On Click of 'Cancel' Button"
			{
				type =  form
				form = Alert_Message
				button = Cancel
				on click
				{
					actions 
					{
						openURL("#Report:Pending_Orders","new window");
					}
				}
			}
			Run_Scripts_On_Click_of_S as "Run Scripts - On Click of 'Submit' Button"
			{
				type =  form
				form = Add_Reason
				button = Submit
				on click
				{
					actions 
					{
					on click
					(
						orderDetails = New_Order[ID == input.Order_ID];
										orderDetails.Reason_For_Cancellation=input.Reason_For_Cancellation;
										orderDetails.Status="Cancelled";
										sendmail
										[
											from :zoho.loginuserid
											to :orderDetails.Customer.Email
											subject :"[Order ID-" + orderDetails.Order_ID + "]Order Cancellation"
											message :"Hi,<div><br></div><div>We are writing to let you know that this order has been cancelled.</div><div><br></div><div>Order Number is " + orderDetails.Order_ID + "</div><div><br></div><div>We regret any inconvenience this may cause, and we hope you will provide us the opportunity to serve you as a customer again.</div><div><br></div><div>Regards,</div><div>" + zoho.loginuser + "</div>"
										]
					)
						openURL("#Report:Pending_Orders","new window");
					}
				}
			}
			Run_Scripts_On_Click_of_N as "Run Scripts - On Click of 'Notify Customer' Button"
			{
				type =  form
				form = Alert_Message
				button = Notify_Customer
				on click
				{
					actions 
					{
					on click
					(
						sendmail
										[
