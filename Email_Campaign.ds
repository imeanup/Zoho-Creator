															from :zoho.adminuserid
															to :rec.Email
															subject :input.Email_Subject_for_this_Campaign1
															message :input.Message1
														]
													}
												}
												delete from Add_Subscriber[Name1.isEmpty() && Email is null];
											}
											else if(input.Subscribers_Type == "Type Manually")
											{
												insert into Create_New_List
												[
													Added_User=zoho.loginuser
													List_Name="Subscribers for " + input.Campaign_Name
												]
												subsList = Create_New_List[List_Name == "Subscribers for " + input.Campaign_Name].ID;
												for each  sub in input.Type_Subscribers
												{
													insert into Add_Subscriber
													[
														Added_User=zoho.loginuser
														Email=sub.Email
														Name1=sub.Name1
														New_List_Bi=subsList
														Status="Active"
													]
													insert into Subscribers_Subform_For_New_Campaign
													[
														Added_User=zoho.loginuser
														Email=sub.Email
														Name1=sub.Name1
														New_Campaign_Bi=input.ID
													]
													Email_Count = Email_Count + 1;
													sendmail
													[
														from :zoho.adminuserid
														to :sub.Email
														subject :input.Email_Subject_for_this_Campaign1
														message :input.Message1
													]
												}
											}
											if(Email_Count1[ID != 0].count() == 0)
											{
												insert into Email_Count1
												[
													Added_User=zoho.loginuser
													Count_field=Email_Count
												]
											}
											else
											{
												EmailCount = Email_Count1[ID != 0];
												EmailCount.Count_field=EmailCount.Count_field + Email_Count;
											}
											input.Status = "Sent";
										}
					)
					}
				}
			}
		}
