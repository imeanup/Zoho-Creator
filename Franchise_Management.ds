			Run_Scripts_On_Always16 as "Run Scripts - On Always"
			{
				type =  form
				form = Purchase_Order
				record event = on add or edit

	on user input of Products.Product
	{
					actions 
					{
						custom deluge script
						(
											if(row.Product != null)
										{
											//Validating the product existence
											productExistence = 0;
											for each  eachRow in Products
											{
												if(eachRow.Product == row.Product)
												{
													productExistence = productExistence + 1;
												}
											}
											if(productExistence > 1)
											{
												alert "Please check your selection, product already exist.";
												row.Product=null;
												row.Unit_Price=0.0;
												input.Sub_Total = input.Sub_Total - row.Total;
												row.Total=0.0;
											}
											else
											{
												productDetails = Add_Product[ID == row.Product];
												if(row.Category == null)
												{
													row.Category=productDetails.Product_Category;
												}
												row.Unit_Price=productDetails.Unit_Price;
												if(row.Quantity != null && row.Unit_Price != null)
												{
													row.Total=row.Quantity * row.Unit_Price;
												}
												else
												{
													row.Total=0.0;
												}
												input.Sub_Total = 0.0;
												//calculating each product cost and updating in "Sub Total" field
												for each  eachProduct in Products
												{
													input.Sub_Total = input.Sub_Total + eachProduct.Total;
												}
											}
										}
										else
										{
											row.Unit_Price=0.0;
											input.Sub_Total = input.Sub_Total - row.Total;
											row.Total=0.0;
										}
						)
					}
	}

			}
