			Run_Scripts_While_Adding_4 as "Run Scripts - While Adding a Record"
			{
				type =  form
				form = Add_Call
				record event = on add

	on validate
	{
					actions 
					{
						custom deluge script
						(
											if(input.Call_Start_Time >= input.Call_End_Time || input.Call_Start_Time > zoho.currenttime || input.Call_End_Time > zoho.currenttime)
										{
											alert "Please check the call time";
											cancel submit;
										}
										if(input.Follow_up_Required == "Yes")
										{
											if(input.Follow_up_Time == null || input.Follow_up_Time <= zoho.currenttime)
											{
												alert "Please specify/check the follow-up time";
												cancel submit;
											}
										}
										else
										{
											input.Follow_up_Time = null;
											input.Follow_up_Notes = "";
										}
						)
					}
	}

			}
