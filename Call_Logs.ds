		schedule
		{
			Call_Followup as "Call_Followup"
			{
				type =  schedule
				form = Add_Call
				start = before 1 hours from Follow_up_Time
				time zone = "Asia/Kolkata"
				on start
				{
					actions 
					{
					on load
					(
						notes = input.Follow_up_Notes;
										if(input.Follow_up_Notes == "" || input.Follow_up_Notes == null)
										{
											notes = "-";
										}
										sendmail
										[
											from :zoho.adminuserid
											to :input.Call_Owner.Email
											subject :"Followup Reminder at " + input.Follow_up_Time
											message :"Hi " + input.Call_Owner.Name.first_name + ",<div><br></div><div>This is a reminder that you have a followup activity in an hour.</div><div><br></div><div><div>Followup Notes :&nbsp;" + notes + "</div></div><div><br></div><div>Click <a href=\"" + "https://app.zohocreator.com" + zoho.appuri + "#Report:Customer_Conversation?Customer=" + input.Customer.Phone + "&amp;Customer_op=26\"" + " target=\"_blank\">here</a>&nbsp;to view the coversation.</div><div><br></div><div>Regards,</div><div>" + zoho.adminuser + "</div>"
										]
					)
					}
				}
			}
