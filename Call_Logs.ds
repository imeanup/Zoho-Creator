			Run_Scripts_On_Always2 as "Run Scripts - On Always"
			{
				type =  form
				form = Add_Call
				record event = on add or edit

	on user input of Type
	{
					actions 
					{
						custom deluge script
						(
											if(input.Type == "Current Call")
										{
											show End_Call;
											//disable Call_Start_Time;
											disable Call_End_Time;
											input.Call_Start_Time = zoho.currenttime;
											input.Call_End_Time = null;
										}
										else
										{
											hide End_Call;
											//enable Call_Start_Time;
											enable Call_End_Time;
										}
						)
					}
	}

			}
			Run_Scripts_On_Always3 as "Run Scripts - On Always"
			{
				type =  form
				form = Add_Call
				record event = on add or edit

	on user input of Call_End_Time
	{
					actions 
					{
						custom deluge script
						(
											if(input.Call_Start_Time != null && input.Call_End_Time != null)
										{
											millis = (input.Call_End_Time - input.Call_Start_Time);
											if(millis <= 0)
											{
												input.Duration = "00:00:00";
											}
											else
											{
												days = (millis / 86400000).toLong();
												hours = ((millis % 86400000) / 3600000).toLong();
												mins = ((millis % 86400000 % 3600000) / 60000).toLong();
												secs = ((millis % 86400000 % 3600000 % 60000) / 1000).toLong();
												hours = days * 24 + hours;
												input.Duration = hours + ":" + mins + ":" + secs;
											}
										}
						)
					}
	}

			}
