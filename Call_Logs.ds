				 page DashboardDetails(userType) 
	 	 { 
 	 	 displayname = "DashboardDetails" 

 	 	 icon = "ui-1-database" 

 	 	 hide =  true
	 	 Content="<zml \t\t\n\t\t\n\t\t\n\t\t\n\t\t\n\t\t>\n\t<layout>\n\t<row>\n\t<column\n   \t\t width=\"100%\"\n   \t>\n\t<dsp id='DashboardDetails1'>\n\t<![CDATA[htmlpage DashboardDetails1(userType)\ncontent\n&lt;%{\n\tfromDateTime = zoho.currentdate.toTime();\n\ttoDateTime = zoho.currentdate.addHour(23).addMinutes(59).addSeconds(59);\n\tweekStartDate = toStartOfWeek(zoho.currentdate).toTime();\n\tweekEndDate = weekStartDate.addDay(6).addHour(23).addMinutes(59).addSeconds(59);\n\tmonthlyStartDate = toStartOfMonth(zoho.currentdate).toTime();\n\tmonthlyEndDate = eomonth(zoho.currentdate,0).addHour(23).addMinutes(59).addSeconds(59);\n\tif(input.userType == &quot;admin&quot;)\n\t{\n\t\tweeklyCallLink = &quot;Weekly_Logs&quot;;\n\t\tmonthlyCallLink = &quot;Monthly_Logs&quot;;\n\t\tcallInbound = Add_Call[Call_Direction == &quot;Inbound&quot; &amp;&amp; Call_Start_Time &gt;= fromDateTime &amp;&amp; Call_Start_Time &lt;= toDateTime].count();\n\t\tcallOutbound = Add_Call[Call_Direction == &quot;Outbound&quot; &amp;&amp; Call_Start_Time &gt;= fromDateTime &amp;&amp; Call_Start_Time &lt;= toDateTime].count();\n\t\tweeklyCallInbound = Add_Call[Call_Direction == &quot;Inbound&quot; &amp;&amp; Call_Start_Time &gt;= weekStartDate &amp;&amp; Call_Start_Time &lt;= weekEndDate].count();\n\t\tweeklyCallOutbound = Add_Call[Call_Direction == &quot;Outbound&quot; &amp;&amp; Call_Start_Time &gt;= weekStartDate &amp;&amp; Call_Start_Time &lt;= weekEndDate].count();\n\t\tmonthlyCallInbound = Add_Call[Call_Direction == &quot;Inbound&quot; &amp;&amp; Call_Start_Time &gt;= monthlyStartDate &amp;&amp; Call_Start_Time &lt;= monthlyEndDate].count();\n\t\tmonthlyCallOutbound = Add_Call[Call_Direction == &quot;Outbound&quot; &amp;&amp; Call_Start_Time &gt;= monthlyStartDate &amp;&amp; Call_Start_Time &lt;= monthlyEndDate].count();\n\t\tcallReportLink = &quot;All_Calls&quot;;\n\t}\n\telse\n\t{\n\t\tweeklyCallLink = &quot;My_Weekly_Calls&quot;;\n\t\tmonthlyCallLink = &quot;My_Monthly_Calls&quot;;\n\t\tcallInbound = Add_Call[Call_Direction == &quot;Inbound&quot; &amp;&amp; Call_Start_Time &gt;= fromDateTime &amp;&amp; Call_Start_Time &lt;= toDateTime &amp;&amp; Call_Owner.Email == zoho.loginuserid].count();\n\t\tcallOutbound = Add_Call[Call_Direction == &quot;Outbound&quot; &amp;&amp; Call_Start_Time &gt;= fromDateTime &amp;&amp; Call_Start_Time &lt;= toDateTime &amp;&amp; Call_Owner.Email == zoho.loginuserid].count();\n\t\tweeklyCallInbound = Add_Call[Call_Direction == &quot;Inbound&quot; &amp;&amp; Call_Start_Time &gt;= weekStartDate &amp;&amp; Call_Start_Time &lt;= weekEndDate &amp;&amp; Call_Owner.Email == zoho.loginuserid].count();\n\t\tweeklyCallOutbound = Add_Call[Call_Direction == &quot;Outbound&quot; &amp;&amp; Call_Start_Time &gt;= weekStartDate &amp;&amp; Call_Start_Time &lt;= weekEndDate &amp;&amp; Call_Owner.Email == zoho.loginuserid].count();\n\t\tmonthlyCallInbound = Add_Call[Call_Direction == &quot;Inbound&quot; &amp;&amp; Call_Start_Time &gt;= monthlyStartDate &amp;&amp; Call_Start_Time &lt;= monthlyEndDate &amp;&amp; Call_Owner.Email == zoho.loginuserid].count();\n\t\tmonthlyCallOutbound = Add_Call[Call_Direction == &quot;Outbound&quot; &amp;&amp; Call_Start_Time &gt;= monthlyStartDate &amp;&amp; Call_Start_Time &lt;= monthlyEndDate &amp;&amp; Call_Owner.Email == zoho.loginuserid].count();\n\t\tcallReportLink = &quot;My_Calls&quot;;\n\t}\n\t%&gt;\n&lt;style&gt;\n\t\t\t\t\t\t\t\t\t    .embdDiv .reportfullwidth iframe\n\t\t\t\t\t\t\t\t\t      {\n\t\t\t\t\t\t\t\t\t      height:310px !important;\n\t\t\t\t\t\t\t\t\t      }\n\t\t\t\t\t\t\t\t\t\t  .zc-perma-pane-right table .divEmbedContainer {\n\t\t\t\t\t\t\t\t\t    height: 100%;\n\t\t\t\t\t\t\t\t\t}\n\t\t\t\t\t\t\t\t\t\t  .embdDivRow2 .reportfullwidth iframe\n\t\t\t\t\t\t\t\t\t      {\n\t\t\t\t\t\t\t\t\t      height:320px !important;\n\t\t\t\t\t\t\t\t\t      }\n\t\t\t\t\t\t\t\t\t\t  .embdDivRow2 .legendarea{\n\t\t\t\t\t\t\t\t\t\t\t  left:90% !important;\n\t\t\t\t\t\t\t\t\t\t  }\n\t\t\t\t\t\t\t\t\t\t