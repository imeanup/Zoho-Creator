			Run_Scripts_While_Adding_3 as "Run Scripts - While Adding a Record"
			{
				type =  form
				form = Add_Call
				record event = on add

	on load
	{
					actions 
					{
						custom deluge script
						(
											hide End_Call;
										disable Duration;
										hide Follow_up_Time;
										hide Follow_up_Notes;
										input.Call_Owner = Add_Sales_Rep[Email == zoho.loginuserid].ID;
										disable Call_Owner;
						)
					}
	}

