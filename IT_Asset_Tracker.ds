	workflow
	{
		form
		{
			Run_Scripts_While_Adding_1 as "Run Scripts - While Adding a Record"
			{
				type =  form
				form = addhardware
				record event = on add

	on load
	{
					actions 
					{
						custom deluge script
						(
											input.Active_Service_Date = zoho.currentdate;
						)
					}
	}

			}
			Field_scripts as "Field scripts"
			{
				type =  form
				form = installation
				record event = on add or edit

	on user input of Software
	{
					actions 
					{
						custom deluge script
						(
											isfree = true;
										isexpired = false;
										total_licenses_count = 0;
										usage_count = 0;
										exceeded_licenses_count = 0;
										for each  license in addlicense[Software == input.Software]
										{
											if(license.License_Type != "Free")
											{
												isexpired = false;
												isfree = false;
												total_licenses_count = total_licenses_count + license.No_of_Licenses;
												if(license.Expire_On != null)
												{
													if((zoho.currentdate - license.Expire_On) > 0)
													{
														isexpired = true;
														exceeded_licenses_count = exceeded_licenses_count + license.No_of_Licenses;
													}
												}
												if(!isexpired)
												{
													usage_count = usage_count + license.Usage_Count;
													if(license.No_of_Licenses > usage_count)
													{
														input.License_ID = license.ID;
													}
												}
											}
											else
											{
												input.License_ID = license.ID;
											}
										}
										if(input.License_ID == null)
										{
											input.msg = "<font size='2' color='#ff6600'><strong>No License configured for this Software.</strong></font>";
											input.Is_Valid_Installation = false;
										}
										else if(isfree)
										{
											input.Is_Valid_Installation = true;
										}
										else
										{
											remaining_count = total_licenses_count - (exceeded_licenses_count + usage_count);
											input.msg = "<font size='2'><strong>Total Licenses : <font color='#ff6600'>" + total_licenses_count + "</font></strong></font><br> <font size='2'><strong>Exceeded Licenses : <font color='#ff6600'>" + exceeded_licenses_count + "</font></strong></font><br><font size='2'><strong>Licenses Used : <font color='#ff6600'>" + usage_count + "</font></strong></font><br><font size='2'><strong>Remaining Licenses : <font color='#ff6600'>" + remaining_count + "</font></strong></font><br>";
											if(remaining_count <= 0)
											{
												input.Is_Valid_Installation = false;
											}
											else
											{
												input.Is_Valid_Installation = true;
											}
										}
						)
					}
	}

			}
			Field_scripts3 as "Field scripts"
			{
				type =  form
				form = addlicense
				record event = on add or edit

	on user input of License_Type
	{
					actions 
					{
						custom deluge script
						(
											if(input.License_Type == "Free")
										{
											hide No_of_Licenses;
											hide Expire_On;
										}
										else
										{
											show No_of_Licenses;
											show Expire_On;
										}
						)
					}
	}

			}
			Edit_record_scripts as "Edit record scripts"
			{
				type =  form
				form = addlicense
				record event = on edit

	on load
	{
					actions 
					{
						custom deluge script
						(
											hide Usage_Count;
										disable Software;
										if(input.License_Type != "Free")
										{
											show No_of_Licenses;
											show Expire_On;
										}
										else
										{
											hide No_of_Licenses;
											hide Expire_On;
										}
						)
					}
	}

			}
			Create_record_scripts1 as "Create record scripts"
			{
				type =  form
				form = installation
				record event = on add

	on load
	{
					actions 
					{
						custom deluge script
						(
											//You can drag and drop tasks from the immediate left pane
										//Below is the SAMPLE script that hide fields while loading a form.
										hide Expire_On;
										hide Is_Valid_Installation;
										hide License_ID;
										input.Installed_On = zoho.currentdate;
						)
					}
	}

			}
			Create_record_scripts4 as "Create record scripts"
			{
				type =  form
				form = installation
				record event = on add

	on validate
	{
					actions 
					{
						custom deluge script
						(
											if(input.Is_Valid_Installation != true)
										{
											alert "<font size='2'><strong>" + input.Software.Name + " doesn't have any remaining License.</strong></font>";
											cancel submit;
										}
										installs_count = installation[Software == input.Software && Machine_Name == input.Machine_Name].count();
										if(installs_count > 0)
										{
											alert "<font size='2'><strong>" + input.Software + " is already installed in the selected machine.</strong></font>";
											cancel submit;
										}
						)
					}
	}

			}
			Create_record_scripts5 as "Create record scripts"
			{
				type =  form
				form = installation
				record event = on add

	on success
	{
					actions 
					{
						custom deluge script
						(
											if(input.Is_Valid_Installation == true)
										{
											if(input.License_ID != null)
											{
												license = addlicense[ID == input.License_ID];
												license.Usage_Count=license.Usage_Count + 1;
											}
										}
						)
					}
	}

			}
			Create_record_scripts6 as "Create record scripts"
			{
				type =  form
				form = addlicense
				record event = on add

	on load
	{
					actions 
					{
						custom deluge script
						(
											hide No_of_Licenses;
										hide Expire_On;
										hide Usage_Count;
										input.Purchased_On = zoho.currentdate;
										if(input.Software != null)
										{
											disable Software;
										}
						)
					}
	}

			}
			Create_record_scripts7 as "Create record scripts"
			{
				type =  form
				form = addlicense
				record event = on add

	on validate
	{
					actions 
					{
						custom deluge script
						(
											if(input.License_Type == "Licensed" && input.No_of_Licenses == null)
										{
											alert "No of Licenses cannot be empty.";
											cancel submit;
										}
						)
					}
	}

			}
			Create_record_scripts8 as "Create record scripts"
			{
				type =  form
				form = addsoftware
				record event = on add

	on success
	{
					actions  (Add_License == true)
					{
						custom deluge script
						(
											openUrl("#Form:addlicense?Software=" + input.ID,"same window");
						)
					}
	}

			}
		}
