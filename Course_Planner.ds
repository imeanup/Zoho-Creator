			Run_Scripts_On_Click_of_Y as "Run Scripts - On Click of 'Yes' button"
			{
				type =  form
				form = Tutoring_Approval_Confirmation
				button = Yes
				on click
				{
					actions 
					{
					on click
					(
						Status = "Approved";
										requestRec = Tutoring_Request[ID == input.Tutor_Request];
										courseRecord = Add_Course[ID == requestRec.Course];
										courseRecord.Select_Tutor=requestRec.Tutor;
										tutorRec = Enroll_User[ID == requestRec.Tutor];
										sendmail
										[
											from :zoho.adminuserid
											to :tutorRec.Email
											subject :"Tutoring Request Approval Notification"
											message :("Hi " + tutorRec.Name) + ",<div><br></div><div>Your tutoring request for " + courseRecord.Course_Name + " has been approved.&nbsp;</div><div><br></div><div>Click <a href=\"" + "https://app.zohocreator.com" + zoho.appuri + "#Report:Tutoring_Requests_Tutor\"" + " target=\"_blank\">here</a> to view the details.</div><div><br></div><div>Regards,</div><div>" + zoho.adminuser + "</div>"
										]
										allRequests = Tutoring_Request[Course == requestRec.Course && Status == "Requested" && ID != input.Tutor_Request];
										for each  request in allRequests
										{
											tutorRec = Enroll_User[ID == request.Tutor];
											request.Status="Rejected";
											sendmail
											[
												from :zoho.adminuserid
												to :tutorRec.Email
												subject :"Tutoring Request Rejection Notification"
												message :("Hi " + tutorRec.Name) + ",<div><br></div><div>We regret to inform you that your request for tutoring '" + courseRecord.Course_Name + "' has been rejected by admin due to some valid reasons.</div><div><br></div><div>Click <a href='https://app.zohocreator.com" + zoho.appuri + "#Report:Tutoring_Requests_Tutor' target='_blank'>here</a> to view the status.</div><div><br></div><div>Regards,</div><div>" + zoho.adminuser + "</div><div><br></div>"
											]
										}
