			Run_Scripts_On_Click_of_O as "Run Scripts - On Click of 'Ok' button"
			{
				type =  form
				form = Course_Subscription
				button = Ok
				on click
				{
					actions 
					{
					on click
					(
						courseRecord = Add_Course[ID == input.Course];
										studentId = Enroll_User[Email == zoho.loginuserid].ID;
										if(courseRecord.Course_Type == "Free")
										{
											//Add student to the corresponding course
											studentRecord = Enroll_User[Email == zoho.loginuserid];
											courseRecord = Add_Course[ID == input.Course];
											studentsList = courseRecord.Students;
											studentsList.add(studentRecord.ID);
											courseRecord.Students=studentsList;
										}
										else if(courseRecord.Course_Type == "Paid")
										{
											openUrl("#Form:Course_Payment?Course=" + input.Course + "&Student=" + studentId + "&Amount=" + courseRecord.Amount,"same window");
										}
					)
					}
