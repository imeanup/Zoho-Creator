			Course_Subscription as "Course Subscription"
			{
				type =  functions
				form = Add_Course
				on start
				{
					actions 
					{
					on click
					(
						//Subscription confirmation message in case of free course
										if(input.Course_Type == "Free")
										{
											openUrl("#Form:Course_Subscription?Course=" + input.ID,"popup window");
										}
										//Proceed to payment in case of paid course
										else if(Course_Type == "Paid")
										{
											studentRecord = Enroll_User[Email == zoho.loginuserid];
											openUrl("#Form:Course_Payment?Course=" + input.ID + "&Student=" + studentRecord.ID + "&Amount=" + input.Amount,"same window");
										}
					)
					}
				}
			}
			View_Course_Material as "View Course Material"
			{
				type =  functions
				form = Add_Course
				on start
				{
					actions 
					{
					on click
					(
						//Show course materials
										openUrl("https://app.zohocreator.com" + zoho.appuri + "#Report:Course_Page?Course=" + input.Course_Name,"new window");
					)
					}
				}
			}
			Tutor_this_course as "Tutor this course"
			{
				type =  functions
				form = Add_Course
				on start
				{
					actions 
					{
					on click
					(
						//Sending tutor request to admin
										TutorID = Enroll_User[Email == zoho.loginuserid].ID;
										insert into Tutoring_Request
										[
											Added_User=zoho.loginuser
											Tutor=TutorID
											Course=input.ID
											Status="Requested"
										]
					)
					}
				}
			}
			Course_Materials_Tutor as "Course Materials - Tutor"
			{
				type =  functions
				form = Add_Course
				on start
				{
					actions 
					{
					on click
					(
						//Show course materials
										openUrl("https://app.zohocreator.com" + zoho.appuri + "#Report:Course_Page?Course=" + input.Course_Name,"new window");
					)
					}
				}
			}
			Approve_tutor as "Approve tutor"
			{
				type =  functions
				form = Tutoring_Request
				on start
				{
					actions 
					{
					on click
					(
						//Assign tutor to the requested course
										if(Tutoring_Request[Course == input.Course && Status == "Requested"].count() > 1)
										{
											openUrl("#Form:Tutoring_Approval_Confirmation?Tutor_Request=" + input.ID + "&zc_Header=false","popup window");
										}
										else
										{
											input.Status = "Approved";
											tutorRec = Enroll_User[ID == input.Tutor];
											courseRecord = Add_Course[ID == input.Course];
											courseRecord.Select_Tutor=input.Tutor;
											sendmail
											[
												from :zoho.adminuserid
												to :tutorRec.Email
												subject :"Tutoring Request Approval Notification"
												message :("Hi " + tutorRec.Name) + ",<div><br></div><div>Your tutoring request for " + courseRecord.Course_Name + " has been approved.&nbsp;</div><div><br></div><div>Click <a href=\"" + "https://app.zohocreator.com" + zoho.appuri + "#Report:Tutoring_Requests_Tutor\"" + " target=\"_blank\">here</a> to view the details.</div><div><br></div><div>Regards,</div><div>" + zoho.adminuser + "</div>"
											]
										}
					)
					}
				}
			}
