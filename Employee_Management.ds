						custom deluge script
						(
											//Validating 'To' date value
										if(To != null && From != null)
										{
											if(input.From.getYear() == input.To.getYear())
											{
												if(input.To < input.From)
												{
													input.No_of_Days = 0;
													alert "'To' date must be greater than or equal to 'From' date!";
												}
												else
												{
													input.No_of_Days = days360(input.From,input.To) + 1;
												}
											}
											else
											{
												input.No_of_Days = 0;
												alert "Kindly avail current year leaves alone and apply separate leave for the upcoming year!";
											}
										}
										else
										{
											input.No_of_Days = 0;
										}
						)
					}
	}

