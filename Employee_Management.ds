	functions
	{
 			void customAction.approveLeave(Apply_Leave leaveRecord)
			{
				leaveRecord.Status="Approved";
			}
 			void customAction.rejectLeave(Apply_Leave leaveRecord)
			{
				leaveRecord.Status="Rejected";
			}
 			map profile.getSearchList(Add_Employee recRowList)
			{
				mapContent = Map();
				output = "";
				rowCount = 0;
				if(recRowList.ID != null)
				{
					output = output + "<div style='margin:0 auto;width:90%;'><table cellspacing='15px' style='padding:24px 78px;width:80%' align='center'>";
					for each  recRow in recRowList
					{
						rowCount = rowCount + 1;
						balCount = rowCount % 2;
						if(balCount != 0)
						{
							output = output + "<tr>";
						}
						output = output + "<td>";
						output = output + "<table algin='right'>";
						output = output + "<tr>";
						output = output + "<td rowspan='6' style='padding-right: 10px;'>" + thisapp.profile.replaceImgTag(recRow.Photo,"All_Employees","") + "</td>";
						output = output + "<td>";
						output = output + "<div id='searchContent'>";
						output = (output + "<p><span class='empId'>" + recRow.Employee_ID + "<a href='#View:Employee_Profile?profileID=" + recRow.ID + "'><span class='name' style='padding-left:3px;'>" + recRow.Name) + "</span></a></p>";
						output = output + "<p><a href='#View:Employee_Profile?department=" + recRow.Department + "'><span class='team'>" + recRow.Department + "</span></a></p>";
						output = output + "<p class='email'><a href='mailto:" + recRow.Official_Email + "' style='color:357FD4'>" + recRow.Official_Email + "</a></p>";
						output = output + "<p><span class='contact'>Work Phone: </span><span class='number'>" + "recRow.Work_Phone" + ", </span><span class='contact'>Extn.No: </span><span class='number'>" + recRow.Extension + "</span></p>";
						output = output + "<p><span class='contact'>Mobile.No: </span><span class='number'>" + recRow.Mobile_Number + "</span></p>";
						output = output + "<p><span class='contact'>Location: </span><span class='number'>" + recRow.Seat_Location + "</span></p>";
						output = output + "</div>";
						output = output + "</td>";
						output = output + "</tr>";
						output = output + "</table>";
						output = output + "</td>";
						if(balCount == 0)
						{
							output = output + "</tr>";
						}
					}
					output = output + "</table><div style='clear:both;'></div></div>";
				}
				else
				{
					output = output + "<div style='border: 2px solid #F3F3F3;border-radius:5px;width: 28%;text-align:center;margin: 17px 34%;height: 60px;'><p style='color: #A3A3A3;font-size: 15px;font-weight: bold;position: relative;top: 19px;'>Noting match with this key word!</p></div>";
				}
				mapContent.put("searchContent",output);
				return mapContent;
			}
 			string profile.replaceImgTag(string imgTag, string viewLinkName, string className)
			{
				imgTag = imgTag.replaceAll("/sharedBy","https://creator.zoho.com/" + zoho.adminuser,false);
				imgTag = imgTag.replaceAll("appLinkName",zoho.appname,false);
				imgTag = imgTag.replaceAll("viewLinkName",viewLinkName,false);
				imgTag = imgTag.replaceAll("fieldName","Photo",false);
				if(input.className != null && input.className != "")
				{
					imgTag = imgTag.replaceAll(">","class='" + className + "'>",false);
				}
				else
				{
					imgTag = imgTag.replaceAll(">","style='width:100px;height:100px;'>",false);
				}
				return imgTag;
			}
 			map profile.getProfileInfo(int profileID, string isEdit)
			{
				return Map();
			}
