	section Recruitment_Events
	{
		displayname= "Recruitment Events"
		icon = "users-2-search"
		form Add_Event
		{
			displayname = "Add Event"
			success message = "The Event\n Added Successfully!"
			field alignment = left
			feature property = 32
			Section
			(
				type = section
	 			row = 1
	 			column = 0   
				width = medium
			)
			must have Event_Name
			(
    			type = text
				displayname = "Event Name"
	 			row = 1
	 			column = 1   
				width = medium
			)
			Event_Type
			(
				type = picklist
				displayname = "Event Type"
				values = {"Walk in Interview", "Campus Interview", "Out Campus Interview", "Job Fair"}
				others option = true
	 			row = 1
	 			column = 1   
				width = medium
			)
			Recruiters_Mail_ID
			(
    			type = textarea
				displayname = "Recruiters Mail ID"
				height = 100px
	 			row = 1
	 			column = 1   
				width = medium
			)
			Recruiters
			(
				type = list
				values  = Add_Recruiter.ID
    			displayformat = [Recruiter_Email]
				sortorder = ascending
				height = 60px
	 			row = 1
	 			column = 1   
				width = medium
			)
			Job_Places
			(
				type = list
				displayname = "Job Places"
				values  = Add_Job_Vacancy[Number_Of_Vacancies > 0].ID
    			displayformat = [Job_Title]
				sortorder = ascending
				height = 60px
	 			row = 1
	 			column = 1   
				width = medium
			)
			Total_Job_Places
			(
				type = number
				displayname = "Total Job Places"
	 			row = 1
	 			column = 1   
				width = medium
			)
			Event_Venue
			(
				type = picklist
				displayname = "Event Venue"
				values  = Add_Event_Venue.ID
    			displayformat = [Venue_Name]
				sortorder = ascending
	 			row = 1
	 			column = 1   
				width = medium
			)
			must have Event_Starting_Date
			(
    			type = date
				displayname = "Event Starting Date"
				alloweddays = 0,1,2,3,4,5,6
	 			row = 1
	 			column = 1   
				width = medium
			)
			Event_End_Date
			(
    			type = date
				displayname = "Event End Date"
				alloweddays = 0,1,2,3,4,5,6
	 			row = 1
	 			column = 1   
				width = medium
			)
			Registration_End_Date
			(
    			type = date
				displayname = "Registration End Date"
				alloweddays = 0,1,2,3,4,5,6
	 			row = 1
	 			column = 1   
				width = medium
			)
			Status
			(
				type = radiobuttons
				values = {"Upcoming", "Ongoing", "Finished"}
				initial value = "Upcoming"
	 			row = 1
	 			column = 1   
				width = medium
			)
	
			customize
			(
				icon = "ui-3-calendar-add"
			)
			actions
			{
				on add
				{
					submit
					(
   						type = submit
   						displayname = "Submit"
					)
					reset
					(
   						type = reset
   						displayname = "Reset"
					)
				}
				on edit
				{
					update
					(
   						type = submit
   						displayname = "Update"
					)
					cancel
					(
   						type = cancel
   						displayname = "Cancel"
					)
				}
			}
		}
		custom Upcoming_Events
		{
			displayName = "Upcoming Events"
			show all rows from Add_Event  [Status == "Upcoming"]  
			(
				Registration_End_Date as "Registration End Date"
				custom action "Start Event"
				(
	  			workflow = Start_Event
				column header = "Start Event"
				success message = "The event has been started"
				)
				Event_Type as "Event Type"
				Total_Job_Places as "Total Job Places"
				Event_Name as "Event Name"
				Event_Starting_Date as "Event Starting Date"
				Event_End_Date as "Event End Date"
				Event_Venue as "Event Venue"
				(
					view = Event_Venues
				)
			)
			group by
			(
				Event_Type ascending
			)
			options
			(
				icon = "ui-1-calendar-grid-58"
	 		)
			quickview
			(
				layout
				(
		 			type = 2
					datablock1
					(
			 			layout type = 4
		     			title = "Overview"
						fields
						(
							Event_Name as "Event Name"
							Event_Venue as "Event Venue"
							Event_Starting_Date as "Event Starting Date"
							Event_End_Date as "Event End Date"
							Total_Job_Places as "Total Job Places"
						)
					)
				)

				menu
    			(
    	 			header
    	 			(
    		 			Edit 
    		 			Duplicate 
    		 			Delete 
    		 			Add 
    		 			Print 
    		 			Import 
    		 			Export 
    	 			)
							record
							(
								"Start Event"
								Edit   	   
								Delete   	   
								Duplicate   	   
								Print   	   
    						)
    			)
    			action
    			(
							on click
							(
								View Record   	   
    						)
							on right click
							(
								Edit   	   
								Delete   	   
								Duplicate   	   
								Print   	   
								View Record   	   
								"Start Event"
    						)
     			)
			)
			detailview
			(
				layout
				(
		 			type = 2
					datablock1
					(
			 			layout type = -2
		     			title = "Overview"
						fields
						(
							Event_Name as "Event Name"
							Event_Type as "Event Type"
							Event_Venue as "Event Venue"
							Event_Starting_Date as "Event Starting Date"
							Event_End_Date as "Event End Date"
						)
					)
					datablock2
					(
			 			layout type = 34
		     			title = "Recruiters"
		     			related field =  Recruiters[Status == "Upcoming"]
						fields
						(
							Recruiter_Name as "Recruiter Name"
							Recruiter_Email as "Recruiter Email"
							Phone_Number as "Phone Number"
							Photo
						)
					)
					datablock3
					(
			 			layout type = -1
		     			title = "Job Places"
		     			related field =  Job_Places[Status == "Upcoming"]
						fields
						(
							Job_Title as "Job Title"
							Department
							Number_Of_Vacancies as "Number Of Vacancies"
						)
					)
				)

				menu
    			(
    	 			header
