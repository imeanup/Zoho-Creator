	functions
	{
 			void Event_Custom_Actions.finishEvent(int eventID)
			{
				// The event is finished.
				// The event status is updtated as the "Finished".
				thisEvent = Add_Event[ID == eventID];
				thisEvent.Status="Finished";
			}
 			void Candidate_Custom_Actions.CallForInterview(Registration_Form selectedCandidate)
			{
				// Candidate is called for interview.
				// Candidate status is updated as the "Called for Interview".
				candidatesID = List();
				for each  candidate in selectedCandidate
				{
					candidatesID.add(candidate.ID);
				}
				openUrl(zoho.appuri + "#Form:Call_For_Interview?Candidates_Email=" + candidatesID,"popup window");
			}
 			void Candidate_Custom_Actions.selectCandidate(int candidateID)
			{
				// Candidate is Selected for job placement.
				// Candidate status is updated as the "Hired".
				thisCandidate = Registration_Form[ID == candidateID];
				openUrl(zoho.appuri + "#Form:Select_Job_to_Candidate?Candidate=" + thisCandidate.ID,"popup window");
			}
 			void Event_Custom_Actions.eventSummery(int eventID)
			{
				openUrl(zoho.appuri + "#Page:Event_Summery1?event=" + eventID,"popup window");
			}
 			void Event_Custom_Actions.startEvent(int eventID)
			{
				// The event will be started.
				// The event status is updated as "Ongoing"
				thisEvent = Add_Event[ID == eventID];
				thisEvent.Status="Ongoing";
				openUrl(zoho.appuri + "/#Report:With_Held_Candidates?Event=" + thisEvent.ID,"same window");
			}
 			void Candidate_Custom_Actions.holdCandidate(int candidateID)
			{
				// Candidate is added as a waiting list.
				// Candidate status is updated as the "On Hold".
				thisCandidate = Registration_Form[ID == candidateID];
				openUrl(zoho.appuri + "#Form:Feed_Back_Form?Candidate" + thisCandidate.ID + "&Result=On Hold","same window");
			}
 			void Candidate_Custom_Actions.rejectCandidate(int candidateID)
			{
				// Candidate is Rejected for the placement.
				// Candidate status is updated as the "Rejected".
				thisCandidate = Registration_Form[ID == candidateID];
				openUrl(zoho.appuri + "#Form:Feed_Back_Form?Candidate" + thisCandidate.ID + "&Result=Rejected","popup window");
			}
	}



