			Send_Conformation_Mail_Ac as "Send Conformation Mail"
			{
				type =  form
				form = Select_Job_to_Candidate
				button = Send_Conformation_Mail
				on click
				{
					actions 
					{
					on click
					(
						//status will be updated.
										thisCandidate = Registration_Form[ID == input.Candidate];
										thisCandidate.Status="Hired";
										selected_job = Add_Job_Vacancy[ID == input.Job_Place];
										thisCandidate.Hired_Job_Place=selected_job.Job_Title + "-" + selected_job.Department;
										org = Add_Organization_Details[ID != 0];
										//Conformation mail will be sent.
										sendmail
										[
											from :zoho.adminuserid
											to :thisCandidate.Email_ID
											subject :"Hapspy Mail"
											message :("Dear " + thisCandidate.Candidate_Name) + "<br><br>Congrats, With reference to your recent interview, We are pleased to offer you the position of <b>" + thisCandidate.Hired_Job_Place + "</b><br><br> We will send you the appointment order soon <br><br> Regards,<br>" + org.Organization_Name
										]
										openUrl("#Script:page.close","same window");
					)
					}
				}
			}
			Send_Result as "Send Result"
			{
				type =  form
				form = Result_Form
				button = Send_Result
				on click
				{
					actions 
					{
					on click
					(
						thisCandidate = Registration_Form[ID == input.Candidate];
										org = Add_Organization_Details[ID != 0];
										msg = "";
										//The Appropriat message will be sent to the candidate.
										if(input.Result == "On Hold")
										{
											//If Result is "On Hold" the msg will be loaded as appropriate content of that.
											msg = ("Dear " + thisCandidate.Candidate_Name) + ",<br><br> We refer to your recent interview of the above position and regret to inform you that, on this occasion, you were not successful with your application. With your permission, We like to retain your application on file. In the event that a suitable vacancy occurs in the future We will contact you again to establish if you are still available.<br><br>wish you every success in finding a suitable position.<br><br>";
										}
										if(input.Result == "Rejected")
										{
											//If Result is "Rejected" the msg will be loaded as appropriate content of that.
											msg = ("Dear " + thisCandidate.Candidate_Name) + ",<br><br> Thank you for attending the recent interview for our company.<br><br>YAfter very careful consideration, We regret that, on this occasion you have not been successful with your application. wish you every success in finding a suitable position.<br><br>";
										}
										if(thisCandidate.Feed_back != "" || thisCandidate.Feed_back != null)
										{
											//If Feedback is given , the feed back should be added with msg.
											msg = msg + "<b>Feedback on your performance:</b><br>" + input.Feed_Back + "<br><br>";
											thisCandidate.Feed_back=input.Feed_Back;
										}
										thisCandidate.Status=input.Result;
										//The result will be sent as email.
										sendmail
										[
											from :zoho.adminuserid
											to :thisCandidate.Email_ID
											subject :"Regret Mail"
											message :msg + "Regards,<br>" + org.Organization_Name
										]
										openUrl("#Script:page.close","same window");
					)
					}
				}
			}
