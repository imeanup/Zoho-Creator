			Run_Scripts_While_click_a as "Run Scripts - While click a button"
			{
				type =  form
				form = Update_Case_Hearing_Result
				button = Submit
				on click
				{
					actions 
					{
					on click
					(
						hearing_details = Add_Hearing_Date[ID == input.Hearing];
										case_details = Add_Case[ID == hearing_details.Case];
										if(input.Hearing_Result == "Case Closed")
										{
											if(input.Case_Result == null || input.Case_Judgement == null)
											{
												alert "Update the Case Result";
											}
											else
											{
												hearing_details.Hearing_Result=input.Hearing_Result;
												hearing_details.heard_Judges=input.Judges;
												case_details = Add_Case[ID == hearing_details.Case];
												case_details.Case_Status="Close";
												case_details.Case_Result=input.Case_Result;
												case_details.Judgement_In_Brief=input.Case_Judgement;
												case_details.Judgement_Date=hearing_details.Hearing_Date;
												openUrl("#Scripts:page.refresh","same window");
											}
										}
										else if(input.Hearing_Result == "Postponed Without Date")
										{
											hearing_details.Hearing_Result=input.Hearing_Result;
											hearing_details.heard_Judges=input.Judges;
											openUrl("#Scripts:page.refresh","same window");
										}
										else if(input.Hearing_Result == "Postponed On Date")
										{
											hearing_details.Hearing_Result=input.Hearing_Result;
											hearing_details.heard_Judges=input.Judges;
											hearing_details.Postponed_Date=input.Postponed_On_date;
											insert into Add_Hearing_Date
											[
												Email_Address=zoho.loginuserid
												Case=case_details.ID
												Hearing_Date=input.Postponed_On_date
												Added_User=zoho.loginuser
												Hearing_Result="Pending"
											]
											openUrl("#Scripts:page.refresh","same window");
										}
					)
					}
				}
			}
			Run_Scripts_While_Adding_1 as "Run Scripts - While Adding a Record"
			{
				type =  form
				form = Add_Lawyer
				record event = on add

	on load
	{
					actions 
					{
						custom deluge script
						(
											//Checking Admin added himself as as a user.
										lawyers = Add_Lawyer[Email == zoho.adminuserid];
										if(lawyers.count() == 0)
										{
											input.Email = zoho.adminuserid;
											disable Email;
											alert "Add yourself first";
										}
						)
					}
	}

			}
