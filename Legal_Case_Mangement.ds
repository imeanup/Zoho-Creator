	section Start_Case
	{
		displayname= "Start Case"
		form Start_Case
		{
			displayname = "Start Case"
			store data in zc = false
			success message = "Data Added Successfully!"
			field alignment = left
			feature property = 32
					hide = true
			Section
			(
				type = section
	 			row = 1
	 			column = 0   
				width = medium
			)
			must have Case
			(
				type = picklist
				values  = Add_Case.ID
    			displayformat = [Case_Title]
				sortorder = ascending
	 			row = 1
	 			column = 1   
				width = medium
			)
			must have Court
			(
				type = picklist
				values  = Add_Court.ID
    			displayformat = [Court_Name]
				sortorder = ascending
	 			row = 1
	 			column = 1   
				width = medium
			)
			must have Case_Number_In_Court
			(
    			type = text
				displayname = "Case Number In Court"
	 			row = 1
	 			column = 1   
				width = medium
			)
			must have Case_Start_Date
			(
    			type = date
				displayname = "Case Start Date"
				alloweddays = 0,1,2,3,4,5,6
	 			row = 1
	 			column = 1   
				width = medium
			)
	
			actions
			{
				Submit
				(
   					type = submit
   					displayname = "Submit"
				)
				Reset
				(
   					type = reset
   					displayname = "Reset"
				)
			}
		}
	}
	section Update_Case_Hearing_Result
	{
		displayname= "Update Case/Hearing Result"
		form Update_Case_Hearing_Result
		{
			displayname = "Update Case/Hearing Result"
			store data in zc = false
			success message = ""
			field alignment = left
			feature property = 32
					hide = true
			Section
			(
				type = section
	 			row = 1
	 			column = 0   
				width = medium
			)
			must have Hearing
			(
				type = picklist
				values  = Add_Hearing_Date.ID
    			displayformat = [Hearing_Date]
				sortorder = ascending
	 			row = 1
	 			column = 1   
				width = medium
			)
			Judges
			(
				type = list
				values  = Add_Judge.ID
    			displayformat = [" " + Judge_Name.prefix + " " + Judge_Name.first_name + " " + Judge_Name.last_name]
				allow new entries = true
				[
					displayname = "Add New Judge"
				]
				sortorder = ascending
				height = 60px
	 			row = 1
	 			column = 1   
				width = medium
			)
			must have Hearing_Result
			(
				type = radiobuttons
				values = {"Postponed On Date", "Postponed Without Date", "Case Closed"}
	 			row = 1
	 			column = 1   
				width = medium
			)
			Postponed_On_date
			(
    			type = date
				displayname = "Postponed On date"
				alloweddays = 0,1,2,3,4,5,6
	 			row = 1
	 			column = 1   
				width = medium
			)
			Case_Result
			(
				type = radiobuttons
				displayname = "Case Result"
				values = {"Won", "Dismissed", "Failure"}
				sortorder = ascending
	 			row = 1
	 			column = 1   
				width = medium
			)
			Case_Judgement
			(
    			type = textarea
				displayname = "Case Judgement"
				height = 100px
	 			row = 1
	 			column = 1   
				width = medium
			)
	
			actions
			{
				Submit
				(
   					type = submit
   					displayname = "Submit"
				)
				Reset
				(
   					type = reset
   					displayname = "Reset"
				)
			}
		}
	}
	section Copy_of_Search
	{
		displayname= "Copy of Search"
		form Search_Case
		{
			displayname = "Search Case"
			store data in zc = false
			success message = ""
			field alignment = labelinplace
			feature property = 32
					hide = true
			Section
			(
				type = section
	 			row = 1
	 			column = 0   
				width = medium
			)
			Case_Number
			(
				type = picklist
				displayname = "Case Number"
				values  = Add_Case[Case_Status != "Open"].ID
    			displayformat = [Case_Number]
				sortorder = ascending
	 			row = 1
	 			column = 1   
				width = medium
			)
	
			customize
			(
				icon = "files-search"
			)
		}
	}
	functions
	{
 			void custom_actions.taskComplete(int id)
			{
				rec = Add_Task[ID == input.id];
				rec.Status="Completed";
				openUrl("#Script:page.refresh","same window");
			}
 			void custom_actions.holdTask(int id)
			{
				rec = Add_Task[ID == input.id];
				rec.Status="On Hold";
			}
 			void custom_actions.addDocument(int caseID)
			{
				openUrl(zoho.appuri + "#Form:Add_Document?Case=" + caseID,"popup window");
			}
 			void custom_actions.addTask(int caseID)
			{
				openUrl(zoho.appuri + "#Form:Add_Task?Case=" + caseID,"popup window");
			}
 			void custom_actions.addHearing(int caseID)
			{
				openUrl(zoho.appuri + "#Form:Add_Hearing_Date?Case=" + caseID,"popup window");
			}
 			void custom_actions.startTask(int taskID)
			{
				task = Add_Task[ID == taskID];
				task.Status="In Progress";
				openUrl("#Script:page.refresh","same window");
			}
 			void custom_actions.startCase(int caseID)
			{
				openUrl(zoho.appuri + "#Form:Start_Case?Case=" + caseID,"popup window");
			}
 			void custom_actions.caseClosed(int hearingID)
			{
				openUrl(zoho.appuri + "#Form:Update_Case_Hearing_Result?Hearing=" + hearingID + "&Hearing_Result=Case Closed","popup window");
			}
 			void custom_actions.hearingPostpopned(int hearingID)
			{
				openUrl(zoho.appuri + "#Form:Update_Case_Hearing_Result?Hearing=" + hearingID + "&Heaing_Result=Hearing Postponed","popup window");
			}
 			void custom_actions.addCasePoint(int caseID)
			{
				openUrl(zoho.appuri + "#Form:Add_Case_Points?Case=" + caseID,"popup window");
			}
 			void custom_actions.reappeal(int caseID)
			{
				caseID = Add_Case[ID == caseID].Case_Number;
				openUrl(zoho.appuri + "#Form:Add_Case?Associated_Case_Number=" + caseID,"same window");
			}
	}



