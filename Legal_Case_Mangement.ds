				record event = on add or edit

	on success
	{
					actions 
					{
						custom deluge script
						(
											client = Add_People[ID == input.Client];
										client.Case=input.ID;
										for each  oppose_client in Opposing_Clients
										{
											oppose_client.Case=input.ID;
											oppose_client.Type="Opposing client";
										}
										for each  witness in Case_Witness
										{
											witness.Case=input.ID;
											witness.Type="Witness";
										}
						)
					}
	}

			}
