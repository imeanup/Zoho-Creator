	functions
	{
 			string Util.imgUrlParsing(string imgUrl, string viewLinkName, string fieldName)
			{
				//parsing the imageField to display in page
				imgStr = imgUrl.replaceAll("/sharedBy/appLinkName/viewLinkName/fieldName",zoho.appuri + "/" + viewLinkName + "/" + fieldName);
				imgStr = imgStr.replaceFirst("<img ","<img style='width:100%;'");
				return imgStr;
			}
 			void appSharing.toCustomers(string customerEmail)
			{
				thisapp.permissions.assignUserInProfile(customerEmail,"Customer Profile");
			}
 			void appUnsharing.toCustomers(string customerEmail)
			{
				thisapp.permissions.deleteuser(customerEmail);
			}
 			void custom_actions.statusUpdate(int complaintRecID)
			{
				//updating the status
				openUrl("#Form:Status_Update?Complaint_ID=" + complaintRecID,"popup window");
			}
 			void custom_actions.markActive(int staffRecID)
			{
				//marking the staff as active
				getStaffDetails = Add_Staffs[ID == input.staffRecID];
				getStaffDetails.Status="Active";
				getStaffDetails.Exit_Date=zoho.currentdate;
				thisapp.appSharing.toStaffs(getStaffDetails.Staff_Email);
				thisapp.notification.invitationMail(getStaffDetails.Staff_Email);
			}
 			void appSharing.toStaffs(string staffEmail)
			{
				thisapp.permissions.assignUserInProfile(staffEmail,"Staff");
			}
 			void notification.invitationMail(string recipient)
			{
				//sending invitation mail
				appname = zoho.appname;
				sendmail
				[
					from :zoho.adminuserid
					to :recipient
					subject :"Invitation to access '" + zoho.appname + "' "
					message :"<div style='width:100%;display:inline-block;box-sizing:border-box;border:1px solid #ddd;'><div style='padding:2%;text-align:center;'>You've been invited to use an application built on Zoho Creator by \"" + zoho.adminuserid + "\".</div><div style='background: #efefef; padding: 20.0px;margin: 20.0px 0 10.0px;text-align:center;'><h2>" + appname.toUpperCase() + "</h2><div style='margin: 20.0px 0 10.0px;'><a style='padding:10px;background-color: rgb(62,149,205);text-decoration: none;color: rgb(255,255,255);font-size: 14.0px;' href='https://creator.zoho.com" + zoho.appuri + "' target='_blank'> Go to application </a></div></div></div>"
				]
			}
 			void custom_actions.markInactive(int staffRecID)
			{
				//marking staff as inactive
				getStaffDetails = Add_Staffs[ID == input.staffRecID];
				getStaffDetails.Status="Inactive";
				getStaffDetails.Exit_Date=zoho.currentdate;
				thisapp.appUnsharing.toStaffs(getStaffDetails.Staff_Email);
			}
 			void appUnsharing.toStaffs(string staffEmail)
			{
				thisapp.permissions.deleteuser(staffEmail);
			}
 			int Util.thisMonthRevenue()
			{
				// returning this month revenue
				return Add_Invoice[ID != 0 && Added_Time in this month].sum(Total).toLong();
			}
 			int Util.todayRevenue()
			{
				// returning today's revenuw
				return Add_Invoice[ID != 0 && Added_Time == today].sum(Total).toLong();
			}
 			int Util.thisMonthClosedTickets()
			{
				//returning the number of tickets closed in this month
				return Complaint_Registration[ID != 0 && Modified_Time in this month && Status == "Closed"].count();
			}
 			int Util.todayClosedTickets()
			{
				// returning number of tickets closed today
				return Complaint_Registration[ID != 0 && Modified_Time == today && Status == "Closed"].count();
			}
 			int Util.openTickets()
			{
				// returning number of tickets created in this month
				return Complaint_Registration[ID != 0 && Status != "Closed" && Assign_to is not null].count();
			}
 			int Util.newTickets()
			{
				// returning number of tickets created today
				return Complaint_Registration[ID != 0 && Assign_to is null && Status != "Closed"].count();
			}
 			void notification.complaintClosed(int complaintRecID)
			{
				getComplaintStatus = Complaint_Registration[ID == input.complaintRecID];
				getProd = Add_Product[ID == getComplaintStatus.Product_Name].Product_Name;
				getCustomerDetails = Add_Customer[ID == getComplaintStatus.Customer_ID];
				getAdminDetails = Add_Admin_Details[ID != 0];
				sendmail
				[
					from :zoho.adminuserid
					to :getCustomerDetails.Email
					subject :"Your product " + getProd + " is fixed now"
					message :(("<div> Dear " + getCustomerDetails.Name) + ",</div> <div>   Complaint ID: <font style='font-weight:400;color:red;'>#" + getComplaintStatus.Complaint_ID + "</font> For your " + getProd + ", is serviced and redy to serve.</div><div> Kindly Recieve your Product. </div> <div> Thanks for choosing our service. Hope you enjoyed our service.</div><div>Click this <a href='https://creator.zoho.com" + zoho.appuri + "#'> Link </a> to track you complaint.</div><div>Thanks,</div><div>" + getAdminDetails.Admin_Name) + "</div>"
				]
			}
 			void notification.complaintRegistration(int complaintRecID)
			{
				//sending mail notification to customer, about their complaint
				getRec = Complaint_Registration[ID == input.complaintRecID];
				getCustomerDetails = Add_Customer[ID == getRec.Customer_ID];
				getProductDetails = Add_Product[ID == getRec.Product_Name];
				getAdminDetails = Add_Admin_Details[ID != 0];
				sendmail
				[
					from :zoho.adminuserid
					to :getCustomerDetails.Email
					cc:zoho.adminuserid
					subject :"Your complaint is successfully Registered!"
					message :(("<div> Dear " + getCustomerDetails.Name) + ",</div> <div> We have received your complaint, Complaint ID: <font style='font-weight:400;color:red;'>#" + getRec.Complaint_ID + "</font> For your " + getProductDetails.Product_Name + ".</div> <div> Thanks for choosing our service. Soon,we will get back to you.</div><div>Click this <a href='https://creator.zoho.com" + zoho.appuri + "#'> Link </a> to track you complaint.</div><div>Thanks,</div><div>" + getAdminDetails.Admin_Name) + "</div>"
				]
			}
 			void notification.ticketAssign(int complaintRecID, int staffID)
			{
				getRec = Complaint_Registration[ID == input.complaintRecID];
				getStatusRec = Status_Update[Complaint_ID == input.complaintRecID];
				getStaffDetails = Add_Staffs[ID == input.staffID];
				getProductName = Add_Product[ID == getRec.Product_Name].Product_Name;
				getAdminDetails = Add_Admin_Details[ID != 0];
				sendmail
				[
					from :zoho.adminuserid
					to :getStaffDetails.Staff_Email
					subject :" Ticket is assigned to Assigned to you **" + getRec.Complaint_ID + "**"
					message :("<div>Dear " + getStaffDetails.Staff_Name) + ",</div><div> A ticket is assigned to you. ticket details as follows,</div><div><table style='border:1px solid #ececec;border-collapse:collapse;width:100%;'><tr><td colspan='2'> Complaint ID: " + getRec.Complaint_ID + "</td></tr><tr> <th>Product Name</th><td>" + getProductName + "</td> </tr> <tr><th> Serial Number </th><td>" + getRec.Serial_Number + "</td></tr><tr><th> Complaint Title </th> <td> " + getRec.Complaint_Title + "</td></tr><tr><th> Complaint Description </th> <td> " + getRec.Complete_Problem_Description + " </td></tr></table></div><div> Please, Check with the ticket and update the status as soon as possible.</div><div>Thanks and regards,</div>" + getAdminDetails.Admin_Name
				]
			}
	}



	workflow
	{
		form
		{
			Run_Scripts_While_Adding_6 as "Run Scripts - While Adding a Record"
			{
				type =  form
				form = Search
				record event = on add

	on load
	{
					actions 
					{
						custom deluge script
						(
											// Customers and Staffs can see and track only their Complaints.
										// Admin can see all Complaints
										if(zoho.loginuserid != zoho.adminuserid)
										{
											getStaffDetails = Add_Staffs[Staff_Email == zoho.loginuserid];
											getCustomerDetails = Add_Customer[Email == zoho.loginuserid];
											if(getStaffDetails.count() != 0)
											{
												input.Complaint_ID:ui.add(getStaffDetails.Assigned_Tickets);
											}
											else
											{
												getAllComplaints = Complaint_Registration[Customer_ID == getCustomerDetails.ID].ID.getAll();
												input.Complaint_ID:ui.add(getAllComplaints);
											}
										}
						)
					}
	}

			}
			Run_Scripts_While_Adding_7 as "Run Scripts - While Adding a Record"
			{
				type =  form
				form = Search
				record event = on add

	on user input of Complaint_ID
	{
					actions 
					{
						custom deluge script
						(
											openUrl(zoho.appuri + "page-embed/trackingPage?recID=" + input.Complaint_ID,"iframe","trackframe");
						)
					}
	}

			}
			Run_Scripts_While_Adding_8 as "Run Scripts - While Adding a Record"
			{
				type =  form
				form = Add_Admin_Details
				record event = on add

	on load
	{
					actions 
					{
						custom deluge script
						(
											disable Admin_Email;
										input.Admin_Email = zoho.adminuserid;
						)
					}
	}

			}
			Run_Scripts_While_Adding_9 as "Run Scripts - While Adding a Record"
			{
				type =  form
				form = Add_Admin_Details
				record event = on add

	on success
	{
					actions 
					{
						custom deluge script
						(
											//Adding Admin as a Staff in 'Add_Staffs'
										insert into Add_Staffs
										[
											Staff_Name=input.Admin_Name
											Firm_Name=input.ID
											Staff_Email=input.Admin_Email
											Added_User=zoho.loginuser
											Status="Active"
											Staff_ID="0001"
										]
										openUrl("#Form:Add_Staffs?Firm_Name=" + input.ID,"same window");
						)
					}
	}

			}
			Run_Scripts_While_Adding_10 as "Run Scripts - While Adding a Record"
			{
				type =  form
				form = Add_Customer
				record event = on add

	field rules
	{
					actions 
					{
						hide Customer_ID;
					}
	}

			}
			Run_Scripts_While_Adding_11 as "Run Scripts - While Adding a Record"
			{
				type =  form
				form = Add_Customer
				record event = on add

	on validate
	{
					actions 
					{
						custom deluge script
						(
											// Generating the Customer ID (starts from 0001)
										getRec = Add_Customer[ID is not null] sort by Added_Time desc;
										if(getRec.count() == 0)
										{
											input.Customer_ID = "0001";
										}
										else
										{
											nextValue = (getRec.Customer_ID.toLong() + 1).toString();
											nextValue = leftpad(nextValue,4).replaceAll(" ","0");
											input.Customer_ID = nextValue;
										}
						)
					}
	}

			}
			Run_Scripts_While_Adding_12 as "Run Scripts - While Adding a Record"
			{
				type =  form
				form = Add_Customer
				record event = on add

	on success
	{
					actions 
					{
						custom deluge script
						(
											// sharing the specific modules to the customers.
										thisapp.appSharing.toCustomers(input.Email);
										// sending invitation to access the applicaiton
										thisapp.notification.invitationMail(input.Email);
						)
					}
	}

			}
			Run_Scripts_While_Editing3 as "Run Scripts - While Editing a Record"
			{
