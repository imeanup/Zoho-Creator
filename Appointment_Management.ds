							}
							else if(getRec.Status == "Cancelled")
							{
								str = (str + "<li style='border-color:#ff7e7b; text-decoration: line-through;color:red;'><label class='label'style='-webkit-filter:hue-rotate(190deg);'></label><div class='status_Note'><span style='color:#ff7e7b;font-size:18px;'>" + gettime + "</span><br><span style='font-size:16px;color:#800800;position: relative;left: 10px;'>" + getRec.Customer_Name) + "</span></div></li>";
							}
							else
							{
								str = (str + "<li><label class='label'></label><div class='status_Note'><span style='color:#888;font-size:18px;'>" + gettime + "</span><br><span style='font-size:16px;color:#800800;position: relative;left: 10px;'>" + getRec.Customer_Name) + "</span></div></li>";
