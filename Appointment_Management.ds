	functions
	{
 			string Page.getStaffDetails()
			{
				getDetails = Add_Staffs[Email == zoho.loginuserid];
				getBranch = Add_Branch[ID == getDetails.Working_at];
				ser = "";
				for each  rec in getDetails.Services
				{
					getRec = Add_Services[ID == rec];
					ser = ser + getRec.Service + ",";
				}
				str = ("<ul id=\"myList\"style=\"list-style:none;padding:5%;\"><li>Name : " + getDetails.Name) + "</li><li>Email : " + getDetails.Email + "</li>  <li>Contact Number : " + getDetails.Contact_Number + "</li>  <li>Services :" + ser.removeLastOccurence(",") + "</li>  <li>Branch : " + getBranch.Branch_Name + "</li></ul>";
				return str;
			}
 			string Page.getUserAppointments()
			{
				getTodayApointments = (Appointment_Booking[Customer_Email == zoho.loginuserid && Schedule_Date_Time >= zoho.currenttime && Schedule_upto < zoho.currenttime.addDay(1)].ID sort by Schedule_Date_Time desc).getAll();
				str = "<ul style='list-style:none;padding:0;'><li style='font-size:18px;color:#888;padding:8px;'>Today's Appointments</li>";
				if(getTodayApointments.size() == 0)
				{
					str = str + "<li style='padding:12%;height:100%;color:#888;'>You Don't Have any Appointment today</li>";
				}
				else
				{
					for each  eachToday in getTodayApointments
					{
						fet = Appointment_Booking[ID == eachToday];
						staff = Add_Staffs[ID == fet.Staff_Name];
						str = (str + "<li> Your Appointment with <font style='color:purple'>" + staff.Name) + "</font> on <b>" + fet.Schedule_Date_Time + "</b>. Booking ID <font style='color:red'>#" + fet.Booking_Id + ".</font></li><hr>";
					}
				}
				str = str + "</ul>";
				return str;
			}
 			string Page.getUserDetails()
			{
				customer = Add_Customer[Customer_Email == zoho.loginuserid];
				str = (("<ul id=\"myList\"style=\"list-style:none;padding:5%;\">  <li>Name : " + customer.Customer_Name) + " </li>  <li>Email : " + customer.Customer_Email + " </li>  <li>Contact Number :  " + customer.Customer_Contact_Number + " </li>  <li>Address :  " + customer.Address) + " </li> </ul>";
				return str;
			}
 			string Page.getWorkingHours()
			{
				getRec = Add_Staffs[Email == zoho.loginuserid];
				str = "<table style=\"width:100%;\" cellspacing=\"5\" cellpadding=\"5\"> <tr style=\"color:purple\"> <th><b>Days</b></th><th colspan=\"2\"> <b>From</b> </th>  <th> <b>Till</b> </th> </tr>";
				for each  eachDay in getRec.Working_Hours
				{
					str = str + "<tr> <th>" + eachDay.Days + "</th><th>" + eachDay.From_Timing + "</th> <th> to </th> <th> " + eachDay.To_Timing + " </th> </tr>";
				}
				str = str + "</table>";
				return str;
			}
 			bool Util.CheckAvailability(int staffrecid, date startingTime, date endingTime)
			{
				getStaffDetails = Add_Staffs[ID == input.staffrecid];
				// checking whether any other appointments in between time
				getRecords = Appointment_Booking[Staff_Name == input.staffrecid && (Schedule_Date_Time >= input.startingTime && Schedule_upto <= endingTime) || (Schedule_Date_Time <= input.startingTime && Schedule_upto > input.startingTime) || (Schedule_Date_Time < input.endingTime && Schedule_upto > input.endingTime)].ID.getAll();
				//if any appointement is there return false
				if(getRecords.size() != 0)
				{
					return false;
				}
				else
				{
					// else check whether the given time is applicable with the service provider or not
					weekDays = {"","Sunday","Monday","Tuesday","Wednesday","Thursday","Friday","Saturday"};
					dayWeek = startingTime.getDayOfWeek();
					get_Day_ofWeek = weekDays.get(dayWeek);
					for each  eachr in getStaffDetails.Working_Hours
					{
						if(eachr.Days == get_Day_ofWeek)
						{
							getStartTiming = (startingTime.toDate() + " " + eachr.From_Timing.substring(0,5) + ":00 " + eachr.From_Timing.substring(6)).toTime();
							getFinishTiming = (startingTime.toDate() + " " + eachr.To_Timing.substring(0,5) + ":00 " + eachr.To_Timing.substring(6)).toTime();
						}
					}
					// 		Check with the staff availability, on the booking date where the staff has working schedule
					if(getStartTiming <= startingTime && getFinishTiming > startingTime)
					{
						return true;
					}
					else
					{
						return false;
					}
				}
			}
