				{
				condition = (Tentative_Release_Date > zoho.currentdate && Status != "Released")
				format = "color:#2f7ba3;background-color:#bbe2fc;"
				}
				"Scheduled Release-Today"
				{
				condition = (Tentative_Release_Date == zoho.currentdate && Status != "Released")
				format = "color:#2f7ba3;background-color:#bbe2fc;"
				}
			)
