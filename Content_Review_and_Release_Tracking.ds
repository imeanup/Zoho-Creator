	    			day   = Today
	    			month = Currentmonth
	    			year  = Currentyear
				)
				icon = "ui-1-calendar-grid-61"
			)
			conditional formatting
			(
				"Released Contents"
				{
				condition = (Status == "Released")
				format = "color:#23825f;background-color:#9ef0c8;"
				}
				"Overdue"
				{
				condition = (Tentative_Release_Date < zoho.currentdate && Status != "Released")
				format = "color:#c23932;background-color:#f59c96;"
				}
