		calendar All_Releases
		{
			displayName = "All Releases"
			show all rows from New_Release    
			(
				Release_Type as "Release Type"
				Tentative_Release_Date as "Tentative Release Date"
				Actual_Release_Date as "Actual Release Date"
				Contents.Title as "Contents"
				Contents.Writer as "Contents.Writer"
				Release_Notes as "Release Notes"
				Territory
				DateHidden
				Description
				Title
			)
			options
			(
				display type = month
				week start day = sunday
				display field = Title
				start date = DateHidden
