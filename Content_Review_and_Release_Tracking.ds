	functions
	{
 			void CustomAction.assign_editor(int ID)
			{
				openUrl("#Form:Assign_Editor?RecID=" + ID,"popup window");
			}
 			void CustomAction.completeRelease(int RecID)
			{
				openUrl("#Form:Release_Details?RecID=" + RecID + "&zc_nextUrl=#Script:dialog.close","popup window");
			}
 			void CustomAction.resubmit(int ID)
			{
				openUrl("#Form:Add_Conversation?DocumentID=" + ID + "&Added_By=Writer&zc_Header=false","popup window");
			}
 			void CustomAction.review(int ID)
			{
				openUrl("#Form:Add_Conversation?DocumentID=" + ID + "&Added_By=Editor&zc_Header=false","popup window");
			}
 			void CustomAction.viewComments(int Id)
			{
				openUrl("#Page:Comments_View?docID=" + Id,"popup window","width=600px");
			}
 			void CustomAction.approve(int ID)
			{
				record = Submit_Your_Document[ID == input.ID];
				record.Review_Status="Review Completed";
				record.Stage_of_Approval="Approved";
				releaseId = Add_Content[ID == record.Title].ReleaseID;
				conditionalVariable = true;
				for each  document in Add_Content[ReleaseID == releaseId]
				{
					if(Submit_Your_Document[Title == document.ID].Stage_of_Approval != "Approved")
					{
						conditionalVariable = false;
					}
				}
				if(conditionalVariable)
				{
					releaseRec = New_Release[ID == releaseId];
					releaseRec.Status="Ready for Release";
				}
				writerRec = Add_User[ID == record.Submitter];
				sendmail
				[
					from :zoho.loginuserid
					to :writerRec.Email
					subject :"Your Content Approval-Confirmation"
					message :("Hi " + writerRec.Name) + ",<div><br></div><div>Congratulations!! This mail is to notify that your document named \"" + record.Title.Title + "\"" + " has been approved successfully.</div><div><div><br></div><div><br></div><div>Regards</div><div>" + zoho.loginuser + "</div></div>"
				]
			}
	}



	workflow
	{
		form
		{
			Run_Scripts_On_Button_Cli as "Run Scripts - On Button Click"
			{
				type =  form
				form = Assign_Editor
				button = Assign
				on click
				{
					actions 
					{
					on click
					(
						//Update the selected editor to the corresponding document
										record = Submit_Your_Document[ID == input.RecID];
										record.Assigned_To=input.Select_Editor;
										record.Stage_of_Approval="Assigned to Editor";
										editorRec = Add_User[ID == input.Select_Editor];
										//Notification mail to the Editor 
										sendmail
										[
											from :zoho.loginuserid
											to :editorRec.Email
											subject :"New Review Request"
											message :("Hi " + editorRec.Name) + ",<div><br></div><div>New document-\"" + record.Title + "\"" + " has been assigned to you for approval.</div><div><br></div><div>Please review and update your comments.</div><div><br></div><div>Thanks and Regards</div><div>" + zoho.loginuser + "</div>"
										]
										openUrl("#Script:page.refresh","same window");
					)
					}
				}
			}
			Run_Scripts_On_Button_Cli1 as "Run Scripts - On Button Click"
			{
				type =  form
				form = Release_Details
				button = Submit
				on click
				{
					actions 
					{
					on click
					(
						//Updating the release details to the corresponding scheduled release and marking as completed
										releaseRecord = New_Release[ID == input.RecID];
										releaseRecord.Actual_Release_Date=input.Release_Date;
										releaseRecord.Territory=input.Territory;
										releaseRecord.Release_Notes=input.Release_Notes;
										releaseRecord.Status="Released";
										openUrl("#Script:dialog.close","same window");
					)
					}
				}
			}
