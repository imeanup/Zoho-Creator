			Create_record_scripts1 as "Create record scripts"
			{
				type =  form
				form = Add_Conversation
				record event = on add

	on success
	{
					actions 
					{
						custom deluge script
						(
											if(input.Added_By == "Writer")
										{
											record = Submit_Your_Document[ID == input.DocumentID];
											record.Review_Status="Resubmitted";
											editorRec = Add_User[ID == record.Assigned_To];
											//Send notification mail to editor if writer resubmits a document
											sendmail
											[
												from :zoho.loginuserid
												to :editorRec.Email
												subject :"Document Resubmitted for Review"
												message :("Hi&nbsp;" + editorRec.Name) + "<div><br></div><div>I have updated the document as per the review comments.</div><div><br></div><div>Please check and let me know if any corrections or suggestions needed.</div><div><br></div><div>Thanks and Regards</div><div>" + zoho.loginuser + "</div><div><div><br></div></div>"
											]
										}
										if(input.Added_By == "Editor")
										{
											record = Submit_Your_Document[ID == input.DocumentID];
											record.Review_Status="Reviewed";
											writerRec = Add_User[ID == record.Submitter];
											//Send notification mail to writer if editor adds review to a document
											sendmail
											[
												from :zoho.loginuserid
												to :writerRec.Email
												subject :"Document Review Notification"
												message :("Hi&nbsp;" + writerRec.Name) + "<div><br></div><div>Editor has reviewed your article and added the comments. &nbsp; &nbsp;</div><div><br></div><div>Please update your content and get back to us for approval.</div><div><br></div><div><br></div><div>Regards,</div><div>" + zoho.loginuser + "</div>"
											]
										}
										openUrl("#Script:dialog.close","same window");
						)
					}
	}

			}
